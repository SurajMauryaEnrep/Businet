        if (Model.Command == "New" || Model.Command == "Add")
        {
            Disable = false;
            ViewBag.Disable = false;
        }
    }
    if (Model.DNStatus == "C" || Model.DN_No == null || ((Model.DNStatus == "D" || Model.DNStatus == "A") && Model.Command == "Edit"))
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }


    if (Disable)
    {
        ViewBag.Disable = true;
        ForDisableOCDlt = "Disable";
    }
    else
    {
        ViewBag.Disable = false;
        ForDisableOCDlt = "Enable";

    }
