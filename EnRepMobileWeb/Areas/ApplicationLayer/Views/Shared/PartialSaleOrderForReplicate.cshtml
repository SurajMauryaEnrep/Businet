@using EnRepMobileWeb.Resources;
<div class="modal-content">
    <div class="modal-header">
        <button type="button" id="" class="close" data-dismiss="modal">×</button>
    </div>
    <div class="modal-body pop_top">
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <div class="tableFixHead height30VH">
                    <div class="col-md-12 col-sm-12"> @Resource.ReplicateWith</div>
                    <div class="col-md-12 col-sm-12 border pt-1 pb-1" id="">
                        <div class="checkbox">
                            <label>
                                <input type="checkbox" id="ChkReplicateWithOrd_Qty" value="ReplicateWithOrderQuantity" onclick="ReplicateWithOrderQuantity()"> @Resource.Order @Resource.Quantity
                            </label>
                        </div>
                        <div class="checkbox">
                            <label>
                                <input type="checkbox" id="ChkReplicateWithPen_Qty" value="ReplicateWithPendingQuantity" onclick="ReplicateWithPendingQuantity()"> @Resource.Pending @Resource.Quantity
                            </label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-primary btn-sm" id="ReplicateWithOk" data-dismiss="modal" onclick="OnClickReplicateWith()">@Resource.OK1</button>
        <button type="button" class="btn btn-primary btn-sm" id="DiscardAndExit_FW" data-dismiss="modal">@Resource.Close</button>
    </div>
</div>