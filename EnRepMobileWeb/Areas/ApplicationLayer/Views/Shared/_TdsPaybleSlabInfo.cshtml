@using EnRepMobileWeb.Resources;
<div class="modal-content" id="tbody_ItemHistory">
    <div class="modal-header">
        <h2> @Resource.TDS @Resource.Information</h2>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>
    <div class="modal-body pop_top">
        <div class="row">
            <div class="col-md-8 col-sm-12">
                <label class="col-form-label label-align">@Resource.SupplierName</label>
                <input id="PopTdsSuppName" class="form-control" autocomplete="off" type="text" name="SupplierName" placeholder="@Resource.SupplierName" disabled="">
            </div>
            <div class="col-md-4 col-md-55 ">
                <label class="col-form-label label-align">@Resource.TDS @Resource.Payable</label>
                <input id="PopTdsPayble" class="form-control" autocomplete="off" type="text" name="TDSPayable" placeholder="@Resource.TDS @Resource.Payable" disabled="">
            </div>
        </div>
        <div class="card-box table-responsive dy_table margin10">
            <div id="" class="sticky-table sticky-ltr-cells sb-container container-example1 OrderHistory height30VH">
                <table id="TdsPaybleDtTbl" class="table table-striped table-bordered rowhighlighttbl">
                    <thead>
                        <tr class="sticky-header">
                            <th width="4%">@Resource.SrNo</th>
                            <th width="21%">@Resource.TDS @Resource.Amount</th>
                            <th width="15%">@Resource.TDS @Resource.InPercentage</th>
                            <th width="15%">@Resource.TDS @Resource.Payable</th>
                        </tr>
                    </thead>
                    <tbody>
                    
                    </tbody>
                </table>
            </div>
            <table id="" class="table table-striped table-bordered">                
                <tbody>                   
                    <tr class="total">
                        <td colspan="3" align="right" width="75%"><strong>@Resource.Total</strong></td>
                        <td class="num_right font-weight-bold"><span id="TotTdsPayble"></span></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal">Close</button>
    </div>
</div>

