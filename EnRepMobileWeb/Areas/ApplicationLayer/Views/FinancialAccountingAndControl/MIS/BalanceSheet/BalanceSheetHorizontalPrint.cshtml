@using EnRepMobileWeb.Resources;
@using System.Globalization;
@{
    var grpstr = string.Empty;
    var grpstr_id = string.Empty;
    var start = 0;
    string tot_asset_amt = string.Empty;
    string tot_liba_amt = string.Empty;
    string tot_finliba_amt = string.Empty;
    string tot_passet_amt = string.Empty;
    string tot_pliba_amt = string.Empty;
    string tot_pl_amt = string.Empty;

    string tot_assetcap_amt = string.Empty;
    string tot_assetcurr_amt = string.Empty;
    string tot_assetncurr_amt = string.Empty;
    string tot_passetcap_amt = string.Empty;
    string tot_passetcurr_amt = string.Empty;
    string tot_passetncurr_amt = string.Empty;
    string tot_libcap_amt = string.Empty;
    string tot_libcurr_amt = string.Empty;
    string tot_libncurr_amt = string.Empty;
    string tot_plibcap_amt = string.Empty;
    string tot_plibcurr_amt = string.Empty;
    string tot_plibncurr_amt = string.Empty;
    var cultureInfo = new CultureInfo("hi-IN");

    if (ViewBag.ass_capflag != null)
    {
        if (ViewBag.ass_capflag == "ca")
        {
            start = 0;
            foreach (System.Data.DataRow Row in ViewBag.capi_assetdata.Rows)
            {

                tot_asset_amt = Row["ctot_amt"].ToString();
                tot_passet_amt = Row["ptot_amt"].ToString();
                tot_assetcap_amt = Row["cgrptot_amt"].ToString();
                tot_passetcap_amt = Row["pgrptot_amt"].ToString();

                start = 1;
                if (start == 1)
                {
                    break;
                }
            }
        }
    }
    else
    {
        tot_assetcap_amt = "0.00";
        tot_passetcap_amt = "0.00";
    }
    if (ViewBag.ass_currflag != null)
    {
        if (ViewBag.ass_currflag == "cu")
        {
            start = 0;
            foreach (System.Data.DataRow Row in ViewBag.curr_assetdata.Rows)
            {
                tot_asset_amt = Row["ctot_amt"].ToString();
                tot_passet_amt = Row["ptot_amt"].ToString();
                tot_assetcurr_amt = Row["cgrptot_amt"].ToString();
                tot_passetcurr_amt = Row["pgrptot_amt"].ToString();
                start = 1;
                if (start == 1)
                {
                    break;
                }
            }
        }
    }
    else
    {
        tot_assetcurr_amt = "0.00";
        tot_passetcurr_amt = "0.00";
    }
    if (ViewBag.ass_ncurrflag != null)
    {
        if (ViewBag.ass_ncurrflag == "nc")
        {
            start = 0;
            foreach (System.Data.DataRow Row in ViewBag.ncurr_assetdata.Rows)
            {
                tot_asset_amt = Row["ctot_amt"].ToString();
                tot_passet_amt = Row["ptot_amt"].ToString();
                tot_assetncurr_amt = Row["cgrptot_amt"].ToString();
                tot_passetncurr_amt = Row["pgrptot_amt"].ToString();
                start = 1;
                if (start == 1)
                {
                    break;
                }
            }
        }
    }
    else
    {
        tot_assetncurr_amt = "0.00";
        tot_passetncurr_amt = "0.00";
    }

    if (ViewBag.lib_capflag != null)
    {
        if (ViewBag.lib_capflag == "ca")
        {
            start = 0;
            foreach (System.Data.DataRow Row in ViewBag.capi_libadata.Rows)
            {
                tot_liba_amt = Row["ctot_amt"].ToString();
                tot_pliba_amt = Row["ptot_amt"].ToString();
                tot_libcap_amt = Row["cgrptot_amt"].ToString();
                tot_plibcap_amt = Row["pgrptot_amt"].ToString();
                start = 1;
                if (start == 1)
                {
                    break;
                }
            }
        }
    }
    else
    {
        tot_libcap_amt = "0.00";
        tot_plibcap_amt = "0.00";
    }
    if (ViewBag.lib_currflag != null)
    {
        if (ViewBag.lib_currflag == "cu")
        {
            start = 0;
            foreach (System.Data.DataRow Row in ViewBag.curr_libadata.Rows)
            {
                tot_liba_amt = Row["ctot_amt"].ToString();
                tot_pliba_amt = Row["ptot_amt"].ToString();
                tot_libcurr_amt = Row["cgrptot_amt"].ToString();
                tot_plibcurr_amt = Row["pgrptot_amt"].ToString();
                start = 1;
                if (start == 1)
                {
                    break;
                }
            }
        }
    }
    else
    {
        tot_libcurr_amt = "0.00";
        tot_plibcurr_amt = "0.00";
    }
    if (ViewBag.lib_ncurrflag != null)
    {
        if (ViewBag.lib_ncurrflag == "nc")
        {
            start = 0;
            foreach (System.Data.DataRow Row in ViewBag.ncurr_libadata.Rows)
            {
                tot_liba_amt = Row["ctot_amt"].ToString();
                tot_pliba_amt = Row["ptot_amt"].ToString();
                tot_libncurr_amt = Row["cgrptot_amt"].ToString();
                tot_plibncurr_amt = Row["pgrptot_amt"].ToString();
                start = 1;
                if (start == 1)
                {
                    break;
                }
            }
        }
    }
    else
    {
        tot_libncurr_amt = "0.00";
        tot_plibncurr_amt = "0.00";
    }

    if (ViewBag.PL_amt != null)
    {
        foreach (System.Data.DataRow Row in ViewBag.PL_amt.Rows)
        {
            tot_pl_amt = Row["netprofit_tot_amt"].ToString();
        }
    }

    if (tot_liba_amt != null && tot_liba_amt != "")
    {
        if (tot_pl_amt != null && tot_pl_amt != "")
        {
            tot_finliba_amt = (Convert.ToDouble(tot_liba_amt) + Convert.ToDouble(tot_pl_amt)).ToString();
        }
        else
        {
            tot_finliba_amt = Convert.ToDouble(tot_liba_amt).ToString();
        }
        //tot_liba_amt = tot_finliba_amt;
        tot_liba_amt = string.Format(cultureInfo, "{0:N}", Convert.ToDouble(tot_finliba_amt));
    }
