@{
    //var Disable = false;
    var Disable = true;
    string Lbl_d_none = "";
    if (Model.Country != "101" && Model.Country != "")
    {
        Lbl_d_none = "none";
    }
    if (Model.TransType != null)
    {
        //if (Session["TransType"].ToString() == "Update")
        if (Model.TransType == "Update")
        {
            //if (Session["Command"].ToString() == "Edit")
            if (Model.Command == "Edit")
            {
                Disable = false;
            }
            else
            {
                Disable = true;
            }
        }
        else
        {
            Disable = false;
        }
        //if (Session["TransType"].ToString() == "Refresh")
        if (Model.TransType == "Refresh")
        {
            Disable = true;
        }
        //if (Session["TransType"].ToString() == "Duplicate")
        if (Model.TransType == "Duplicate")
        {
            Disable = false;
        }
        //if (Session["TransType"].ToString() == "Save")
        if (Model.TransType == "Save")
        {
            //if (Session["Command"].ToString() == "Add")
            if (Model.Command == "Add")
            {
                Disable = false;
            }
            else
            {
                Disable = true;
            }
        }
    }
    if (Model.Message != null)
    {
        if (Model.Message == "Duplicate")
        {
            Disable = false;
        }

    }
}
@{
    if (Model.Message != null)
    {
        var str = Model.Message;
        Model.hdnsaveApprovebtn = null;

        <script>
            if ("@str" == "Save") {
            swal("", $("#savemsg").text(), "success");
        }
        if ("@str" == "Duplicate") {
            $(document).ready(function () {
                document.getElementById("vmentity").innerHTML = $("#valueduplicate").text();
                $("#EntityName").css("border-color", "red");
                $("#vmentity").css("display", "block");
                return false;
            });
            }
        if ("@str" == "DuplicatePerfix") {
            $(document).ready(function () {
                document.getElementById("vmEntityPrefix").innerHTML = $("#valueduplicate").text();
                $("#Entityprefix").css("border-color", "red");
                $("#vmEntityPrefix").css("display", "block");
                return false;
            });
        }
        </script>
        Model.Message = "";
        str = "";
    }
}
