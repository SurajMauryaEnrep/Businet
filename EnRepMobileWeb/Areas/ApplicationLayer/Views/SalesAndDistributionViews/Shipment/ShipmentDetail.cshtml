@{
    var ForDisableOCDlt = "Enable";
    var Disable = true;
    //var DisableTransport = true;
    var disablePrintbtn = "";

    if (Model.TransType == "Update")
    {
        if (Model.Command == "Edit")
        {
            if (ViewBag.DocumentCode == "SH")
            {
                Disable = true;
            }
            else
            {
                Disable = false;
            }
        }
        else
        {
            Disable = true;
        }
        //if (Model.Command == "UpdateTransPortDetail")
        //{
        //    DisableTransport = false;
        //}
    }
    else
    {
        if (Model.Command == "Refresh")
        {
            Disable = true;
        }
        else
        {
            Disable = false;
        }
    }
    if (Disable)
    {
        ForDisableOCDlt = "Disable";
    }
    else
    {
        ForDisableOCDlt = "Enable";
    }
    if (!Disable)
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
    //if (Model.StatusCode == "C" || Model.ship_no == null || ((Model.StatusCode == "D" || Model.StatusCode == "A") && Model.Command == "Edit"))/*Add || ((Model.StatusCode == "D" || Model.StatusCode == "A") && Model.Command == "Edit") by Hina sharma on 11-10-2024*/
    if (Model.ship_no == null || ((Model.StatusCode == "D" || Model.StatusCode == "A") && Model.Command == "Edit"))/*Add || ((Model.StatusCode == "D" || Model.StatusCode == "A") && Model.Command == "Edit") by Hina sharma on 11-10-2024*/
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
}
@{

    if (Model.Message != null)
    {
        Model.hdnsaveApprovebtn = null;
        //if (Session["Message"].ToString() == "Save")
        if (Model.Message == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
            </script>
        }
        //if (Session["Message"].ToString() == "Deleted")
        if (Model.Message == "Deleted")
        {
            <script type="text/javascript">
                swal("", $("#deletemsg").text(), "success");
            </script>
        }
        //if (Session["Message"].ToString() == "Approved")
        if (Model.Message == "Approved")
        {
            <script type="text/javascript">
                swal("", $("#approvemsg").text(), "success");
            </script>
        }
        if (Model.Message == "Approved_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    $("#hdnsavebtn").val("Approved_ErrorInMail");
                    swal("", $("#approvemsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        //if (Session["Message"].ToString() == "Cancelled")
        if (Model.Message == "Cancelled")
        {
            <script type="text/javascript">
                swal("", $("#cancelmsg").text(), "success");
            </script>
        }
        if (Model.Message == "Cancelled_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    $("#hdnsavebtn").val("Cancelled_ErrorInMail");
                    swal("", $("#cancelmsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        //if (Session["Message"].ToString() == "StockNotAvail")
        if (Model.Message == "StockNotAvail")
        {
            <script type="text/javascript">
                swal("", $("#SelectedlotqtyhasbeenchangedDoccantbesaved").text(), "warning");
            </script>
        }
        //if (Session["Message"].ToString() == "Used")
        if (Model.Message == "Used")
        {
            <script type="text/javascript">
                swal("", $("#SaleInvoiceHasBnPrcsdCanNotBeMdfd").text(), "warning");
            </script>
        }
        if (Model.Message == "DocModify")
        {
            <script type="text/javascript">
                swal("", $("#SourceDocumentHasBeenModifiedCanNotProceed").text(), "warning");
            </script>
        }
        if (Model.Message == "DuplicateCustomInvNo")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    $("#hdnsavebtn").val("Duplicate");
                    $("#Span_custom_inv_no").text($("#valueduplicate").text());
                    $("#Span_custom_inv_no").css("display", "block");
                    $("#custom_inv_no").css("border-color", "red");
                });
            </script>
        }
        if (Model.Message == "ErrorInMail")
        {
            <script type="text/javascript">
                swal("", $("#Emailsendingfailedduetotechnicalissue").text(), "warning");
            </script>
        }
        //Session["Message"] = null;
        Model.Message = null;
    }
}
@{
    string ValDigit = "0.";
    string QtyDigit = "0.";
    string RateDigit = "0.";
    if (Model.DocumentMenuId == "105103145120")
    {
        var VD = Convert.ToInt32(Session["ExpImpValDigit"].ToString());
        for (var i = 0; i < VD; i++)
        {
            ValDigit += "0";
        }
        var QD = Convert.ToInt32(Session["ExpImpQtyDigit"].ToString());
        for (var i = 0; i < QD; i++)
        {
            QtyDigit += "0";
        }
        var RD = Convert.ToInt32(Session["ExpImpRateDigit"].ToString());
        for (var i = 0; i < RD; i++)
        {
            RateDigit += "0";
        }
    }
    else
    {
        var VD = Convert.ToInt32(Session["ValDigit"].ToString());
        for (var i = 0; i < VD; i++)
        {
            ValDigit += "0";
        }
        var QD = Convert.ToInt32(Session["QtyDigit"].ToString());
        for (var i = 0; i < QD; i++)
        {
            QtyDigit += "0";
        }
        var RD = Convert.ToInt32(Session["RateDigit"].ToString());
        for (var i = 0; i < RD; i++)
        {
            RateDigit += "0";
        }
    }
}
