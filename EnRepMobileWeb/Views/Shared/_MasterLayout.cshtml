        $("#file-1").fileinput({
            theme: 'fa',
            uploadUrl: '#',
            allowedFileExtensions: ['jpg', 'png', 'gif', 'pdf'],
            overwriteInitial: false,
            maxFileSize: 2048,
            maxFilesNum: 10,
            slugCallback: function (filename) {

                var flagmatch = "N";
                var imgFiles = $("#FilesUpload .file-preview-thumbnails .krajee-default .file-thumbnail-footer #fname:odd");
                imgFiles.each(function (index) {
                    var nfilename = $(this).text();

                    if (nfilename === filename) {
                        flagmatch = "Y";
                    }
                });
                if (flagmatch == "Y") {
                    filename = filename.replace('.', (imgFiles.length + 1) + '.');
                }
                return filename.replace('(', '_').replace(']', '_');
            }
        });
    </script>
