@model EnRepMobileWeb.MODELS.ApplicationLayer.SubContracting.MaterialDispatch.MDListModel

@using EnRepMobileWeb.Resources;
@{
    var rowId = 0;
}
<script src="~/Content/Scripts/Common/CommonPartialJS.js"></script>
<script src="~/Content/Scripts/ApplicationLayer/SubContracting/MaterialDispatch/MaterialDispatchList.js"></script>

<table id="datatable-buttons" class="table table-striped table-bordered width2200 rowhighlighttbl itmfrz" style="width:100%;">
    <thead>
        <tr class="sticky-header">
            <th width="2%">@Resource.SrNo</th>
            <th width="8%" class="itmStick">@Resource.Issue <br /> @Resource.Number</th>
            <th width="5%">@Resource.Issue <br /> @Resource.Date</th>
            <th style="display:none;">@Resource.Issue  <br /> @Resource.Date</th>
            <th width="13%">@Resource.ContractorName</th>
            <th width="8%">@Resource.Job <br /> @Resource.Order @Resource.Number</th>
            <th width="5%">@Resource.Job @Resource.Date</th>
            <th width="11%">@Resource.Finished<br /> @Resource.Product</th>
            <th width="4%">@Resource.UOM</th>
            <th width="7%">@Resource.OperationName</th>
            <th width="11%">@Resource.Operation <br /> @Resource.Output @Resource.Product</th>
            <th width="5%">@Resource.UOM</th>
            <th width="7%">@Resource.Status</th>
            <th width="7%">@Resource.Createdon</th>
            <th width="7%">@Resource.ApprovedOn</th>

        </tr>
    </thead>
    <tbody>

        @if (Model.MaterialDispatchList != null)
        {
            foreach (var row in Model.MaterialDispatchList)
            {
                rowId++;
        <tr>
            <td>@rowId</td>
            <td id="MDIssueNoList" class="ItmNameBreak itmStick tditemfrz">@row.MDIssueNo</td>
            <td id="MDIssueDateList">@row.MDIssueDate</td>
            <td style="display:none;" id="MDIssueDtList">@row.MDIssueDt</td>
            <td id="SuppName">@row.SuppName</td>
            <td>@row.JobOrdNo</td>
            <td>@row.JobOrdDate</td>
            <td>@row.FItemName</td>
            <td>@row.FUOM</td>
            <td>@row.OprationName</td>
            <td>@row.ProductName</td>
            <td>@row.UOM</td>


            <td id="Doc_Status">@row.MD_Status</td>
            <td>@row.CreateDate</td>
            <td>@row.ApproveDate</td>
            @*<td>@row.ModifyDate</td>*@

        </tr>
            }
        }


    </tbody>
</table>
@*@if (Session["MDSearch"] != null)*@
@if (Model.MDSearch != null)
{
    //if (Session["MDSearch"].ToString() == "MD_Search")
    if (Model.MDSearch == "MD_Search")
    {
        @Scripts.Render("~/Content/Scripts/DataTableRefreshJs")
        @Scripts.Render("~/Content/Scripts/DatatablesJs")
        //Session["MDSearch"] = null;
        Model.MDSearch = null;
    }
}