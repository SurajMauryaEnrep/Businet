                <div class="x_title pad_mar ">
                    <div class="col-md-12 col-sm-12 icon_tool padding_zero">
                        <form class="available_stock pad_mar" action="" method="post" novalidate="">
                            <div class="col-md-12 col-sm-12 no-padding">
                                <div class="col-md-55 col-sm-12">
                                    <label class="col-form-label label-align"> @Resource.FY </label>
                                    <select class="form-control" id="ddl_financial_year" name="ddl_financial_year" onchange="ddl_financial_year_onchange()">
                                        <option value="0">---Select---</option>
                                        <option value="2022-04-01,2023-03-31">01-04-2022 to 31-03-2023</option>
                                        <option value="2021-04-01,2022-03-31">01-04-2021 to 31-03-2022</option>
                                        <option value="2020-04-01,2021-03-31">01-04-2020 to 31-03-2021</option>
                                        <option value="2019-04-01,2020-03-31">01-04-2019 to 31-03-2020</option>
                                    </select><span class="field-validation-valid" data-valmsg-for="ddl_financial_year" data-valmsg-replace="true" id="vm_ddl_financial_year" style="color:red;!important"></span><input id="hdn_ddl_financial_year" name="ddl_financial_year" type="hidden" value="">
                                </div>
                                <div class="col-md-55 col-sm-12">
                                    <label class="col-form-label label-align">@Resource.Period</label>

                                    <select class="form-control" id="ddl_period" name="ddl_period" onchange="ddl_period_onchange()">
                                        <option value="0">---Select---</option>
                                        <option value="0">Period</option>
                                        <option value="0">Period 1</option>
                                    </select><span class="field-validation-valid" data-valmsg-for="ddl_period" data-valmsg-replace="true" id="vm_ddl_period" style="color:red;!important"></span><input id="hdn_ddl_period" name="ddl_period" type="hidden" value="">
                                </div>
                                <div class="col-md-4 col-sm-12">
                                    <label class="col-form-label label-align col-md-12 col-sm-12 no-padding">@Resource.DateRange </label>
                                    <div class="col-md-6 col-sm-12 no-padding">

                                        <input autocomplete="off" class="form-control" id="txtFromDate" name="txtFromDate" placeholder="" type="date" value="">
                                        <span class="field-validation-valid" data-valmsg-for="txtFromDate" data-valmsg-replace="true" id="vm_txtFromDate" style="color:red;!important"></span>
                                    </div>
                                    <div class="col-md-1 col-sm-12 no-padding" style="text-align:center;"> -</div>
                                    <div class="col-md-5 col-sm-12 no-padding">

