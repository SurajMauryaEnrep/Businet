                                <table cellspacing="0" cellpadding="0" style=" width:100%; font-size: 12px; font-weight:normal;padding: 0px 0px; line-height:18px; font-weight:normal; vertical-align:top; border: solid 1px #000000;">
                                    <thead>
                                        <tr>
                                            <th width="3%" style="border-right: solid 1px #000000; paddingtop: 5px; paddingbottom: 5px; font-weight: bold; text-align: center; ">S.NO.</th>
                                            <th style="border-right: solid 1px #000000; paddingtop: 5px; paddingbottom: 5px; font-weight: bold; text-align: center;">Voucher Number</th>
                                            <th width="80" style="border-right: solid 1px #000000; paddingtop: 5px; paddingbottom: 5px; text-align: center; ">Voucher <br /> Date</th>
                                            <th style="border-right: solid 1px #000000; paddingtop: 5px; paddingbottom: 5px; width: 150px; font-weight: bold; text-align: center; ">Narration</th>
                                            <th style="border-right: solid 1px #000000; paddingtop: 5px; paddingbottom: 5px; font-weight: bold; text-align: center; ">Opening Balance</th>
                                            <th width="5%" style="border-right: solid 1px #000000; paddingtop: 5px; paddingbottom: 5px; font-weight: bold; text-align: center; ">Balance Type</th>
                                            <th style="border-right: solid 1px #000000; paddingtop: 5px; paddingbottom: 5px; font-weight: bold; text-align: center; ">Debit Amount</th>
                                            <th style="border-right: solid 1px #000000; paddingtop: 5px; paddingbottom: 5px; font-weight: bold; text-align: center; ">Credit Amount</th>
                                            <th style="border-right: solid 1px #000000; paddingtop: 5px; paddingbottom: 5px; font-weight: bold; text-align: center; ">Closing Balance </th>
                                            <th width="5%" style="font-weight: bold; paddingtop: 5px; paddingbottom: 5px; text-align: center; ">Balance Type</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (ViewBag.PrintGL != null)
                                        {
                                            var rowno = 1;
                                            foreach (System.Data.DataRow dr in ViewBag.PrintGL.Rows)
                                            {
                                                <tr>
                                                    <td style="text-align:center; width:5%;vertical-align:top;border-top: solid 1px #000000;border-right: solid 1px #000000;">
                                                        @rowno
                                                    </td>
                                                    <td style="paddingleft:5px;vertical-align:top;border-top: solid 1px #000000;border-right: solid 1px #000000;">
                                                        @dr["vou_no"]
                                                    </td>
                                                    <td style="paddingleft:5px;vertical-align:top;border-top: solid 1px #000000;border-right: solid 1px #000000;">
                                                        @dr["vou_dt"]
                                                    </td>
                                                    <td style="paddingleft:5px;vertical-align:top;border-top: solid 1px #000000;border-right: solid 1px #000000;">
                                                        @dr["narr"]
                                                    </td>
                                                    <td style="text-align:right; padding-right:5px;vertical-align:top;border-top: solid 1px #000000;border-right: solid 1px #000000;">
                                                        @dr["op_bal"]
                                                    </td>
                                                    <td style="text-align:center; padding-right:5px;vertical-align:top;border-top: solid 1px #000000;border-right: solid 1px #000000;">
                                                        @dr["op_type"]
                                                    </td>
                                                    <td style="text-align:right; padding-right:5px;vertical-align:top;border-top: solid 1px #000000;border-right: solid 1px #000000;">
                                                        @dr["dr_amt"]
                                                    </td>
                                                    <td style="text-align:right; padding-right:5px;vertical-align:top;border-top: solid 1px #000000;border-right: solid 1px #000000;">
                                                        @dr["cr_amt"]
                                                    </td>
                                                    <td style="text-align:right; padding-right:5px;vertical-align:top;border-top: solid 1px #000000;border-right: solid 1px #000000;">
                                                        @dr["closing_bal"]
                                                    </td>
                                                    <td style=" text-align:center; paddingleft:5px;vertical-align:top;border-top: solid 1px #000000;">
                                                        @dr["closing_type"]
                                                    </td>
                                                </tr>
                                                rowno = rowno + 1;
                                            }
                                        }

                                    </tbody>
                                </table>
                            </td>
                            <td width="15">&nbsp;</td>
                        </tr>
                    </table>
                </td>
