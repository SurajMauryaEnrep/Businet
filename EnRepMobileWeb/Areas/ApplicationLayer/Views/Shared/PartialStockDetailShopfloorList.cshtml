@model EnRepMobileWeb.MODELS.ApplicationLayer.InventoryManagement.MIS.StockDetailShopfloor.StockDetailShopfloor_Model

@using EnRepMobileWeb.Resources;
@{
    var rowId = 0;
}
@*<style>
        #datatable-buttons_wrapper .btn-group a:nth-child(3) {
            display: inline-block;
            position: absolute;
        }
    </style>*@
<script src="~/Content/Scripts/Common/CommonPartialJS.js"></script>
@{
    string ShowValue = "NotShowValue";
    if (ViewBag.VBRoleList != null)
    {
        foreach (System.Data.DataRow Row in ViewBag.VBRoleList.Rows)
        {
            if (Row["feature_id"].ToString() == "105102180103003")
            {
                ShowValue = "ShowValue";
            }
        }
    }
    string tableWidth = "width2300";
    if (Model.StockBy == "Branchwisestock")
    {
        tableWidth = "width2300";
    }
    else if (Model.StockBy == "Warehousewisestock")
    {
        tableWidth = "width2500";
    }
    else if (Model.StockBy == "LotBatchwisestock")
    {
        tableWidth = "width3000";
    }
    else if (Model.StockBy == "SubitemWise")
    {
        tableWidth = "width3200";
    }

}
@if (ShowValue == "ShowValue")
{
    <style>
        .showstkvalue {
            display: block;
        }
    </style>
}
else
{
    <style>
        .showstkvalue {
            display: none;
        }
    </style>
}



@*<table id="datatable-buttons" class="table table-striped @tableWidth itmfrz" cellspacing="0" width="100%">*@
<table id="NewTableStockDetailShfl" class="table table-striped @tableWidth itmfrz" cellspacing="0" width="100%">
    <thead>
        <tr class="sticky-header">
            @if (Model.StockBy != null)
            {
                if (Model.StockBy == "Branchwisestock")
                {
                    <th width="3%">@Resource.SrNo</th>
                    <th width="16%" class="itmStick">@Resource.ItemName</th>
                    <th width="5%">@Resource.HSNCode</th>
                    <th style="display: none;">ItemNameID</th>
                    <th width="5%" title="@Resource.UnitOfMeasure"> @Resource.UOM</th>
                    <th width="8%">@Resource.Branch</th>
                    <th style="display: none;">BranchID</th>
                    <th width="7%">@Resource.Receipts</th>
                    <th width="7%">@Resource.Issued</th>
                    <th width="7%">@Resource.Rejected <br />  @Resource.Stock</th>
                    <th width="7%">@Resource.Reworkable <br />  @Resource.Stock</th>
                    <th width="7%">@Resource.WIP <br />  @Resource.Stock</th>
                    <th width="7%">@Resource.Total <br />  @Resource.Stock</th>
                    if (ShowValue == "ShowValue")
                    {
                        <th width="7%">@Resource.Total <br />  @Resource.Stock @Resource.Value</th>
                    }
                    else
                    {
                        <th width="7%" hidden="hidden">@Resource.Total <br />  @Resource.Stock @Resource.Value</th>
                    }
                    <th width="7%">@Resource.Available <br />  @Resource.Stock</th>
                    if (ShowValue == "ShowValue")
                    {
                        <th width="7%">@Resource.Available <br />  @Resource.Stock @Resource.Value</th>
                    }
                    else
                    {
                        <th width="7%" hidden="hidden">@Resource.Available <br />  @Resource.Stock @Resource.Value</th>
                    }
                }
                if (Model.StockBy == "Warehousewisestock")
                {
                    <th width="3%">@Resource.SrNo</th>
                    <th width="13%" class="itmStick">@Resource.ItemName</th>
                    <th width="5%">@Resource.HSNCode</th>
                    <th style="display: none;">ItemNameID</th>
                    <th width="5%" title="@Resource.UnitOfMeasure"> @Resource.UOM</th>
                    <th width="8%">@Resource.Branch</th>
                    <th style="display: none;">BranchID</th>
                    <th width="9%">@Resource.Shopfloor</th>
                    <th style="display: none;">ShopfloorID</th>
                    <th width="6%">@Resource.Receipts</th>
                    <th width="6%">@Resource.Issued</th>
                    <th width="6%">@Resource.Rejected <br />  @Resource.Stock</th>
                    <th width="6%">@Resource.Reworkable <br />  @Resource.Stock</th>
                    <th width="6%">@Resource.WIP <br />  @Resource.Stock</th>
                    <th width="6%">@Resource.Total <br />  @Resource.Stock</th>
