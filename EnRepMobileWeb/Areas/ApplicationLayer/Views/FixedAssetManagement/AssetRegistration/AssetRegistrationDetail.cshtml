@{
    var disablePrintbtn = "";
    var Disable = true;
    var DisableUsedInDP = true;
    var DisableAssetDescription = true;
    var DisableDepFreq = true;
    if (Model.TransType == "Update")
    {
        var v = Model.DocumentStatus;
        if (v == "D")
        {
            if (Model.Command == "Edit")
            {
                Disable = false;
                DisableUsedInDP = false;
            }
            else
            {
                Disable = true;
            }
        }
        else if (v == "A")
        {
            if (Model.Command == "")
            {
                Disable = false;
            }
            else if (Model.Command == "Edit")
            {
                if (ViewBag.UsedInDP == "Y")
                {
                    Disable = false;
                    DisableUsedInDP = true;
                }
                else if (ViewBag.UsedInDP == "N")
                {
                    Disable = false;
                    DisableUsedInDP = false;
                }
            }
        }
    }
    else
    {
        if (Model.Command == "Refresh")
        {
            Disable = true;
            DisableAssetDescription = true;
        }
        else
        {
            Disable = false;
            DisableUsedInDP = false;
            DisableAssetDescription = false;
        }
    }
    //if (Model.DocumentStatus == "A")
    //{
    //    Disable = true;
    //}
    if (Model.BillNumber == null || ((Model.DocumentStatus == "D" || Model.DocumentStatus == "A") && Model.Command == "Edit"))
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
    if (Model.Depreciationfreq != "Monthly")
    {
        DisableDepFreq = false;
    }
    else
    {
        DisableDepFreq = false;
    }
}
@{
    if (Model.Message != null)
    {
        var message = Model.Message;
        // Model.hdnsaveApprovebtn = null;
        if (message == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
            </script>
        }
        if (message == "Deleted")
        {
            <script type="text/javascript">
                swal("", $("#deletemsg").text(), "success");
            </script>
        }
        if (message == "Approved")
        {
            <script type="text/javascript">
                swal("", $("#approvemsg").text(), "success");
            </script>
        }
        if (message == "OrderRaised")
        {
            <script type="text/javascript">
                swal("", $("#approvemsg").text(), "success");
            </script>
        }
        if (Model.Message == "Approved_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    swal("", $("#approvemsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        if (message == "Cancelled")
        {
            <script type="text/javascript">
                swal("", $("#cancelmsg").text(), "success");
            </script>
        }
        if (Model.Message == "Cancelled_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    swal("", $("#cancelmsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        if (message == "Used")
        {
            <script type="text/javascript">
                swal("", $("#PurchaseOrderHasBnPrcsdCanNotBeMdfd").text(), "warning");
            </script>
        }
        if (message == "DocModify")
        {
            <script type="text/javascript">
                swal("", $("#SourceDocumentHasBeenModifiedCanNotProceed").text(), "warning");
            </script>
        }
        if (message == "Duplicate")
        {
            @*<script type="text/javascript">
                    console.log('!');
                    swal("", $("#valueduplicate").text(), "warning");
                </script>*@
            @*<script type="text/javascript">
                    $(document).ready(function () {
                        $("#hdnonetimeclick").val("");
                        $("#SpanSerialNumberErrorMsg").text($("#valueduplicate").text());
                        $("#SerialNumber").css("border-color", "red");
                        $("#SpanSerialNumberErrorMsg").css("display", "block");
                    });
                </script>*@
        }
        //Session["Message"] = "New";
    }
}

