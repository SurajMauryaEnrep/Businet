                <input autocomplete="off" class="form-control" id="txtDispFUom" name="UOM" placeholder="@Resource.UOM" readonly="readonly" type="text" value="">
            </div>
            <div class="col-md-3 col-sm-12">
                <label class="col-form-label label-align">@Resource.Operation @Resource.Name </label>
                <input autocomplete="off" class="form-control" id="txtDispOPName" name="OperationName" placeholder="@Resource.Operation @Resource.Name" readonly="readonly" type="text" value="">
