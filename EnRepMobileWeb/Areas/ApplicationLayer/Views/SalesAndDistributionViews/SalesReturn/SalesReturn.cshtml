@{
    //var rowId = 0;
    var Disable = true;
    var disablePrintbtn = "";
    var ForDisableOCDlt = "Enable";
    //if (Model.TransType == "Update")
    if (Model.TransType == "Update")
    {
        if (Model.Command == "Edit")
        {
            Disable = false;
        }
        else
        {
            Disable = true;
        }
    }
    else
    {
        if (Model.Command == "Refresh")
        {
            Disable = true;
        }
        else
        {
            Disable = false;
        }
    }
    ViewBag.Disable = Disable;
    //if (Model.DocumentStatus == "C" || Model.srt_no == null || ((Model.DocumentStatus == "D" || Model.DocumentStatus == "A") && Model.Command == "Edit"))/*Add || ((Model.StatusCode == "D" || Model.StatusCode == "A") && Model.Command == "Edit") by Hina sharma on 16-10-2024*/
    if (Model.srt_no == null || ((Model.DocumentStatus == "D" || Model.DocumentStatus == "A") && Model.Command == "Edit"))/*Add || ((Model.StatusCode == "D" || Model.StatusCode == "A") && Model.Command == "Edit") by Hina sharma on 16-10-2024*/
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
    if (Disable == true)
    {
        Model.BatchCommand = "Y";
    }
    if (Disable == false)
    {
        Model.BatchCommand = "N";
    }
}

@{
    if (Model.Message != null)
    {
        Model.hdnsaveApprovebtn = null;
        if (Model.Message == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
                sessionStorage.removeItem("EditBtnClickSR");
                sessionStorage.removeItem("ItemRetQtyDetails");
            </script>

        }
        if (Model.Message == "Deleted")
        {
            <script type="text/javascript">
                swal("", $("#deletemsg").text(), "success");
                sessionStorage.removeItem("EditBtnClickSR");
                sessionStorage.removeItem("ItemRetQtyDetails");
            </script>
        }
        if (Model.Message == "Approved")
        {
            <script type="text/javascript">
                swal("", $("#approvemsg").text(), "success");
                sessionStorage.removeItem("EditBtnClickSR");
                sessionStorage.removeItem("ItemRetQtyDetails");
            </script>
        }
        if (Model.Message == "Approved_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    swal("", $("#approvemsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        if (Model.Message == "Cancelled")
        {
            <script type="text/javascript">
                swal("", $("#cancelmsg").text(), "success");
                sessionStorage.removeItem("EditBtnClickSR");
                sessionStorage.removeItem("ItemRetQtyDetails");
            </script>
        }
        if (Model.Message == "Cancelled_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    swal("", $("#cancelmsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        if (Model.Message == "StkNotAvl")
        {
            <script type="text/javascript">
                swal("", $("#SelectedlotqtyhasbeenchangedDoccantbesaved").text(), "warning");
            </script>
        }
        if (Model.Message == "Refresh")
        {
            <script type="text/javascript">
                sessionStorage.removeItem("EditBtnClickSR");
                sessionStorage.removeItem("ItemRetQtyDetails");
            </script>
        }
        if (Model.Message == "DocModify")
        {
            <script type="text/javascript">
                swal("", $("#SourceDocumentHasBeenModifiedCanNotProceed").text(), "warning");
            </script>
        }
        if (Model.Message == "ErrorInMail")
        {
            <script type="text/javascript">
                swal("", $("#Emailsendingfailedduetotechnicalissue").text(), "warning");
            </script>
        }
        Model.Message = "New";
    }
    if (Disable)
    {
        ViewBag.Disable = true;
        ForDisableOCDlt = "Disable";
    }
    else
    {
        ViewBag.Disable = false;
        ForDisableOCDlt = "Enable";
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
}
