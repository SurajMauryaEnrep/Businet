                                    <input value="" class="form-control" id="PackSize" maxlength="20" name="PackSize" placeholder="@Resource.PackSize  @Resource.Area" type="text">
                                </div>
                                <div class="col-md-2 col-sm-12 pl-0">
                                    <label class="col-form-label label-align"> @Resource.MRP</label>
                                    <input value="" class="form-control num_right" id="MRP" maxlength="20" name="MRP" placeholder="0000.00" type="text">
                                </div>
                                <div class="col-md-2 col-sm-12 pl-0">
                                    <label class="col-form-label label-align"> @Resource.Price</label>
                                    <input value="" class="form-control num_right" id="Price" maxlength="20" name="Price" placeholder="0000.00" type="text">
                                </div>
                                <div class="col-md-2 col-sm-12 pl-0">
