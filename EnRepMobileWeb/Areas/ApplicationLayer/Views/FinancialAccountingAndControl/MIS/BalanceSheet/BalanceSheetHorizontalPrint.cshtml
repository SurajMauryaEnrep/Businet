                                            grpstr_id = Row["grp_str_id"].ToString();
                                        }

                                        if (start == 0 && grpstr == @Row["grp_str_name"].ToString())
                                        {
                                            start = 1;
                                            <tr>
                                                <td width="36%" class="reserve_txt_nor_v1">@Row["grp_str_name"].ToString()</td>
                                                <td width="32%" class="reserve_txt_nor_v1 num_right">@Row["csubgrp_tot_amt"].ToString()</td>
                                                <td width="32%" class="reserve_txt_nor_v1 num_right">@Row["psubgrp_tot_amt"].ToString()</td>
                                            </tr>
                                        }

                                        if (start == 1 && grpstr == @Row["grp_str_name"].ToString())
                                        {
                                            <tr>
                                                <td width="36%" class="reserve_txt_nor_v2">@Row["acc_name"].ToString()</td>
                                                <td width="32%" class="reserve_txt_nor_v2 num_right">@Row["cacc_amt"].ToString()</td>
                                                <td width="32%" class="reserve_txt_nor_v2 num_right">@Row["pacc_amt"].ToString()</td>
                                            </tr>
                                        }
                                        else
                                        {
                                            grpstr = @Row["grp_str_name"].ToString();
                                            grpstr_id = @Row["grp_str_id"].ToString();
                                            <tr>
                                                <td width="36%" class="reserve_txt_nor_v1">@Row["grp_str_name"].ToString()</td>
                                                <td width="32%" class="reserve_txt_nor_v1 num_right">@Row["csubgrp_tot_amt"].ToString()</td>
                                                <td width="32%" class="reserve_txt_nor_v1 num_right">@Row["psubgrp_tot_amt"].ToString()</td>
