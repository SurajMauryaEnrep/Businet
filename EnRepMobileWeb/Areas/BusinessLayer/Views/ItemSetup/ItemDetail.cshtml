@{
    var Disable = true;
    if (Model.TransType != null)
    {
        //if (Session["TransType"].ToString() == "Update")
        if (Model.TransType == "Update")
        {
            //if (Session["Command"].ToString() == "Edit")
            if (Model.Command == "Edit")
            {
                Disable = false;
            }
            else
            {
                Disable = true;
            }
        }
        else
        {
            Disable = false;
        }
        //if (Session["TransType"].ToString() == "Refresh")
        if (Model.TransType == "Refresh")
        {
            Disable = true;
        }
        //if (Session["TransType"].ToString() == "Save")
        if (Model.TransType == "Save")
        {
            //if (Session["Command"].ToString() == "Add")
            if (Model.Command == "Add")
            {
                Disable = false;
            }
            else
            {
                Disable = true;
            }
        }
    }
    if (Disable)
    {
        ViewBag.Disable = true;
    }
    else
    {
        ViewBag.Disable = false;
    }
}
@{
    //if (Session["Message"] != null)
    if (Model.Message != null)
    {
        var str = Model.Message;
        <script>
        if ("@str" == "Save")
        {
            $("#hdnSavebtn").val("");
        swal("", $("#savemsg").text(), "success");
            }
        if ("@str" == "Deleted")
        {
                $("#hdnSavebtn").val("");
            swal("", $("#deletemsg").text(), "success");
          }
        if ("@str" == "Approved")
        {
                $("#hdnSavebtn").val("");
            swal("", $("#approvemsg").text(), "success");
        }
        if ("@str" == "Duplicate")
        {
            $(document).ready(function () {
                $("#hdnSavebtn").val("");
                $("#spanItemName").text($("#valueduplicate").text());
                $("#act_stats").prop("checked", true);
                $("#item_name").css("border-color", "red");
                $("#spanItemName").css("display", "block");
            });
               }
        </script>
    }
}
@{
    //if (Session["i_packdetail"] != null)
    if (Model.i_packdetail != null)
    {
        var i_packdetail = Model.i_packdetail;
        <script>
            if ("@i_packdetail" == "Y") {
                $("#PackdetailAccord").css("display", "block");
            }
            else {
                $("#PackdetailAccord").css("display", "none");
            }
        </script>
    }
}
