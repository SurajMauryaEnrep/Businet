@using EnRepMobileWeb.Resources;
@{
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}
@*<title>@Model.Title</title>*@
<div class="row">
    <!-- Workflow Information -->
    <div id="WorkflowInformation" class="modal fade" role="dialog">
        <div class="modal-dialog" style="max-width:700px;">
            <div class="price_item_detail">
                <!-- Modal content-->
                @Html.Partial("~/Areas/Common/Views/_WorkFlow.cshtml")
            </div>
        </div>
    </div>
    <div class="col-md-12 col-sm-12 ">
        <div class="x_panel mar_pad1 ">
            <div class="x_content mar_pad1">
                <h2 class="col-md-5 col-sm-12 heading_left_padding ">@ViewBag.MenuPageName</h2>
                <div id="wizard" class="form_wizard wizard_horizontal col-md-3 col-sm-12 work_flow">
                    @Html.Partial("~/Areas/Common/Views/_PartialWorkflowBar.cshtml")
                </div>
                <div id="" class="col-md-4 col-sm-12 heading_right_padding">
                    <ul>
                        <li>
                            <a class="tooltip-test"
                               title=" @Resource.AddNew" href="#"><img src="~/Content/Images/addgreen.png" width="32" height="32" id="Btn_AddNewGRN" alt="Add"></a>
                        </li>
                    </ul>
                </div>

                <div class="x_title pad_mar ">
                    <div class="col-md-12 col-sm-12 icon_tool padding_zero">
                        <div class="col-md-12 col-sm-12 pl-0 pb-2 pt-2" >
                            <div class="col-md-5 col-sm-12">
                                <label class="col-form-label label-align col-md-3 col-sm-12 pl-0">User Name</label>
                                <input class="form-control col-md-9 col-sm-12" id="" name="User Name"  type="text" placeholder="Surender Singh" value="Surender Singh" >
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-md-3 col-sm-12">
                                <label class="col-form-label label-align">Product Description</label>
                                <select class="form-control" id="ProductDescription" name="ProductDescription">
                                    <option value="">All</option>
                                    <option value="">Engine Mount -TMT</option>
                                    <option value="">V-Belt 1208 </option>
                                </select>
                            </div>
                            <div class="col-md-2 col-sm-12 pl-0">
                                <label class="col-form-label label-align">Activity Type</label>
                                <select class="form-control" id="ActivityType" name="ActivityType">
                                    <option value="0">All</option>
                                    <option value="">Pending</option>
                                    <option value="">Due</option>

                                </select>
                            </div>                          
                            <div class="col-md-1 col-sm-12 pl-0">
                                <button type="button" class="btn btn-primary btn-sm margintop20" id="Btn_Search" onclick="BtnSearch()"><i class="fa fa-search" aria-hidden="true"></i></button>
                            </div>
                        </div>

                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12 col-sm-12 ">
        <div class="x_panel mar_pad1 ">
            <div class="x_content mar_pad1">
                <div class="card-box table-responsive dy_table">
                    <div id="" class="sticky-table sticky-ltr-cells sb-container container-example1">
                        @Html.Partial("~/Areas/ApplicationLayer/Views/Shared/PartialMyTaskList.cshtml")

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- page content end -->
