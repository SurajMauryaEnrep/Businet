@using EnRepMobileWeb.Resources;
<style>
    .dataTables_length, .DataTable1_filter, .dt-buttons, .buttons-csv, .buttons-pdf {
        float: left;
    }

    .dt-buttons {
        width: 6%;
        margin-left: 2%;
    }
</style>
<div class="modal-content" id="tbody_ItemHistory">
    <div class="modal-header">
        <h2> @Resource.Upload @Resource.Data</h2>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>
    <div class="modal-body pop_top">
        <div class="row">
            <div class="col-md-6 col-sm-12 no-padding">
                <div class="col-md-8 col-sm-12">
                    <div class="file-upload">
                        <div class="file-select mt-4">
                            <input type="file" name="cplfile" id="cplfile" accept=".xlsx, .xls" >
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-12 no-padding">
                    <div class="col-md-12 mt-12">
                        <button type="button" class="btn btn-primary btn-sm margintop20" onclick="UploadCPLData();">@Resource.Upload</button>
                    </div>
                </div>
                <div>
                    <div id="excelTable"></div>
                </div>
            </div>
        </div>
    </div>
</div>


