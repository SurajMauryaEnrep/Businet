@{
    var Disable = true;
    var disablePrintbtn = "";
    if (Model.TransType == "Update")
    {
        if (Model.Command == "Edit")
        {
            if (Model.DocumentStatus == "A")
            {
                Disable = true;
            }
            else
            {
                Disable = false;
            }
        }
        else
        {
            Disable = true;
        }
    }
    else
    {
        Disable = false;
    }
    if (Model.TransType == "Refresh")
    {
        Disable = true;
    }

    if (Model.TransType == "Approve")
    {
        Disable = true;
    }
    if (Model.TransType == "Save")
    {
        if (Model.Command == "New")
        {
            Disable = false;
        }
        else
        {
            Disable = true;
        }
    }
    if (ViewBag.DocumentCode == "QP")
    {
        Disable = true;
    }
    if (Model.StatusCode == "C" || Model.confirmation_no == null || ((Model.StatusCode == "D" || Model.StatusCode == "A") && Model.Command == "Edit"))/*Add || ((Model.StatusCode == "D" || Model.StatusCode == "A") && Model.Command == "Edit") by Hina sharma on 16-10-2024*/
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
}
@{
    if (Model.Message != null)
    {
        var str = Model.Message;
        if (Model.Message != null)
        {
            Model
                .hdnsaveApprovebtn = null;
            if (Model.Message == "Save")
            {
                <script type="text/javascript">
                    swal("", $("#savemsg").text(), "success");
                </script>
            }
            if (Model.Message == "Deleted")
            {
                <script type="text/javascript">
                    swal("", $("#deletemsg").text(), "success");
                </script>
            }
            if (Model.Message == "Approved")
            {
                <script type="text/javascript">
                    swal("", $("#approvemsg").text(), "success");
                </script>
            }
            if (Model.Message == "Approved_ErrorInMail")
            {
                <script type="text/javascript">
                    $(document).ready(function () {
                        swal("", $("#approvemsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                    });
                </script>
            }
            if (Model.Message == "Cancelled")
            {
                <script type="text/javascript">
                    swal("", $("#cancelmsg").text(), "success");
                </script>
            }
            if (Model.Message == "Cancelled_ErrorInMail")
            {
                <script type="text/javascript">
                    $(document).ready(function () {
                        swal("", $("#cancelmsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                    });
                </script>
            }
            if (Model.Message == "Used")
            {
                <script type="text/javascript">
                    swal("", $("#QualityInspectionHasBeenProcessedDocumentCanNotBeModified").text(), "warning");
                </script>
            }
            if (str == "StockNotAvail" || str == "StkNotAvl")
            {
                <script type="text/javascript">
                    swal("", $("#SelectedlotqtyhasbeenchangedDoccantbesaved").text(), "warning");
                </script>
            }
            if (Model.Message == "DocModified")
            {
                <script type="text/javascript">
                    swal("", $("#SourceDocumentHasBeenModifiedCanNotProceed").text(), "warning");
                </script>
            }
            if (Model.Message == "ErrorInMail")
            {
                <script type="text/javascript">
                    swal("", $("#Emailsendingfailedduetotechnicalissue").text(), "warning");
                </script>
            }
            //Session.Remove("Message");
            Model.Message = null;
        }
    }
}
