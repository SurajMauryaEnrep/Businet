@{
    var Disable = true;
    var disablePrintbtn = "";
    if (Model.TransType == "Update")
    {
        if (Model.Command == "Edit")
        {
            if (Model.DocumentStatus == "A" || Model.DocumentStatus == "JP")
            {
                Disable = true;
            }
            else
            {
                Disable = false;
            }

        }
        else
        {
            Disable = true;
        }
    }
    else
    {
        Disable = false;
    }

    if (Model.TransType == "Refresh")
    {
        Disable = true;
    }

    if (Model.TransType == "Approve")
    {
        Disable = true;
    }
    if (Model.TransType == "Save")
    {
        if (Model.Command == "Add")
        {
            var Batchno = DateTime.Now.ToString("dd/MM/yyyy hh:mm:ss.fff");
            var Batchno1 = (((Batchno.Replace("/", "")).Replace(" ", "")).Replace(":", "")).Replace(".", "");
            Model.batch_no = Batchno1;
            Disable = false;
        }
        else
        {
            Disable = true;
        }
    }
    if (Model.Command == "Refresh")
    {
        Disable = true;
    }
    if (Model.Command == "Approve")
    {
        Disable = true;
    }
    if (Model.DocumentStatus == "C" || Model.jc_no == null || ((Model.DocumentStatus == "D" || Model.DocumentStatus == "A") && Model.Command == "Edit"))
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
}
@{
    if (Model.Message != null)
    {
        var str = Model.Message;
        if (Model.Message != null)
        {
            Model.hdnsaveApprovebtn = null;
            if (Model.Message == "Save")
            {
                <script type="text/javascript">
                    swal("", $("#savemsg").text(), "success");
                </script>
            }
            if (Model.Message == "Deleted")
            {
                <script type="text/javascript">
                    swal("", $("#deletemsg").text(), "success");
                </script>
            }
            if (Model.Message == "Approved")
            {
                <script type="text/javascript">
                    swal("", $("#approvemsg").text(), "success");
                </script>
            }
            if (Model.Message == "Approved_ErrorInMail")
            {
                <script type="text/javascript">
                    $(document).ready(function () {
                        swal("", $("#approvemsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                    });
                </script>
            }
            if (Model.Message == "Duplicate")
            {
                Disable = false;
            }
            if (Model.Message == "Cancelled")
            {
                <script type="text/javascript">
                    swal("", $("#cancelmsg").text(), "success");
                </script>
            }
            if (Model.Message == "Cancelled_ErrorInMail")
            {
                <script type="text/javascript">
                    $(document).ready(function () {
                        swal("", $("#cancelmsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                    });
                </script>
            }
            if (Model.Message == "ForceClosed")
            {
                <script type="text/javascript">
                    swal("", $("#cancelmsg").text(), "success");
                </script>
            }
            if (Model.Message == "UsedPRDCF")
            {
                <script type="text/javascript">
                    swal("", $("#ProductionConfirmationHasBeenProcessedCanNotBeModified").text(), "warning");
                </script>
            }
            if (Model.Message == "UsedJO")
            {
                <script type="text/javascript">
                    swal("", $("#JobOrderHasBeenProcessedCanNotBeModified").text(), "warning");
                </script>
            }
            if (Model.Message == "ErrorInMail")
            {
                <script type="text/javascript">
                    swal("", $("#Emailsendingfailedduetotechnicalissue").text(), "warning");
                </script>
            }
            @*if (Model.Message == "UsedMRS")/*Commented By Nitesh massage is Changed*/
                {
                    <script type="text/javascript">
                        swal("", $("#ProductionOrderHasBeenProcessedDocumentCanNotBeModified").text(), "warning");
