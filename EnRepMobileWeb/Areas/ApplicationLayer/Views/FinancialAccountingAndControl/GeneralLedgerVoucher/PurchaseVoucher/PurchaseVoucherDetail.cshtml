@{
    if (Model.Message != null)
    {
        Model.hdnsaveApprovebtn = null;
        if (Model.Message == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
                sessionStorage.removeItem("BillDetailSession");
            </script>
        }
        if (Model.Message == "Deleted")
        {
            <script type="text/javascript">
                swal("", $("#deletemsg").text(), "success");
                sessionStorage.removeItem("BillDetailSession");
            </script>
        }
        if (Model.Message == "Approved")
        {
            <script type="text/javascript">
                swal("", $("#approvemsg").text(), "success");
                sessionStorage.removeItem("BillDetailSession");
            </script>
        }
        if (Model.Message == "Cancelled")
        {
            <script type="text/javascript">
                swal("", $("#cancelmsg").text(), "success");
                sessionStorage.removeItem("BillDetailSession");
            </script>
        }
        if (Model.Message == "Used")
        {
            <script type="text/javascript">
                swal("", $("#PaymentsHasBeenAdjustedAgainstThisInvoiceItCanNotBeModified").text(), "warning");
                sessionStorage.removeItem("BillDetailSession");
            </script>
        }
        if (Model.Message == "Used1")
        {
            <script type="text/javascript">
                swal("", $("#AutoGeneratedVoucherCannotModified").text(), "warning");
                sessionStorage.removeItem("BillDetailSession");
            </script>
        }
        if (Model.Message == "New")
        {
            <script type="text/javascript">
                sessionStorage.removeItem("BillDetailSession");
            </script>
        }
        if (Model.Message == "N")
        {
            <script type="text/javascript">
                swal("", $("#DebtCredtAmntMismatch").text(), "warning");
                sessionStorage.removeItem("BillDetailSession");
            </script>
        }
        if (Model.Message == "N")
        {

        }
        else
        {

            Model.Message = "";
        }
    }

}
