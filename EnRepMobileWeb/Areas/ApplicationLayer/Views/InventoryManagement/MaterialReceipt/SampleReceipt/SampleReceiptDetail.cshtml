@{
    var Disable = true;
    var disablePrintbtn = "";
    if (Model.Command != null)
    {
        var cmd = Model.Command;
    }

    if (Model.TransType == "Update")
    {

        if (Model.Command == "Edit")
        {
            if (Model.DocumentStatus == "A" || Model.DocumentStatus == "QP" || Model.DocumentStatus == "R")
            {
                Disable = true;
            }
            else
            {
                Disable = false;
            }
        }
        else
        {
            Disable = true;
        }
    }
    else
    {
        if (Model.Command == "Refresh")
        {
            Disable = true;

        }
        if (Model.Command == "New")
        {
            Disable = false;
        }
    }
    if (Model.DocumentStatus != "C")/*Add by Hina on 11-10-2024 cz print is not created*/
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
    if (Model.DocumentStatus == "C" || Model.srcpt_no == null || ((Model.DocumentStatus == "D" || Model.DocumentStatus == "A") && Model.Command == "Edit"))/*Add by Hina shrama on 11-10-2024*/
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
}

@{
