                                                        }
                                                        else
                                                        {
                                                            @Html.DropDownListFor(model => model.SerialNumber,
                                                            new SelectList(Model.AssetSerialNo, "SerialNo_id", "SerialNo_name", Model.SerialNumber),
                                                            new { @class = "form-control", id = "ddlAssetSerialNo", onchange = "OnChangeddlAssetSerialNo(this)" })
