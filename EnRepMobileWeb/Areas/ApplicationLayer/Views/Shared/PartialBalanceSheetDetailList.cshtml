    string tot_libcurr_amt = string.Empty;
    string tot_libncurr_amt = string.Empty;
    string tot_plibcap_amt = string.Empty;
    string tot_plibcurr_amt = string.Empty;
    string tot_plibncurr_amt = string.Empty;
    var cultureInfo = new CultureInfo("hi-IN");

    if (ViewBag.ass_capflag != null)
    {
        if (ViewBag.ass_capflag == "ca")
        {
            start = 0;
            foreach (System.Data.DataRow Row in ViewBag.capi_assetdata.Rows)
            {

                tot_asset_amt = Row["ctot_amt"].ToString();
                tot_passet_amt = Row["ptot_amt"].ToString();
                tot_assetcap_amt = Row["cgrptot_amt"].ToString();
                tot_passetcap_amt = Row["pgrptot_amt"].ToString();

                start = 1;
                if (start == 1)
                {
                    break;
                }
            }
        }
    }
    else
    {
        tot_assetcap_amt = "0.00";
        tot_passetcap_amt = "0.00";
    }
    if (ViewBag.ass_currflag != null)
    {
        if (ViewBag.ass_currflag == "cu")
