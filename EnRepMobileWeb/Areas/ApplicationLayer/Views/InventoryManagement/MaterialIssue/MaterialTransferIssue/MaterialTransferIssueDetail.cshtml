@{
    //var rowId = 0;
    var Disable = true;
    //if (Session["TransType"].ToString() == "Update")
    if (Model.TransType == "Update")
    {
        //if (Session["Command"].ToString() == "Edit")
        if (Model.Command == "Edit")
        {
            Disable = false;
        }
        else
        {
            Disable = true;
        }
    }
    else
    {
        //if (Session["Command"].ToString() == "Refresh")
        if (Model.Command == "Refresh")
        {
            Disable = true;
        }
        else
        {
            Disable = false;
        }
    }
}
@{
    //if (Session["Message"] != null)
    if (Model.Message != null)
    {
        Model.hdnsaveApprovebtn = null;
        //if (Session["Message"].ToString() == "Save")
        if (Model.Message == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
            </script>
        }

        //if (Session["Message"].ToString() == "Cancelled")
        if (Model.Message == "Cancelled")
        {
            <script type="text/javascript">
                swal("", $("#cancelmsg").text(), "success");
            </script>
        }
        //if (Session["Message"].ToString() == "StockNotAvail")
        if (Model.Message == "StockNotAvail")
        {
            <script type="text/javascript">
                swal("", $("#SelectedlotqtyforsomeitemhasbeenchangedDoccannotbeSaved").text(), "warning");
            </script>
        }
        //if (Session["Message"].ToString() == "Used")
        if (Model.Message == "Used")
        {
            <script type="text/javascript">
                swal("", $("#MaterialTransferReceiptIsUnderProcessTransferIssueCannotBeCancelled").text(), "warning");
            </script>
        }
        if (Model.Message == "DocModify")
        {
            <script type="text/javascript">
                swal("", $("#SourceDocumentHasBeenModifiedCanNotProceed").text(), "warning");
            </script>
        }
        //Session["Message"] = "";
        Model.Message = "";
    }
}
