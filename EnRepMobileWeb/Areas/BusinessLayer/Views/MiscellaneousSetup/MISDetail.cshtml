                                                            </label>
                                                            @if (AddNew == "Y" || Model.Message == "DuplicateBIN")
                                                            {
                                                                @Html.TextBoxFor(model => model.BINNumber, new { @id = "BINNumber", @class = " form-control optional", @autocomplete = "off", @maxlength = "100", @type = "text", name = "BINNumber", onchange = "OnChangeBinNumber()", onkeypress = "OnChangeBinNumber()", @placeholder = Resource.BINNumber })
                                                            }
                                                            else
                                                            {
                                                                @Html.TextBoxFor(model => model.BINNumber, new { @id = "BINNumber", @disabled = "disabled", @class = " form-control optional", @autocomplete = "off", @maxlength = "100", @type = "text", name = "BINNumber", onchange = "OnChangeBinNumber()", onkeypress = "OnChangeBinNumber()", @placeholder = Resource.BINNumber })
                                                            }
                                                            @Html.HiddenFor(model => model.BIN_id, new { @id = "BINNumber_hdnId" })
                                                            @Html.ValidationMessageFor(model => model.BINNumber, "", new { @class = "text-danger", @id = "vmBINNumber" })
                                                        </div>
                                                        <div class="col-md-55 col-sm-12">
                                                            @if (AddNew == "Y" || Model.Message == "DuplicateBIN")
                                                            {
                                                                if (Model.BtnName != null && Model.ShowBin == "show")
                                                                {
                                                                    <button class="margin22 address_save" title="Save" type="submit" onclick="return BinSaveBtnClick()" id="btn_saveBIN" name="Command" value="@Model.BtnName"></button>
                                                                }
                                                                else
                                                                {
                                                                    <button class="margin22 address_save" title="Save" type="submit" onclick="return BinSaveBtnClick()" id="btn_saveBIN" name="Command" value="SaveBin"></button>
                                                                }
                                                            }
                                                            else
                                                            {
                                                                <div id="SaveBtnEnableBINSetup" style="display:none">
                                                                    @if (Model.BtnName != null && Model.ShowBin == "show")
                                                                    {
                                                                        <button class="margin22 address_save" title="Save" type="submit" onclick="return BinSaveBtnClick()" id="btn_saveBIN" name="Command" value="@Model.BtnName"></button>
                                                                    }
                                                                    else
                                                                    {
                                                                        <button class="margin22 address_save" title="Save" type="submit" onclick="return BinSaveBtnClick()" id="btn_saveBIN" name="Command" value="SaveBin"></button>
                                                                    }
                                                                </div>
                                                            }
                                                        </div>
