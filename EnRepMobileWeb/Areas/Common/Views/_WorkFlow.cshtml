@using EnRepMobileWeb.Resources;
<div class="modal-content">
    <div class="modal-header">
        <h2>@Resource.Workflow </h2> <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>
    <div class="modal-body" style="padding-top:0px;">                
            <div class="row" id="Workflow">
                <div class="col-md-6 col-sm-12 available_right ">
                    <label class="col-form-label label-align" for="">@Resource.DocumentNumber</label>
                    <input type="text" id="DocumentNumber" class="form-control" placeholder="@Resource.DocumentNumber" readonly>
                </div>
                <div class="col-md-6 col-sm-12 available_right ">
                    <label class="col-form-label label-align" for="">@Resource.DocumentDate</label>
                    <input type="text" id="DocumentDate" class="form-control" placeholder="@Resource.DocumentDate" readonly>
                </div>
            </div>           
       
        <div class="card-box table-responsive dy_table margin10">
            <div class="sticky-table sticky-ltr-cells sb-container container-example1 height30VH ">
                <table id="TblWorkflow" class="table table-striped table-bordered rowhighlighttbl " style="width:100%;">
                    <thead>                       
                        <tr class="sticky-header">
                            <th width="20%">@Resource.Date</th>
                            <th width="40%">@Resource.WorkflowInformation</th>
                            <th width="40%">@Resource.remarks</th>                           
                        </tr>
                    </thead>
                    <tbody >
                        @*<tr>
                            <td>02-11-2021 11:25</td>
                            <td>This is a Workflow Information</td>
                            <td>This is a Remark Information</td>
                        </tr>*@
                    </tbody>
                </table>
            </div>
        </div>
    </div>
       
    </div>
