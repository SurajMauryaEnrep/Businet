        if (ViewBag.lib_ncurrflag == "nc")
        {
            start = 0;
            foreach (System.Data.DataRow Row in ViewBag.ncurr_libadata.Rows)
            {
                tot_liba_amt = Row["ctot_amt"].ToString();
                tot_pliba_amt = Row["ptot_amt"].ToString();
                tot_libncurr_amt = Row["cgrptot_amt"].ToString();
                tot_plibncurr_amt = Row["pgrptot_amt"].ToString();
                start = 1;
                if (start == 1)
                {
                    break;
                }
            }
        }
    }
    else
    {
        tot_libncurr_amt = "0.00";
        tot_plibncurr_amt = "0.00";
    }

    if (ViewBag.PL_amt != null)
    {
        foreach (System.Data.DataRow Row in ViewBag.PL_amt.Rows)
        {
            tot_pl_amt = Row["netprofit_tot_amt"].ToString();
        }
    }

    if (tot_liba_amt != null && tot_liba_amt != "")
    {
        if (tot_pl_amt != null && tot_pl_amt != "")
        {
            tot_finliba_amt = (Convert.ToDouble(tot_liba_amt) + Convert.ToDouble(tot_pl_amt)).ToString();
        }
        else
        {
            tot_finliba_amt = Convert.ToDouble(tot_liba_amt).ToString();
        }
        //tot_liba_amt = tot_finliba_amt;
        tot_liba_amt = string.Format(cultureInfo, "{0:N}", Convert.ToDouble(tot_finliba_amt));
    }
