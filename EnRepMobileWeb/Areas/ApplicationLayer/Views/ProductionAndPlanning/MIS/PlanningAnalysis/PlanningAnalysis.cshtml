                                        <span class="field-validation-valid" data-valmsg-for="txtToDate" data-valmsg-replace="true" id="vm_txtToDate" style="color:red;!important"></span>
                                    </div>
                                </div>
                                <div class="col-md-1 col-sm-12">
                                    <button type="button" class="btn btn-primary btn-sm margintop20" id="Btn_Search" onclick="BtnSearch();"><i class="fa fa-search" aria-hidden="true"></i></button>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
