        DisableAmend = false;
    }

    if (Model.TransType == "Update")
    {

        if (Model.Command == "Edit")
        {
            if (Model.DocumentStatus == "A" && Model.Amend != "Amend")
            {
                Disable = true;
            }
            else
            {
                Disable = false;
            }
        }
        else
        {
            if (Model.Command == "Amendment" && Model.Amend == "Amend")
            {
                Disable = false;
            }
            else
            {
                Disable = true;
            }

        }
    }
    else
    {
        if (Model.Command == "Refresh")
        {
            Disable = true;


        }
        if (Model.Command == "New")
        {
            Disable = false;
        }
    }
    if ((Model.DocumentStatus == "O") && Model.Command == "Edit" && Model.FClosedFlag == "Y")
    {
        Disable = true;
    }
    if (Model.Status == "C" || Model.SQ_no == null ||
((Model.DocumentStatus == "D" || Model.DocumentStatus == "A")
&& Model.Command == "Edit") || (Model.DocumentStatus == "A" && Model.Command == "Amendment") || Model.AmendmentFlag == "Amendment")/*Add || ((Model.DocumentStatus == "D" || Model.DocumentStatus == "A") && Model.Command == "Edit") by Hina on 11-10-2024*/
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
    if (Disable)
    {
        ViewBag.Disable = true;
        ForDisableOCDlt = "Disable";
    }
    else
    {
        ViewBag.Disable = false;
        ForDisableOCDlt = "Enable";

    }

    if (Model.BtnName == "BtnEdit" && Model.Command == "Edit" &&
