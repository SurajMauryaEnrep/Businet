    if (Model.MDStatus == "C" || Model.MDIssue_No == null || ((Model.MDStatus == "D" || Model.MDStatus == "A") && Model.Command == "Edit"))/*Add || ((Model.MDStatus == "D" || Model.MDStatus == "A") && Model.Command == "Edit") by Hina sharma on 16-10-2024*/
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
    //if (Model.MDStatus == "D" || Model.MDStatus == "F" || Model.MDStatus == "A")
    //{
    //disablePrintbtn = "disabled style=filter:grayscale(100%)";
    //}
    if (Disable)
    {
        ViewBag.Disable = true;
        ForDisableOCDlt = "Disable";
    }
    else
    {
        ViewBag.Disable = false;
        ForDisableOCDlt = "Enable";

    }
