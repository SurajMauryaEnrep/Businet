                        <th width="7%" hidden="hidden">@Resource.Total <br />  @Resource.Stock @Resource.Value</th>
                    }
                    <th width="7%">@Resource.Available <br />  @Resource.Stock</th>
                    if (ShowValue == "ShowValue")
                    {
                        <th width="7%">@Resource.Available <br />  @Resource.Stock @Resource.Value</th>
                    }
                    else
                    {
                        <th width="7%" hidden="hidden">@Resource.Available <br />  @Resource.Stock @Resource.Value</th>
                    }
                }
                if (Model.StockBy == "LotBatchwisestock")
                {
                    <th width="3%">@Resource.SrNo</th>
                    <th width="12%" class="itmStick">@Resource.ItemName</th>
                    <th width="5%">@Resource.HSNCode</th>
                    <th style="display: none;">ItemNameID</th>
                    <th width="4%" title="@Resource.UnitOfMeasure"> @Resource.UOM</th>
                    <th width="6%">@Resource.Branch</th>
                    <th style="display: none;">BranchID</th>
                    <th width="6%">@Resource.Shopfloor</th>
                    <th style="display: none;">ShopfloorID</th>
                    <th width="5%">@Resource.Lot</th>
                    <th width="6%">@Resource.Batch1</th>
                    <th width="6%">@Resource.Serial1</th>
                    <th width="6%">@Resource.Receipts</th>
                    <th width="6%">@Resource.Issued</th>
                    <th width="6%">@Resource.Rejected <br />  @Resource.Stock</th>
                    <th width="5%">@Resource.Reworkable <br />  @Resource.Stock</th>
                    @*<th width="5%">@Resource.WIP <br />  @Resource.Stock</th>*@
                    <th width="5%">@Resource.Total <br />  @Resource.Stock</th>
                    if (ShowValue == "ShowValue")
                    {
                        <th width="7%">@Resource.Total <br />  @Resource.Stock @Resource.Value</th>
                    }
                    else
                    {
                        <th width="7%" hidden="hidden">@Resource.Total <br />  @Resource.Stock @Resource.Value</th>
                    }
                    <th width="5%">@Resource.Available <br />  @Resource.Stock</th>
                    if (ShowValue == "ShowValue")
                    {
                        <th width="7%">@Resource.Available <br />  @Resource.Stock @Resource.Value</th>
                    }
                    else
                    {
                        <th width="7%" hidden="hidden">@Resource.Available <br />  @Resource.Stock @Resource.Value</th>
                    }
                }
                if (Model.StockBy == "SubitemWise")
                {
                    <th width="3%">@Resource.SrNo</th>
                    <th width="15%" class="itmStick">@Resource.ItemName</th>
                    <th width="6%">@Resource.HSNCode</th>
                    <th style="display: none;">ItemNameID</th>
                    <th width="5%" title="@Resource.UnitOfMeasure"> @Resource.UOM</th>
                    <th width="9%">@Resource.Branch</th>
                    <th style="display: none;">BranchID</th>
                    <th width="12%">@Resource.Shopfloor</th>
                    <th style="display: none;">ShopfloorID</th>
                    <th width="12%">@Resource.SubItem @Resource.Name</th>
                    <th style="display: none;">Subitemid</th>
                    @*<th width="9%">@Resource.OpeningStock</th>*@
                    <th width="9%">@Resource.Receipts</th>
                    <th width="9%">@Resource.Issued</th>
                    <th width="9%">@Resource.ReservedStock</th>
                    <th width="9%">@Resource.RejectedStock</th>
                    <th width="9%">@Resource.ReworkableStock</th>
                    <th width="9%">@Resource.WIP @Resource.Stock</th>

                    <th width="9%">@Resource.Total @Resource.Stock</th>

                    if (ShowValue == "ShowValue")
                    {
                        <th width="6%">@Resource.Total <br />  @Resource.Stock @Resource.Value</th>
                    }
                    else
                    {
                        <th width="7%" hidden="hidden">@Resource.Total <br />  @Resource.Stock @Resource.Value</th>
                    }
                    <th width="9%">@Resource.AvailableStock</th>
                    if (ShowValue == "ShowValue")
