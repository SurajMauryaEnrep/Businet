    <div id="ProductionDetail" class="modal fade" role="dialog">
        <div class="modal-dialog" style="max-width:1200px;">
            <!-- Modal content-->
            @Html.Partial("~/Areas/Common/Views/PartialProductionDetail.cshtml")

        </div>
    </div>
    <!-- QC Detail -->
    <div id="QCDetail" class="modal fade" role="dialog">
        <div class="modal-dialog" style="max-width:650px;">
            <!-- Modal content-->
            @Html.Partial("~/Areas/Common/Views/Cmn_ReasonRemarks.cshtml")
            @*@Html.Partial("~/Areas/Common/Views/PartialQCDetail.cshtml")*@

        </div>
    </div>
