@model EnRepMobileWeb.MODELS.ApplicationLayer.FinancialAccountingAndControl.MIS.AccountReceivable.AccountReceivableModel
@using EnRepMobileWeb.Resources;
@using System.Data
<script src="~/Content/Css/jquery/dist/jquery.min.js"></script>
<script src="~/Content/Css/build/js/sweetalert.js"></script>
<script src="~/Content/Scripts/ApplicationLayer/FinancialAccountingAndControl/MIS/AccountReceivable/AccountReceivable.js"></script>
@if (Session["UserId"] == null)
{
    Response.Redirect("~/Login");
}
@{
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}
@{
    //if (Session["Message"] != null)
    if (Model.Message != null)
    {
        //if (Session["Message"].ToString() == "Save")
        if (Model.Message == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
            </script>
        }
        //Session["Message"] = "";
        Model.Message = null;
    }
}
<title>@Model.Title</title>


@using (Html.BeginForm("UserRangeSave", "AccountReceivable", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    {
        @*<title>@Model.Title</title>*@

        <div class="row">
            <!-- Account Receivable Invoice Details Start -->
            <div id="InvoiceDetails" class="modal fade" role="dialog">
                <div class="modal-dialog" style=" max-width:800px;">
                    <div id="PartialInvDetail">
                        @Html.Partial("~/Areas/ApplicationLayer/Views/Shared/PartialAccountReceivableInvoiceDetails.cshtml", Model)
                    </div>
                    <!-- Modal content-->
                </div>
            </div>
            <!-- Account Receivable Invoice Details End -->
            <!-- Advance Payment Details Start -->
            <div id="AdvancePaymentDetails" class="modal fade" role="dialog">
                <div class="modal-dialog" style=" max-width:800px;">
                    <div id="AdvancePaymentDetailsPopup">
                        @Html.Partial("~/Areas/ApplicationLayer/Views/Shared/PartialAdvancePaymentDetails.cshtml", Model)
                    </div>
                    <!-- Modal content-->
                </div>
            </div>
            <!-- Advance Payment Details End -->
            <!-- Paid Amount Details Start -->@*/*Add by Hina Sharma on 10-12-2024*/*@
            <div id="PaidAmountDetails" class="modal fade" role="dialog">
                <div class="modal-dialog" style=" max-width:800px;">
                    <div id="PaidAmountDetailsPopup">
                        @Html.Partial("~/Areas/Common/Views/Cmn_PartialPaidAmountDetails.cshtml")

                    </div>
                    <!-- Modal content-->
                </div>
            </div>
            <!-- Paid Amount Details End -->
            <!-- Mail Information -->
            <div id="MailInfo" class="modal fade" role="dialog">
                <div class="modal-dialog" style="max-width:600px;">
                    <!-- Modal content-->
                    @Html.Partial("~/Areas/Common/Views/Cmn_MailInfomation.cshtml")
                </div>
            </div>
            <!-- Multiple Mail Information -->
            <div id="MultipleMailInfo" class="modal fade" role="dialog">
                <div class="modal-dialog" style="max-width:800px;">
                    <!-- Modal content-->
                    @Html.Partial("~/Areas/Common/Views/Cmn_MultipleMailInfo.cshtml")
                </div>
            </div>
            <!-- Mail View -->
            <div id="MailView" class="modal fade" role="dialog">
                <div class="modal-dialog" style="max-width:800px;">
                    <!-- Modal content-->
                    @Html.Partial("~/Areas/Common/Views/Cmn_MailView.cshtml")
                </div>
            </div>

            <!-- Log View -->
            <div id="LogView" class="modal fade" role="dialog">
                <div id="IdLogView" class="modal-dialog" style="max-width:1000px !important;">
                    <!-- Modal content-->
                    @Html.Partial("~/Areas/Common/Views/Cmn_LogView.cshtml")
                </div>
            </div>
            <div class="col-md-12 col-sm-12 ">
                <div class="x_panel mar_pad1 ">
