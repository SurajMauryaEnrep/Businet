@using EnRepMobileWeb.Resources;
@model EnRepMobileWeb.MODELS.ApplicationLayer.InventoryManagement.Assembly_Kit.AssemblyKit_ListModel
@{
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}
<script src="~/Content/Css/build/js/sweetalert.js"></script>
<script src="~/Content/Scripts/ApplicationLayer/InventoryManagement/AssemblyKit/AssemblyKit_JS.js"></script>
@*/*start Add by Hina on 08-02-2024 to chk Financial year exist or not*/*@
@{
    if (TempData["Message"] != null)
    {
        if (TempData["Message"] == "Financial Year not Exist")
        {
            <script type="text/javascript">
                swal("", $("#FinancialYearDoesNotExistTransactionCanNotBeDone").text(), "warning");
            </script>
        }

    }
}
@*/*End to chk Financial year exist or not*/*@
<title>@Model.Title</title>
<div class="row">
    <!-- Item Information -->
    <div id="ItemInfo" class="modal fade" role="dialog">
        <div class="modal-dialog" style=" max-width:700px;">
            <!-- Modal content-->
            @Html.Partial("~/Areas/Common/Views/_ItemInfomation.cshtml")
        </div>
    </div>
    <!-- Workflow Information -->
    <div id="WorkflowInformation" class="modal fade" role="dialog">
        <div class="modal-dialog" style="max-width:700px;">
            <div class="price_item_detail">
                <!-- Modal content-->
                @Html.Partial("~/Areas/Common/Views/_WorkFlow.cshtml")
            </div>
        </div>
    </div>
    <div class="col-md-12 col-sm-12 ">
        <div class="x_panel mar_pad1 ">
            <div class="x_content mar_pad1">
                <h2 class="col-md-7 col-sm-12 heading_left_padding ">@ViewBag.MenuPageName</h2>
                <div id="wizard" class="form_wizard wizard_horizontal col-md-3 col-sm-12 work_flow">
                    @Html.Partial("~/Areas/Common/Views/_PartialWorkflowBar.cshtml")
                </div>
                @if (ViewBag.VBRoleList != null)
                {
                    foreach (System.Data.DataRow Row in ViewBag.VBRoleList.Rows)
                    {
                        if (Row["feature_id"].ToString() == "105102160001")
                        {
                            <div id="" class="col-md-2 col-sm-12 heading_right_padding">
                                <ul>
                                    <li>
                                        <a class="tooltip-test"
                                           title=" @Resource.AddNew" href="@Url.Action("AddAssemblyKitDetail","AssemblyKit")"><img src="~/Content/Images/addgreen.png" width="32" height="32" id="Btn_AddNewFGR" alt="Add"></a>
                                    </li>
                                </ul>
                            </div>
                        }
                    }
                }
                <input type="hidden" id="DocumentMenuId" value="@ViewBag.DocumentMenuId" />
                @Html.HiddenFor(model => model.DocumentMenuId, new { @id = "DocumentMenuId" })
                @Html.HiddenFor(m => m.ListFilterData, new { @id = "ListFilterData" })
                @Html.HiddenFor(m => m.WF_Status, new { @id = "WF_Status" })
                <div class="x_title pad_mar ">
                    <div class="col-md-12 col-sm-12 icon_tool padding_zero">
                        <div class="col-md-12 col-sm-12 padding_zero">
                            <div class="col-md-2 col-sm-12">
                                <label class="col-form-label label-align">@Resource.Assembly @Resource.Product</label>

                                @Html.DropDownListFor(model => model.AssemblyProduct, new SelectList(Model.ItemNameList, "Item_ID", "Item_Name"),
        new { @class = "form-control", id = "ddlAssemblyProduct", @onchange = "return OnchangeItemName()" })
                            </div>
                            <div class="col-md-2 col-sm-12 pl-0">
                                <label class="col-form-label label-align">@Resource.FromDate</label>
                                @*<input class="form-control" id="txtFromdate" max="2024-09-17" name="FromDate" onchange="FromToDateValidation()" type="date" value="2024-09-01">*@
                                @Html.TextBoxFor(model => model.FromDate, new { @class = "form-control", @id = "txtFromdate", @onchange = "FromToDateValidation()", @type = "date", @max = DateTime.Now.ToString("yyyy-MM-dd") })
                                @Html.HiddenFor(model => model.FromDate, new { id = "hdFromdate" })
                            </div>
                            <div class="col-md-2 col-sm-12 pl-0">
                                <label class="col-form-label label-align">@Resource.ToDate</label>
                                @*<input value="2024-09-17" class="form-control" data-val="true" data-val-date="The field ToDate must be a date." data-val-required="The ToDate field is required." id="txtTodate" max="2024-09-17" name="ToDate" onchange="FromToDateValidation()" placeholder="dd-MM-yyyy" type="date">*@
                                @if (Model.ToDate != null)
                                {
                                    @Html.TextBoxFor(model => model.ToDate, new { @class = "form-control", @id = "txtTodate", @placeholder = "dd-MM-yyyy", @onchange = "FromToDateValidation()", @type = "date", @max = DateTime.Now.ToString("yyyy-MM-dd"), @Value = Model.ToDate })
                                }
                                else
                                {
                                    @Html.TextBoxFor(model => model.ToDate, new { @class = "form-control", @id = "txtTodate", @placeholder = "dd-MM-yyyy", @onchange = "FromToDateValidation()", @type = "date", @max = DateTime.Now.ToString("yyyy-MM-dd"), @Value = @DateTime.Now.ToString("yyyy-MM-dd") })
                                }
                            </div>
                            <div class="col-md-2 col-sm-12 pl-0">
                                <label class="col-form-label label-align">@Resource.Status</label>
                                @*<select class="form-control" id="ddlStatus" name="Status">
                                        <option value="0">All</option>
                                        <option value="A">Approved</option>
                                        <option value="C">Cancelled</option>
                                        <option value="D">Drafted</option>
                                        <option value="F">Forwarded</option>
                                        <option value="IN">Invoiced</option>
                                        <option value="PC">Pending For Costing</option>
                                    </select>*@
                                @Html.DropDownListFor(model => model.Status, new SelectList(Model.StatusList, "status_id", "status_name"), new { @class = "form-control", id = "ddlStatus" })
                            </div>
                            <div class="col-md-1 col-sm-12 pl-0">
                                <button type="button" class="btn btn-primary btn-sm margintop20" id="Btn_Search" onclick="BtnSearch()"><i class="fa fa-search" aria-hidden="true"></i></button>
                            </div>
                        </div>

                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12 col-sm-12 ">
        <div class="x_panel mar_pad1 ">
            <div class="x_content mar_pad1">
                <div class="card-box table-responsive dy_table">
                    <div id="AssKitdiv" class="sticky-table sticky-ltr-cells sb-container container-example1">
                        @Html.Partial("~/Areas/ApplicationLayer/Views/Shared/PartialAssemblyKitList.cshtml")
                        @*<img src="~/ImportExcelFiles/agriculture.png" />*@

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- page content end -->
