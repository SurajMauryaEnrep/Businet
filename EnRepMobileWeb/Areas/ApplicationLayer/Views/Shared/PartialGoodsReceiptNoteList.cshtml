@model EnRepMobileWeb.MODELS.ApplicationLayer.InventoryManagement.MaterialReceipt.GoodsReceiptNote.GRN_ListModel

@using EnRepMobileWeb.Resources;
@{
    var rowId = 0;
}
<script src="~/Content/Scripts/Common/CommonPartialJS.js"></script>
<script src="~/Content/Scripts/ApplicationLayer/InventoryManagement/MaterialReceipt/GoodsReceiptNote/GoodsReceiptNoteList.js"></script>

<table id="datatable-buttons" class="table table-striped table-bordered width1700 rowhighlighttbl itmfrz">
    <thead>
        <tr class="sticky-header">
            <th width="3%">@Resource.SrNo</th>
            <th width="8%" class="itmStick">@Resource.GRNNumber</th>
            <th width="8%">@Resource.GRNDate</th>
            <th style="display:none;">@Resource.GRNDate</th>
            <th width="9%">@Resource.DeliveryNote @Resource.Number</th>
            <th width="9%">@Resource.DeliveryNote @Resource.Date</th>
            <th width="9%">@Resource.BillNumber</th>
            <th width="9%">@Resource.BillDate</th>
            <th width="15%">@Resource.Supplier</th>
            <th>@Resource.Status</th>
            <th width="8%">@Resource.CreatedBy</th>
            <th width="8%">@Resource.Createdon</th>
            <th width="8%">@Resource.ApprovedBy</th>
            <th width="8%">@Resource.ApprovedOn</th>
            <th width="8%">@Resource.AmendedBy</th>
            <th width="8%">@Resource.AmendedOn</th>

        </tr>
    </thead>
    <tbody id="">
        @if (Model.GRNList != null)
        {
            foreach (var row in Model.GRNList)
            {
                rowId++;
        <tr>
            <td>@rowId</td>
            <td id="GRNNo" class="ItmNameBreak itmStick tditemfrz">@row.GRNNo</td>
            <td>@row.GRNDate</td>
            <td style="display: none;" id="MrDate">@row.MrDate</td>
            <td>@row.DeliveryNoteNo</td>
            <td>@row.DeliveryNoteDate</td>
            <td>@row.bill_no</td>
            <td>@row.bill_dt</td>
            <td>@row.SuppName</td>
            @*<td>@row.Currency</td>*@
            <td id="Doc_Status">@row.Stauts</td>
            <td>@row.create_by</td>
            <td>@row.CreateDate</td>
            <td>@row.app_by</td>
            <td>@row.ApproveDate</td>
            <td>@row.mod_by</td>
            <td>@row.ModifyDate</td>
            @*<td align="right">@row.ModifyDate</td>*@

        </tr>
            }
        }
    </tbody>
</table>
@if (Model.GRNSearch != null)
{
    if (Model.GRNSearch == "GRN_Search")
    {
        @Scripts.Render("~/Content/Scripts/DataTableRefreshJs")
        @Scripts.Render("~/Content/Scripts/DatatablesJs")
        //Session["GRNSearch"] = null;
    }
}

