@{
    if (Model.Message != null)
    {
        Model.hdnsaveApprovebtn = null;
        var message = Model.Message;
        if (message == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
            </script>
        }
        if (message == "Deleted")
        {
            <script type="text/javascript">
                swal("", $("#deletemsg").text(), "success");
            </script>
        }
        if (message == "Approved")
        {
            <script type="text/javascript">
                swal("", $("#approvemsg").text(), "success");
            </script>
        }
        if (message == "Cancelled")
        {
            <script type="text/javascript">
                swal("", $("#cancelmsg").text(), "success");
            </script>
        }
        if (message == "Used")
        {
            <script type="text/javascript">
                swal("", $("#RFQHasBnPrcsdCanNotBeMdfd").text(), "warning");
            </script>
        }
        if (message == "Approved_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    swal("", $("#approvemsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        if (message == "Cancelled_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    swal("", $("#cancelmsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        if (message == "InvoiceCannotbeModified")
        {
            <script type="text/javascript">
                swal("", $("#InvoiceCannotbeModified").text(), "warning");
            </script>
        }
        if (message == "PaymentsHasBeenAdjustedAgainstThisInvoiceItCanNotBeModified")
        {
            <script type="text/javascript">
                swal("", $("#PaymentsHasBeenAdjustedAgainstThisInvoiceItCanNotBeModified").text(), "warning");
            </script>
        }
        if (Model.Message == "DocModify")
        {
            <script type="text/javascript">
                swal("", $("#SourceDocumentHasBeenModifiedCanNotProceed").text(), "warning");
            </script>
        }
        if (Model.Message == "ErrorInMail")
        {
            <script type="text/javascript">
                swal("", $("#Emailsendingfailedduetotechnicalissue").text(), "warning");
            </script>
        }
        //Session["Message"] = null;
    }
    // Code Added by Suraj on 05-12-2024
    if (Model.BtnName == "BtnEdit" && Model.Command == "Edit" && Model.DocumentStatus == "D")
    {
        <script type="text/javascript">
            $(document).ready(function () {
                var gst_number = $("#ship_add_gstNo").val();
                var GstApplicable = $("#Hdn_GstApplicable").text();
                if (GstApplicable == "Y") {
                    Cmn_OnSaveAddressApplyGST(gst_number, "ServicePIItemTbl", "hfItemID", "SNohiddenfiled", "TxtItemGrossValue");
                }
            });
        </script>
    }
    // Code Added by Suraj on 05-12-2024 End
}
