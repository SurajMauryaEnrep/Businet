@{
    /**Created By Nitesh 02-03-2024 For Check Qty ="0.000" When Set Placeholder**/
    var QtyVsPlaceholder = "";
    void CheckValueNullPlaceholder(string value)
    {

        if ((value.ToString() == "") || (value == null))
        {
            QtyVsPlaceholder = "";
        }
        else
        {
            if (Convert.ToDecimal(value) == Convert.ToDecimal(0))
            {
                QtyVsPlaceholder = "";
            }
            else
            {
                // QtyVsPlaceholder = Case(value.ToString(), "0", "", "same");
                QtyVsPlaceholder = value;
            }

        }
    }
    /**----------------------------end ------------------------------------------**/
}
