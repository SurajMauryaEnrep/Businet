@model EnRepMobileWeb.MODELS.ApplicationLayer.ProductionAndPlanning.ProductionOrder.ProductionOrderModel
@using EnRepMobileWeb.Resources;
@using System.Data
@if (Session["UserId"] == null)
{
    Response.Redirect("~/Login");
}
@{
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}
<script src="~/Content/Css/jquery/dist/jquery.min.js"></script>
<script src="~/Content/Css/build/js/sweetalert.js"></script>
@*/*start Add by Hina on 19-02-2024 to chk Financial year exist or not*/*@
@{
    if (TempData["Message"] != null)
    {
        if (TempData["Message"] == "Financial Year not Exist")
        {
            <script type="text/javascript">
                swal("", $("#FinancialYearDoesNotExistTransactionCanNotBeDone").text(), "warning");
            </script>
        }

    }
}
@*/*End to chk Financial year exist or not*/*@
<script src="~/Content/Scripts/ApplicationLayer/ProductionAndPlanning/ProductionOrder/ProductionOrderList.js"></script>
<title>@Model.Title</title>
@using (Html.BeginForm("ProductionOrder", "ProductionOrder", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="row">
        <!-- Workflow Information -->
        <div id="WorkflowInformation" class="modal fade" role="dialog">
            <div class="modal-dialog" style="max-width:700px;">
                <!-- Modal content-->
                @Html.Partial("~/Areas/Common/Views/_WorkFlow.cshtml")
            </div>
        </div>
        <div class="col-md-12 col-sm-12 ">
            <div class="x_panel mar_pad1 ">
                <div class="x_content mar_pad1">
                    <h2 class="col-md-5 col-sm-12 heading_left_padding ">@ViewBag.MenuPageName</h2>
                    <div id="wizard" class="form_wizard wizard_horizontal col-md-3 col-sm-12 work_flow">
                        @Html.Partial("~/Areas/Common/Views/_PartialWorkflowBar.cshtml")
                    </div>
                    @*@Html.HiddenFor(m => m.ListFilterData, new { @id = "ListFilterData" })*@
                    @Html.HiddenFor(m => m.WF_Status, new { @id = "WF_Status" })
                    <input hidden="hidden" id="ListFilterData" value="@ViewBag.ListData" />
                    <input type="hidden" id="DocumentMenuId" value="@ViewBag.DocumentMenuId" />
                    @if (ViewBag.VBRoleList != null)
                    {
                        foreach (System.Data.DataRow Row in ViewBag.VBRoleList.Rows)
                        {
                            if (Row["feature_id"].ToString() == "105105125001")
                            {
                                <div id="" class="col-md-4 col-sm-12 heading_right_padding">
                                    <ul>
                                        <li>
                                            <a class="tooltip-test"
                                               title=" @Resource.AddNew" href="@Url.Action("AddNewProductionOrderDetail", "ProductionOrder")"><img src="~/Content/Images/addgreen.png" width="32" height="32" id="Btn_AddCust" alt="Add"></a>
                                        </li>
                                    </ul>
                                </div>
                            }
                        }
                    }
                    <div class="x_title pad_mar ">
                        <div class="col-md-12 col-sm-12 icon_tool padding_zero">
                            <form class="available_stock pad_mar" action="" method="post" novalidate="">
                                <div class="col-md-12 col-sm-12 padding_zero">

                                    <div class="col-md-4 col-sm-12">
                                        <label class="col-form-label label-align">@Resource.ProductName</label>
                                        <select class="form-control" id="ddl_ProductNameListPage" name="ddl_ProductNameListPage" onchange="ddl_ProductNameListPage_onchange()">
                                        </select>
                                        @Html.HiddenFor(model => model.product_id, new { @id = "hdn_product_idListPage" })
                                    </div>
                                    <div class="col-md-2 col-sm-12 pl-0">
                                        <label class="col-form-label label-align">@Resource.OperationName</label>
                                        @Html.DropDownListFor(model => model.ddl_OperationName, new SelectList(Model.OperationNameList, "op_id", "op_name"), new { @class = "form-control", id = "ddl_OperationName" })
                                        @Html.ValidationMessageFor(model => model.ddl_OperationName, String.Empty, new { @style = "color:red;!important", @id = "span_vm_ddl_OperationName" })
                                        @Html.HiddenFor(model => model.ddl_OperationName, new { @id = "hdn_OperationName" })
                                    </div>
                                    <div class="col-md-2 col-sm-12 pl-0">
                                        <label class="col-form-label label-align">@Resource.ShopfloorName</label>
                                        @Html.DropDownListFor(model => model.ddl_ShopfloorName, new SelectList(Model.ShopFloorList, "shfl_id", "shfl_name"), new { @class = "form-control", id = "ddl_ShopfloorName" })
                                        @Html.ValidationMessageFor(model => model.ddl_ShopfloorName, String.Empty, new { @style = "color:red;!important", @id = "vm_ddl_ShopfloorName" })
                                        @Html.HiddenFor(model => model.ddl_ShopfloorName, new { @id = "hdn_ShopfloorName" })
                                    </div>
                                    <div class="col-md-2 col-sm-12 pl-0">
                                        <label class="col-form-label label-align"> @Resource.WorkstationName <span class="required">*</span></label>
                                       
                                            @Html.DropDownListFor(model => model.ddl_WorkstationName, new SelectList(Model.WorkstationNameList, "ws_id", "ws_name"), new { @class = "form-control", id = "ddl_WorkstationName"})
                                            @Html.ValidationMessageFor(model => model.ddl_WorkstationName, String.Empty, new { @style = "color:red;!important", @id = "span_vm_ddl_WorkstationName" })
                                            @Html.HiddenFor(model => model.ddl_WorkstationName, new { @id = "hdn_WorkstationName" })
                                            @Html.HiddenFor(model => model.ddl_WorkstationText, new { @id = "hdn_WorkstationText" })
                                    </div>
                                    <div class="col-md-2 col-sm-12 pl-0">
                                        <label class="col-form-label label-align">@Resource.Shift <span class="required">*</span></label>
                                        @Html.DropDownListFor(model => model.ddl_shift, new SelectList(Model.shiftList, "id", "name"), new
                                        {
                                       @class = "form-control",
                                       id = "ddl_shift"/*, onchange = "ddl_ddl_shift_onchange()"*/
                                        })
                                        @Html.ValidationMessageFor(model => model.ddl_shift, String.Empty, new { @style = "color:red;!important", @id = "span_vm_ddl_shift" })
                                        @Html.HiddenFor(model => model.ddl_shift, new { @id = "hdn_shift" })
                                        @Html.HiddenFor(model => model.ddl_shiftName, new { @id = "hdn_shiftName" })
                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="col-md-2 col-sm-12">
                                        <label class="col-form-label label-align">@Resource.FromDate</label>
                                        @Html.TextBoxFor(model => model.txtFromdate, new { @class = "form-control", @id = "txtFromdate", @onchange = "FromToDateValidation()", @type = "date", @max = DateTime.Now.ToString("yyyy-MM-dd") })
                                        @Html.ValidationMessageFor(model => model.txtFromdate, String.Empty, new { @style = "color:red;!important", @id = "vmtxtFromdate" })
                                        @Html.HiddenFor(model => model.txtFromdate, new { id = "hdFromdate" })
                                    </div>
                                    <div class="col-md-2 col-sm-12 pl-0">
                                        <label class="col-form-label label-align">@Resource.ToDate</label>
                                        @if (Model.txtToDate != null)
                                        {
                                            @Html.TextBoxFor(model => model.txtToDate, new { @class = "form-control", @id = "txtToDate", @type = "date", @onchange = "FromToDateValidation()", @max = DateTime.Now.ToString("yyyy-MM-dd"), @value = Model.txtToDate })
                                            @Html.ValidationMessageFor(model => model.txtToDate, String.Empty, new { @style = "color:red;!important", @id = "vmtxtToDate" })
                                        }
                                        else
                                        {
                                            @Html.TextBoxFor(model => model.txtToDate, new { @class = "form-control", @id = "txtToDate", @type = "date", @onchange = "FromToDateValidation()", @max = DateTime.Now.ToString("yyyy-MM-dd") })
                                            @Html.ValidationMessageFor(model => model.txtToDate, String.Empty, new { @style = "color:red;!important", @id = "vmtxtToDate" })
                                        }


                                    </div>
                                    <div class="col-md-2 col-sm-12 no-padding pl-0">
                                        <div class="col-md-9 col-sm-12 pl-0">
                                            <label class="col-form-label label-align">Status</label>
                                            @Html.DropDownListFor(model => model.Status, new SelectList(Model.statusLists, "status_id", "status_name"), new
                                            {
                                                @class = "form-control",
                                           onchange = "ddl_Status_onchange()",
                                                @id = "ddlStatus"
                                            })
                                            @Html.HiddenFor(model => model.Status, new { @id = "hdn_ddlStatus" })
                                        </div>
                                        <div class="col-md-3 col-sm-12 pl-0">
                                            <button type="submit" class="btn btn-primary btn-sm margintop20" id="BtnSearch" name="Command" value="Search"><i class="fa fa-search" aria-hidden="true"></i></button>
                                            @*<button type="button" class="btn btn-primary btn-sm margintop20" id="BtnSearch"><i class="fa fa-search" onclick="FilterJCList()" aria-hidden="true"></i></button>*@
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 ">
            <div class="x_panel mar_pad1 ">
                <div class="x_content mar_pad1">
                    <div class="card-box table-responsive dy_table">
                        <div class="sticky-table sticky-ltr-cells sb-container container-example1">
                            @Html.Partial("~/Areas/ApplicationLayer/Views/Shared/PartialProductionOrderList.cshtml")
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
<!-- page content end -->
