                                    <option value="">APS Boys Grey Trouser SF 1</option>
                                    <option value="">APS Boys White Full Sleeves Shirt</option>
                                </select>
                            </div>
                            <div class="col-md-9 col-sm-12 pl-0">

                                <div class="col-md-2 col-sm-12 pl-0">
                                    <label class="col-form-label label-align">@Resource.UOM</label>
                                    <input class="form-control" data-val="true" id="UOM" name="UOM" readonly="readonly" type="text">
                                </div>
                                <div class="col-md-2 col-sm-12 pl-0">
