                                    <th>@Resource.DefaultLanguage</th>
                                    <th>@Resource.EmailID</th>
                                    <th>@Resource.ContactNumber</th>
                                    <th>@Resource.Active</th>
                                    <th>@Resource.Createdon</th>-->
                                    @*<th>@Resource.ApprovedOn</th>*@
                                    <!--<th>@Resource.AmendedOn</th>
                                </tr>
                            </thead>

                            <tbody id="UserList">-->

                                @Html.Partial("~/Areas/SecurityLayer/Views/Shared/PartialUserList.cshtml")
                            @*</tbody>
                        </table>*@

                    </div>
                </div>

            </div>
        </div>
    </div>

</div>
<!-- page content end -->
