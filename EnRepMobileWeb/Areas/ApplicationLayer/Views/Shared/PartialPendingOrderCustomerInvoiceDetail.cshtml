@using EnRepMobileWeb.Resources;
@{
    string ValDigit = "0.";
    var VD = Convert.ToInt32(Session["ValDigit"].ToString());
    for (var i = 0; i < VD; i++)
    {
        ValDigit += "0";
    }
    string QtyDigit = "0.";
    var QD = Convert.ToInt32(Session["QtyDigit"].ToString());
    for (var i = 0; i < QD; i++)
    {
        QtyDigit += "0";
    }
    string RateDigit = "0.";
    var RD = Convert.ToInt32(Session["ValDigit"].ToString());
    for (var i = 0; i < RD; i++)
    {
        RateDigit += "0";
    }
    decimal totalsale_amt_spec = 0;
    decimal totaltax_amt_spec = 0;
    decimal totalOC_amt_spec = 0;
    decimal totalsale_amt = 0;
    decimal totaltax_amt = 0;
    decimal totalOC_amt = 0;
    decimal totalInvoice_amt = 0;
    decimal totalInvoice_amt_spec = 0;
    decimal orderQty = 0;
    decimal shipQty = 0;
    decimal PendingQty = 0;
}
<script src="~/Content/Scripts/Common/CommonPartialJS.js"></script>
<div class="modal-content">
    <div class="modal-header">
        <h2>@Resource.Order @Resource.Summary</h2>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>
    <div class="modal-body" style="padding-top:0px;">
        <div class="col-md-4 col-sm-12" style="padding-left:0px;">
            <label class="col-form-label label-align">@Resource.CustomerName</label>
            <input id="CustomerName" class="form-control" autocomplete="off" type="text" name="Customer" placeholder="@Resource.CustomerName" disabled="">
            <input hidden="hidden" id="hdnCustId" />
        </div>

        <div class="col-md-2 col-sm-12" style="padding-left:0px;">
            <label class="col-form-label label-align">@Resource.Currency</label>
            <input id="Currency" class="form-control" autocomplete="off" type="text" name="Currency" placeholder="@Resource.Currency" disabled="">
            <input hidden id="Currency_id" />
        </div>
        <div class="col-md-2 col-sm-12" style="padding-left:0px;">
            <label class="col-form-label label-align">@Resource.FromDate</label>
            <input id="FromDate" class="form-control" value="" autocomplete="off" type="date" name="FromDate" placeholder="" disabled="">
        </div>

        <div class="col-md-2 col-sm-12" style="padding-left:0px;">
            <label class="col-form-label label-align">@Resource.ToDate</label>
            <input id="ToDate" class="form-control" value="" autocomplete="off" type="date" name="ToDate" placeholder="" disabled="">
        </div>

        <div class="clearfix"></div>
        @*<div class="tableFixHead height30VH margin10">
            <table id="" class="table table-striped width1800 rowhighlighttbl" cellspacing="0" width="100%">
                <thead>
                    <tr class="headings">*@
        <div class="card-box table-responsive dy_table margin15">
            <div id="" class="sticky-table sticky-ltr-cells sb-container container-example1 OrderHistory height30VH">
                <table id="Datatable23" data-order="" class="table table-striped width1800 table-bordered rowhighlighttbl itmfrz" style="width:100%">
                    <thead>
                        <tr class="sticky-header">

                            <th width="3%">@Resource.SrNo</th>
                            <th width="12%" class="itmStick">@Resource.OrderNumber</th>
                            <th>@Resource.OrderDate</th>
                            <th width="5%">@Resource.Customer @Resource.ReferenceNumber</th>
                            <th hidden="hidden"></th>
                            <th>@Resource.SalesRepresentative</th>
                            <th>@Resource.Ordered <br /> @Resource.Quantity</th>
                            <th>@Resource.Shipped <br /> @Resource.Quantity</th>
                            <th>@Resource.Pending <br /> @Resource.Quantity</th>
                            <th>@Resource.GrossValue <br /> @Resource.InSpecific</th>
                            <th>@Resource.GrossValue <br /> @Resource.InBase</th>
                            <th>@Resource.TaxAmount <br /> @Resource.InSpecific</th>
                            <th>@Resource.TaxAmount <br /> @Resource.InBase</th>
                            <th>@Resource.OtherCharges <br /> @Resource.InSpecific </th>
                            <th>@Resource.OtherCharges <br /> @Resource.InBase </th>
                            <th>@Resource.Order @Resource.Amount  <br /> @Resource.InSpecific </th>
                            <th>@Resource.Order @Resource.Amount  <br /> @Resource.InBase </th>
                            @*<th>@Resource.InvoiceAmount <br /> @Resource.InSpecific</th>
        <th>@Resource.InvoiceAmount <br /> @Resource.InBase</th>*@
                            <th>@Resource.ApprovedOn</th>
                        </tr>
                    </thead>
                    <tbody id="">
                        @if (ViewBag.CWOrderDetailList != null)
                        {
                            var srno = 1;
                            foreach (System.Data.DataRow dr in ViewBag.CWOrderDetailList.Rows)
                            {
                        <tr>
                            <td>@srno</td>
                            <td id="app_inv_no" class="ItmNameBreak itmStick tditemfrz">@dr["app_so_no"].ToString()</td>
                            <td>@dr["so_dt1"].ToString()</td>
                            <td>@dr["ref_doc_no"].ToString()</td>
                            <td hidden="hidden" id="txtInv_dt">@dr["so_dt"].ToString()</td>
                            <td>@dr["sale_per"].ToString()</td>
                            <td class="num_right">@dr["ord_qty_base"].ToString()</td>
                            <td class="num_right">@dr["ship_qty"].ToString()</td>
                            <td class="num_right">@dr["PendingQty"].ToString()</td>
                            <td class="num_right">@Convert.ToDecimal(dr["sale_amount_spec"]).ToString(ValDigit)</td>
                            <td class="num_right">@Convert.ToDecimal(dr["sale_amount_bs"]).ToString(ValDigit)</td>
                            <td class="num_right">@Convert.ToDecimal(dr["tax_amt_spec"]).ToString(ValDigit)</td>
                            <td class="num_right">@Convert.ToDecimal(dr["tax_amt_bs"]).ToString(ValDigit)</td>
                            <td class="num_right">@Convert.ToDecimal(dr["oc_amt_spec"]).ToString(ValDigit)</td>
                            <td class="num_right">@Convert.ToDecimal(dr["oc_amt_bs"]).ToString(ValDigit)</td>
                            <td class="num_right">
                                <div class="col-sm-11">@Convert.ToDecimal(dr["Order_amt_spec"]).ToString(ValDigit) </div>
                                <div class="col-sm-1 i_Icon">
                                    <button type="button" id="" class="calculator iButton" onclick="CW_OnclickPendingInvoiceAmtGetItem(event)" data-toggle="modal" data-target="#CustomerInvoiceProductDetail" data-backdrop="static" data-keyboard="false">  <img src="~/Content/Images/iIcon1.png" alt="" title="@Resource.ProductDetail" /> @*<i class="fa fa-info" aria-hidden="true" title="@Resource.InvoiceDetail"></i>*@ </button>
                                </div>
                            </td>
                            <td class="num_right">
                                <div class="col-sm-11" id="">@Convert.ToDecimal(dr["Order_amt_bs"]).ToString(ValDigit) </div>
                                <div class="col-sm-1 i_Icon">
                                    <button type="button" id="" class="calculator iButton" onclick="CW_OnclickPendingInvoiceAmtGetItem(event)" data-toggle="modal" data-target="#CustomerInvoiceProductDetail" data-backdrop="static" data-keyboard="false">  <img src="~/Content/Images/iIcon1.png" alt="" title="@Resource.ProductDetail" /> @*<i class="fa fa-info" aria-hidden="true" title="@Resource.InvoiceDetail"></i>*@ </button>
                                </div>
                            </td>

                            <td>@dr["app_dt"].ToString()</td>
                        </tr>
                                srno = srno + 1;
                                totalsale_amt_spec = totalsale_amt_spec + Convert.ToDecimal(dr["sale_amount_spec"]);
                                totaltax_amt_spec = totaltax_amt_spec + Convert.ToDecimal(dr["tax_amt_spec"]);
                                totalOC_amt_spec = totalOC_amt_spec + Convert.ToDecimal(dr["oc_amt_spec"]);
                                totalsale_amt = totalsale_amt + Convert.ToDecimal(dr["sale_amount_bs"]);
                                totaltax_amt = totaltax_amt + Convert.ToDecimal(dr["tax_amt_bs"]);
                                totalOC_amt = totalOC_amt + Convert.ToDecimal(dr["oc_amt_bs"]);
                                totalInvoice_amt = totalInvoice_amt + Convert.ToDecimal(dr["Order_amt_bs"]);
                                totalInvoice_amt_spec = totalInvoice_amt_spec + Convert.ToDecimal(dr["Order_amt_spec"]);
                                orderQty = orderQty + Convert.ToDecimal(dr["ord_qty_base"]);
                                shipQty = shipQty + Convert.ToDecimal(dr["ship_qty"]);
                                PendingQty = PendingQty + Convert.ToDecimal(dr["PendingQty"]);
                            }
                        }
                    </tbody>
                    <tfoot>
                        <tr class="total">
                            <td></td>
                            <td></td>
                            <td></td>
                            <td>&nbsp;</td>
                            <td align="right"><strong>@Resource.Total</strong></td>
                            <td class="num_right"><strong>@orderQty.ToString(ValDigit)</strong></td>
                            <td class="num_right"><strong>@shipQty.ToString(ValDigit)</strong></td>
                            <td class="num_right"><strong>@PendingQty.ToString(ValDigit)</strong></td>
                            <td class="num_right"><strong>@totalsale_amt_spec.ToString(ValDigit)</strong></td>
                            <td class="num_right"><strong>@totalsale_amt.ToString(ValDigit)</strong></td>
                            <td class="num_right"><strong>@totaltax_amt_spec.ToString(ValDigit)</strong></td>
                            <td class="num_right"><strong>@totaltax_amt.ToString(ValDigit)</strong></td>
                            <td class="num_right"><strong>@totalOC_amt_spec.ToString(ValDigit)</strong></td>
                            <td class="num_right"><strong>@totalOC_amt.ToString(ValDigit)</strong></td>
                            <td class="num_right"><strong>@totalInvoice_amt_spec.ToString(ValDigit)</strong></td>
                            <td class="num_right"><strong>@totalInvoice_amt.ToString(ValDigit)</strong></td>
                            <td colspan="1" align="right"></td>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal" id="" onclick="OnClickSerialDiscardAndExit()">@Resource.Close</button>
    </div>
</div>





