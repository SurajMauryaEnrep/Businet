@{ var Disable = true;
    var disablePrintbtn = "";/*Add by Hina sharma on 11-10-2024*/
    if (Model.TransType != null)
    {
        if (Model.TransType == "Update")
        {
            var v = Model.DocumentStatus;
            if (Model.Command == "Edit")
            {
                Disable = false;
            }
            else
