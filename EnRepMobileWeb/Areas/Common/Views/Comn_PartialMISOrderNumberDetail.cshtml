@using EnRepMobileWeb.Resources;
<div class="modal-content">
    <div class="modal-header">
        <h2>@Resource.Order   @Resource.Number @Resource.Detail</h2>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>
    <div class="modal-body" style="padding-top: 0px;">
        <div class="row">
            <div class="col-md-3">
                <label class="col-form-label label-align">@Resource.OrderNumber</label>
                <input id="OrderNumber" disabled class="form-control" autocomplete="off" tabindex="5" type="text" name="OrderNumber" placeholder="@Resource.OrderNumber">
            </div>
            <div class="col-md-2" style="padding-left: 0px;">
                <label class="col-form-label label-align"> @Resource.OrderDate</label>
                <input id="OrderDate" disabled class="form-control" autocomplete="off" tabindex="5" type="text" name="OrderDate" placeholder="@Resource.OrderDate">
            </div>
            <div class="col-md-2" style="padding-left: 0px;">
                <label class="col-form-label label-align">@Resource.OrderType</label>
                <input id="OrderType" disabled class="form-control" autocomplete="off" tabindex="5" type="text" name="OrderType" placeholder="@Resource.OrderType">
            </div>
            <div class="col-md-1" style="padding-left: 0px;">
                <label class="col-form-label label-align">@Resource.Currency</label>
                <input id="Currency" disabled class="form-control " autocomplete="off" tabindex="5" type="text" name="Currency" placeholder="@Resource.Currency">
            </div>
            <div class="col-md-4" style="padding-left: 0px;">
                <label class="col-form-label label-align">@Resource.SupplierName</label>
                <input id="SupplierName" disabled class="form-control " autocomplete="off" tabindex="5" type="text" name="SupplierName" placeholder="@Resource.SupplierName">
            </div>

            <div class="col-md-12 col-sm-12 mt-2">
                <div class="x_panel margin_top_zero">
                    <div class="x_content">
                        <div class="sticky-table sticky-ltr-cells sb-container container-example1">
                            <div id="MISOrderItemSummary">@Html.Partial("~/Areas/ApplicationLayer/Views/Shared/PartialMISOrderSummary.cshtml")</div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>    
</div>



