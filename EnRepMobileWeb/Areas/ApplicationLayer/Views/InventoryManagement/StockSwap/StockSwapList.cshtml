@model EnRepMobileWeb.MODELS.ApplicationLayer.InventoryManagement.Stock_Swap.StockSwap_Model
@using EnRepMobileWeb.Resources;
@using System.Data
@{
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}
<script src="~/Content/Css/jquery/dist/jquery.min.js"></script>
<script src="~/Content/Css/build/js/sweetalert.js"></script>
@*/*start Add by Hina on 14-02-2024 to chk Financial year exist or not*/*@
@{
    if (TempData["Message1"] != null)
    {
        if (TempData["Message1"] == "Financial Year not Exist")
        {
            <script type="text/javascript">
                swal("", $("#FinancialYearDoesNotExistTransactionCanNotBeDone").text(), "warning");
            </script>
        }
    }
}
@*/*End to chk Financial year exist or not*/*@
<script src="~/Content/Scripts/ApplicationLayer/InventoryManagement/StockSwap/StockSwap.js"></script>
<title>@Model.Title</title>
<div class="row">
    <!-- Workflow Information -->
    <div id="WorkflowInformation" class="modal fade" role="dialog">
        <div class="modal-dialog" style="max-width:700px;">
            <!-- Modal content-->
            @Html.Partial("~/Areas/Common/Views/_WorkFlow.cshtml")
        </div>
    </div>
    <div class="col-md-12 col-sm-12 ">
        <div class="x_panel mar_pad1 ">
            <div class="x_content mar_pad1">
                <h2 class="col-md-5 col-sm-12 heading_left_padding ">@ViewBag.MenuPageName</h2>
                <!-- Smart Wizard -->
                <div id="wizard" class="form_wizard wizard_horizontal col-md-3 col-sm-12 work_flow">
                    @if (ViewBag.AppLevel.Rows.Count > 0)
                    {
                        @Html.Partial("~/Areas/Common/Views/_PartialWorkflowBar.cshtml")
                    }
                </div>
                @Html.HiddenFor(m => m.WF_status, new { @id = "WF_status" })
                <input type="hidden" id="DocumentMenuId" value="@ViewBag.DocumentMenuId" />
                <div id="" class="col-md-4 col-sm-12 heading_right_padding">

                    <ul>
                        @if (ViewBag.VBRoleList != null)
                        {
                            foreach (System.Data.DataRow Row in ViewBag.VBRoleList.Rows)
                            {
                                if (Row["feature_id"].ToString() == "105102165001")
                                {
                                    <li>
                                        <a class="tooltip-test"
                                           title=" @Resource.AddNew" href="@Url.Action("AddStockSwapDetail","StockSwap")"><img src="~/Content/Images/addgreen.png" width="32" height="32" id="Btn_AddCust" alt="Add"></a>
                                    </li>
                                }

                            }
                        }

                    </ul>
                </div>

                <div class="x_title pad_mar ">
                    <div class="col-md-12 col-sm-12 icon_tool ">
                        <div class="col-md-2 col-sm-12">
                            <div id="SupplierName">
                                <label class="col-form-label label-align">@Resource.Source @Resource.Product </label>
                                @Html.DropDownListFor(m => m.ProductName, new SelectList(Model._ProductNameList, "ProductId", "ProductName"), new { @class = "form-control", @id = "ddlItemNameList" })
                                @*<select class="form-control" id="" name="">
                                        <option value="0">All</option>
                                        <option value="">Source Product</option>
                                        <option value="">Source Product 1</option>
                                        <option value="">Source Product 2</option>
                                        <option value="">Source Product 3</option>
                                        <option value="">Source Product 4</option>
                                        <option value="">Source Product 5</option>
                                    </select>*@
                            </div>
                        </div>
                        <div class="col-md-2 col-sm-12 pl-0">
                            <label class="col-form-label label-align">@Resource.Destination @Resource.Product </label>
                            @Html.DropDownListFor(m => m.ProductName1, new SelectList(Model._ProductNameList, "ProductId", "ProductName"), new { @class = "form-control", @id = "ddlItemNameDestList", onchange = "OnchangeProductName('Dest')" })
                            @*<select class="form-control" id="" name="">
                                    <option value="0">All</option>
                                    <option value="">Destination Product</option>
                                    <option value="">Destination Product 1</option>
                                    <option value="">Destination Product 2</option>
                                    <option value="">Destination Product 3</option>
                                    <option value="">Destination Product 4</option>
                                    <option value="">Destination Product 5</option>
                                </select>*@
                        </div>
                        <div class="col-md-2 col-sm-12 pl-0">
                            <label class="col-form-label label-align">
                                @Resource.FromDate
                            </label>
                            @Html.TextBoxFor(model => model.FromDate, new { @class = "form-control", @id = "txtFromdate", @onchange = "FromToDateValidation()", @type = "date", @max = DateTime.Now.ToString("yyyy-MM-dd") })

                            @*<input class="form-control" id="" max="" name="FromDate" onchange="" type="date" value="">*@
                        </div>
                        <div class="col-md-2 col-sm-12 pl-0">
                            <label class="col-form-label label-align">
                                @Resource.ToDate
                            </label>
                            @if (Model.Swp_ToDate != null)
                            {
                                @Html.TextBoxFor(model => model.ToDate, new { @class = "form-control", @id = "txtTodate", @placeholder = "dd-MM-yyyy", @onchange = "FromToDateValidation()", @type = "date", @max = DateTime.Now.ToString("yyyy-MM-dd"), @Value = Model.Swp_ToDate })
                            }
                            else
                            {
                                @Html.TextBoxFor(model => model.ToDate, new { @class = "form-control", @id = "txtTodate", @placeholder = "dd-MM-yyyy", @onchange = "FromToDateValidation()", @type = "date", @max = DateTime.Now.ToString("yyyy-MM-dd"), @Value = @DateTime.Now.ToString("yyyy-MM-dd") })
                            }
                            @*<input class="form-control" id="" max="" name="" onchange="" type="date" value="">*@
                        </div>
                        <div class="col-md-2 col-sm-12 pl-0">
                            <label class="col-form-label label-align">
                                @Resource.Status
                            </label>
                            @Html.DropDownListFor(model => model.ListStatus, new SelectList(Model.StatusList, "status_id", "status_name"), new { @class = "form-control", id = "ddlStatus" })
                            @*<select class="form-control" id="ddlStatus" name="Status">
                                    <option value="0">All</option>
                                    <option value="A">Approved</option>
                                    <option value="C">Cancelled</option>
                                    <option value="D">Drafted</option>
                                    <option value="F">Forwarded</option>
                                    <option value="GR">Goods Received</option>
                                    <option value="QP">QC Pending</option>
                                </select>*@

                        </div>
                        <div class="col-md-1 col-sm-12 pl-0">
                            <button type="button" class="btn btn-primary btn-sm margintop20" id="BtnSearch" onclick="SwapStockListSearch()"><i class="fa fa-search" aria-hidden="true"></i></button>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12 col-sm-12">
        <div class="x_panel margin_top_zero">
            <div class="x_content">
                <div class="card-box table-responsive dy_table">
                    <div id="SwapStockBody" class="sticky-table sticky-ltr-cells sb-container container-example1">
                        @Html.Partial("~/Areas/ApplicationLayer/Views/Shared/PartialStockSwapList.cshtml")
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


