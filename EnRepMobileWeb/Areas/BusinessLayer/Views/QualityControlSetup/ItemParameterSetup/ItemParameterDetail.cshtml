                                                    @Html.TextAreaFor(model => model.remarks, new { @class = "form-control remarksmessage", @id = "detailremarks", @placeholder = Resource.remarks, maxlength = "100" })
                                                </div>
                                                <div class="col-md-1 col-sm-12 margin25 ">
                                                    <div class="col-md-2 col-sm-12">
                                                        <a href="#">
                                                            <div class="plus_icon1" id="divNew"> <i class="fa fa-plus" id="ParaNewAddBtn" onclick="OnClickAddParaNewAddBtn()" title="@Resource.AddNew"></i> </div>
                                                        </a>
                                                    </div>
                                                    <div class="col-md-1 col-sm-12">
                                                        <a href="#" style="color:#ffffff;">
                                                            <div class="plus_icon1" id="divUpdate" style="display:none;"> <i class="fa fa-refresh ref" id="ParaItemUpdateBtn" onclick="OnClickParaItemUpdateBtn()" title="@Resource.Update"></i> </div>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>                                    
                                    <div class="table-responsive width99 margin10">
                                        @Html.HiddenFor(model => model.hdnItemList, new { @id = "hdnItemList" })
                                        <table class="table table-striped jambo_table bulk_action" id="QCItemParameterDetailTbl">
                                            <thead>
                                                <tr class="headings">
                                                    <th>@Resource.SrNo </th>
                                                    <th class="column-title" width="3%"></th>
                                                    <th class="column-title" width="3%"></th>

                                                    <th class="column-title">@Resource.ParameterName </th>
                                                    <th class="column-title">@Resource.ParameterType </th>
                                                    <th class="column-title">@Resource.LowerRange </th>
                                                    <th class="column-title">@Resource.UpperRange </th>
                                                    <th class="column-title">@Resource.UOM</th>
                                                    <th class="column-title">@Resource.remarks </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @if (ViewBag.datatbl != null)
                                                {
                                                    var rowIdx = 0;
                                                    foreach (System.Data.DataRow Row in ViewBag.datatbl.Rows)
                                                    {
                                                        rowIdx++;
                                                <tr id="@rowIdx">
                                                    <td id="sr_no">@Row["sr_no"].ToString()</td>
                                                    @if (@Row["CheckDependcy"].ToString() == "Use")
                                                    {
                                                        <td class="center" > <i class="fa fa-trash" aria-hidden="true" id=""  title="@Resource.Delete" disabled></i></td>
                                                        
                                                    }
                                                    else
                                                    {
                                                        <td class="red center"> <i class="fa fa-trash" aria-hidden="true" id="delBtnIcon" title="@Resource.Delete"></i></td>
                                                    }

                                                        <td class="center edit_icon"> <i class="fa fa-edit" aria-hidden="true" id="editBtnIcon" title="@Resource.Edit"></i></td>
                                                    
                                                    <td id="ItemParameterNametblValue">@Row["ParaName"].ToString()</td>
                                                    <td id="ParameterTypetblValue">@Row["ParamTypeName"].ToString()</td>
                                                    <td id="LowerRangetblValue">@Row["LowerValue"].ToString()</td>
                                                    <td id="UpperRangetblValue">@Row["UpperValue"].ToString()</td>
                                                    <td id="ParamUom">@Row["uom_name"].ToString()</td>
                                                    <td id="RemarkstblValue">@Row["remarks"].ToString()</td>
                                                    <td hidden id="ItemParameterNametblValue1">@Row["param_Id"].ToString()</td>
                                                    <td hidden id="hduom_cd">@Row["uom_id"].ToString()</td>
                                                </tr>
                                                    }
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>

    }
}

