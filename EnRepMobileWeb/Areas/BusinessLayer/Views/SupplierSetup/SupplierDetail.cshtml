@{
    var Disable = true;
    if (Model.TransType != null)
    {
        //if (Session["TransType"].ToString() == "Update")
        if (Model.TransType == "Update")
        {
            //if (Session["Command"].ToString() == "Edit")
            if (Model.Command == "Edit")
            {
                Disable = false;
            }
            else
            {
                Disable = true;
            }
        }
        else
        {
            Disable = false;
        }
        //if (Session["TransType"].ToString() == "Refresh")
        if (Model.TransType == "Refresh")
        {
            Disable = true;
        }
        //if (Session["TransType"].ToString() == "Duplicate")
        if (Model.TransType == "Duplicate")
        {
            Disable = false;
        }
        //if (Session["TransType"].ToString() == "Save")
        if (Model.TransType == "Save")
        {
            //if (Session["Command"].ToString() == "Add")
            if (Model.Command == "Add")
            {
                Disable = false;
            }
            else
            {
                Disable = true;
            }
        }
    }
    if (Model.Message != null)
    {
        //if (ViewBag.Message == "Duplicate")
        if (Model.Message == "Duplicate")
        {
            Disable = false;
        }
    }
    if (Disable)
    {
        Model.PageLevelDisable = "Y";
    }
    else
    {
        Model.PageLevelDisable = "N";
    }
}

@{
    if (Model.Command != null)
    {
        //if (Session["Command"].ToString() == "Edit")
        if (Model.Command == "Edit")
        {
            <script type="text/javascript">
                $("#act_status").prop("disabled", false);
                $("#supp_hold").prop("disabled", false);
            </script>
        }
    }
    if (Model.Message != null)
    {
        var str = Model.Message;
        Model.Saveapprovebtn = "ABC";
        //Model.Saveapprovebtn = null;
        <script>
            if ("@str" == "Save") {
                $(document).ready(function () {
                    $("#hdnSavebtn").val("");
            })
            swal("", $("#savemsg").text(), "success");
        }
        if ("@str" == "Deleted") {
            $(document).ready(function () {
                $("#hdnSavebtn").val("");
            })
            swal("", $("#deletemsg").text(), "success");
        }
        if ("@str" == "Approved") {
            $(document).ready(function () {
                $("#hdnSavebtn").val("");
            })
            swal("", $("#approvemsg").text(), "success");
        }
        if ("@str" == "Duplicate") {

            $(document).ready(function () {
                $("#hdnSavebtn").val("");
                document.getElementById("vmSuppname").innerHTML = $("#valueduplicate").text();
                $("#supp_name").css("border-color", "red");
            });
        }
        if ("@str" == "DuplicateGL") {
            $(document).ready(function () {
                $("#hdnSavebtn").val("");
            })
            swal("", $("#DuplicateGLAccountNameFound").text(), "warning");
        }
        </script>
        //Model.Saveapprovebtn = "";
    }
}
@{
