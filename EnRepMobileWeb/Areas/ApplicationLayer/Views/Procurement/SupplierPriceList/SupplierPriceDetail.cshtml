                                    }
                                    @*<input class="custom-control-input col-md-3 col-sm-12 margin-switch" data-val="true" data-val-required="" id="ActiveStatus" name="ActiveStatus" type="checkbox" value="true"><input name="Cancelled" type="hidden" value="false">*@
                                    <label class="custom-control-label col-md-9 col-sm-12" for="ActiveStatus" style="padding: 3px 0px;"> @Resource.ActiveStatus  </label>
                                </div>
                            </div>
                        </div>
                        @*<div class="col-md-3 col-sm-12">
                                <label class="col-form-label label-align">@Resource.Item  @Resource.Name</label>
                                <select class="form-control" id="ItemName" name="Status">
