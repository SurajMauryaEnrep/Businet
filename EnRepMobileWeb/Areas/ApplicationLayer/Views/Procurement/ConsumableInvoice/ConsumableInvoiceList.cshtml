@model EnRepMobileWeb.MODELS.ApplicationLayer.Procurement.ConsumableInvoice.ConsumableInvoice_Model
@using EnRepMobileWeb.Resources;
@using System.Data
@if (Session["UserId"] == null)
{
    Response.Redirect("~/Login");
}
@{
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}
<script src="~/Content/Css/jquery/dist/jquery.min.js"></script>
<script src="~/Content/Css/build/js/sweetalert.js"></script>
@*/*start Add by Hina on 12-02-2024 to chk Financial year exist or not*/*@
@{
    if (TempData["Message"] != null)
    {
        if (TempData["Message"] == "Financial Year not Exist")
        {
            <script type="text/javascript">
                swal("", $("#FinancialYearDoesNotExistTransactionCanNotBeDone").text(), "warning");
            </script>
        }

    }
}
@*/*End to chk Financial year exist or not*/*@
<script src="~/Content/Scripts/ApplicationLayer/Procurement/ConsumableInvoice/ConsumableInvoice.js"></script>
<title>@Model.Title</title>
<div class="clearfix"></div>
<div class="row">
    <!-- Workflow Information -->
    <div id="WorkflowInformation" class="modal fade" role="dialog">
        <div class="modal-dialog" style="max-width:700px;">
            <div class="price_item_detail">
                <!-- Modal content-->
                @Html.Partial("~/Areas/Common/Views/_WorkFlow.cshtml")
            </div>
        </div>
    </div>
    <div class="col-md-12 col-sm-12 ">
        <div class="x_panel mar_pad1 ">
            <div class="x_content mar_pad1">
                <h2 class="col-md-5 col-sm-12 heading_left_padding ">@ViewBag.MenuPageName</h2>
                <!-- Smart Wizard -->
                <div id="wizard" class="form_wizard wizard_horizontal col-md-3 col-sm-12 work_flow">
                    @Html.Partial("~/Areas/Common/Views/_PartialWorkflowBar.cshtml")
                </div>
                @Html.HiddenFor(m => m.FilterData, new { @id = "FilterData" })
                @Html.HiddenFor(m => m.WF_status, new { @id = "WF_status" })
                <input type="hidden" id="DocumentMenuId" value="@ViewBag.DocumentMenuId" />
                <div id="" class="col-md-4 col-sm-12 heading_right_padding">
                    <ul>
                        @if (ViewBag.VBRoleList != null)
                        {
                            foreach (System.Data.DataRow Row in ViewBag.VBRoleList.Rows)
                            {
                                if (Row["feature_id"].ToString() == "105101152001")
                                {
                                    <li>
                                        <a class="tooltip-test"
                                           title="@Resource.AddNew" href="@Url.Action("AddConsumableInvoiceDetail", "ConsumableInvoice")"><img src="~/Content/Images/addgreen.png" width="32" id="Btn_AddNewPI" height="32" alt="Add"></a>
                                    </li>
                                }
                            }
                        }
                    </ul>
                </div>
                <div class="x_title pad_mar ">
                    <div class="col-md-12 col-sm-12 icon_tool padding_zero">
                        <div class="col-md-3 col-sm-12">
                            <label class="col-form-label label-align">@Resource.SupplierName</label>
                            @Html.DropDownListFor(model => model.SuppID, new SelectList(Model.SupplierNameList, "supp_id", "supp_name"), new { @class = "form-control", id = "CIListSupplier" })
                            @*<select class="form-control" id="ddlStatus" name="Status">
                                    <option value="0">All</option>
                                    <option value="A">Apex India Pvt. Ltd.</option>
                                    <option value="C">KBC FORGINGS PVT LTD</option>
                                    <option value="D">Mahesh Pvt. Ltd.</option>
                                    <option value="F">BALAJI COMPONENT</option>
                                </select>*@
                        </div>
                        <div class="col-md-2 col-sm-12 pl-0">
                            <label class="col-form-label label-align">@Resource.FromDate</label>
                            @Html.TextBoxFor(model => model.FromDate, new { @class = "form-control", @id = "txtFromdate", @onchange = "FromToDateValidation()", @type = "date", @max = DateTime.Now.ToString("yyyy-MM-dd") })
                            @*<input class="form-control" id="txtFromdate" max="2023-03-02" name="FromDate" onchange="FromToDateValidation()" type="date" value="2023-03-01">*@
                        </div>
                        <div class="col-md-2 col-sm-12 pl-0">
                            <label class="col-form-label label-align">@Resource.ToDate</label>
                            @if (Model.CI_ToDate != null)
                            {
                                @Html.TextBoxFor(model => model.ToDate, new { @class = "form-control", @id = "txtTodate", @placeholder = "dd-MM-yyyy", @onchange = "FromToDateValidation()", @type = "date", @max = DateTime.Now.ToString("yyyy-MM-dd"), @Value = Model.CI_ToDate })
                            }
                            else
                            {
                                @Html.TextBoxFor(model => model.ToDate, new { @class = "form-control", @id = "txtTodate", @placeholder = "dd-MM-yyyy", @onchange = "FromToDateValidation()", @type = "date", @max = DateTime.Now.ToString("yyyy-MM-dd"), @Value = @DateTime.Now.ToString("yyyy-MM-dd") })
                            }
                            @*<input value="2023-03-02" class="form-control" data-val="true" data-val-date="The field ToDate must be a date." data-val-required="The ToDate field is required." id="txtTodate" max="2023-03-02" name="ToDate" onchange="FromToDateValidation()" placeholder="dd-MM-yyyy" type="date">*@
                        </div>
                        <div class="col-md-2 col-sm-12 pl-0">
                            <label class="col-form-label label-align">@Resource.Status</label>
                            @Html.DropDownListFor(model => model.Status, new SelectList(Model.StatusList, "status_id", "status_name"), new { @class = "form-control", id = "ddlStatus" })
                            @*<select class="form-control" id="ddlStatus" name="Status">
                                    <option value="0">All</option>
                                    <option value="A">Approved</option>
                                    <option value="C">Cancelled</option>
                                    <option value="D">Drafted</option>
                                    <option value="F">Forwarded</option>
                                </select>*@
                        </div>
                        <div class="col-md-1 col-sm-12 pl-0">
                            <button type="button" class="btn btn-primary btn-sm margintop20" onclick="BtnSearch();"><i class="fa fa-search" aria-hidden="true"></i></button>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12 col-sm-12">
        <div class="x_panel margin_top_zero">
            <div class="x_content">
                <div class="card-box table-responsive dy_table">
                    <div id="tbodyPIList" class="sticky-table sticky-ltr-cells sb-container container-example1">
                        @Html.Partial("~/Areas/ApplicationLayer/Views/Shared/PartialConsumableInvoiceList.cshtml")
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- page content end -->
