    @* show loader script when click on button , form submit .....etc*@
    <script>
        $(document).ready(function () {
            //cookieStore.delete("fileDownLoad");
            document.cookie = "fileDownLoad=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
            $('form').submit(function (e) {

                //Loader Start
                showLoader();
                document.cookie = "fileDownLoad=true; path=/";//Cookie Creted for Submit Request
                //check loading
                var checkInterval = setInterval(function () {
                    if (!document.cookie.includes("fileDownLoad=true")) {
                        clearInterval(checkInterval);//Clearing checkInterval function
                        hideLoader();
                    }
                }, 1000);
            });

            $('.fa-search').click(function () {
                // Display loader before postback (on button click)
                showLoader();
                var checkInterval = setInterval(function () {
                    if ($.active == 0) {
                        clearInterval(checkInterval);//Clearing checkInterval function
                        hideLoader();
                    }
                }, 1000);
            });
            // Handler for window load event (hide loader after postback)
            $(document).ajaxStart(function () {

                if (sessionStorage.ShowLoader == "N") {
                    sessionStorage.removeItem("ShowLoader");
                } else {
                    showLoader();
                }
                // Hide loader when all AJAX requests are complete
                //hideLoader();
            });
            $(window).on('load', function () {//this will triggered when all static files Loaded.
                /*$("#loaderContainer").fadeOut(500);*/
                //showLoader();
                if ($.active > 0) {
                    console.log("An AJAX request is processing.");
                } else {
                    hideLoader();
                }

            });

            $(document).on('renderComplete', function () {
                hideLoader();
            });
            $(document).ajaxStop(function () {
                // Hide loader when all AJAX requests are complete
                hideLoader();
            });
            //$(document).ajaxComplete(function () {
            //    // Hide loader when all AJAX requests are complete
            //    hideLoader();
            //});
            $(document).ajaxError(function (event, xhr, settings, thrownError) {
                // Hide the loading spinner when any AJAX request fails
                hideLoader();
            });


        });
        function showLoader() {
            $('#loader').show();
        }
        function hideLoader() {
            $('#loader').hide();
        }
