<title>@Model.Title</title>
@{
    var Action = "";
    var Disable = true;
    //if (Session["Action"] != null)
    if (Model.Action != null)
    {
        //Action = Session["Action"].ToString();
        Action = Model.Action;
    }
    if (Action == "AddNew")
    {
        Disable = false;
    }
    //else
    //{
    //    if (Model.status != null)
    //    {
    //        Disable = true;
    //    }
    //}


    var str = ViewBag.Message;
    if (str == "Save")
    {
        <script type="text/javascript">
            $("#hdnSavebtn").val(null);
            swal("", $("#savemsg").text(), "success");
        </script>
    }
    if (str == "Delete")
    {
        <script type="text/javascript">
            $("#hdnSavebtn").val(null);
            swal("", $("#deletemsg").text(), "success");
        </script>
    }
    if (str == "Used")
    {
        <script type="text/javascript">
            $("#hdnSavebtn").val(null);
            swal("", $("#DependencyExist").text(), "warning");
        </script>
    }
    if (str == "Approved")
    {
        <script type="text/javascript">
            $("#hdnSavebtn").val(null);
            swal("", $("#approvemsg").text(), "success");
        </script>
    }
    if (str == "Duplicate")
    {
        <script type="text/javascript">
            $(document).ready(function () {
                $("#hdnSavebtn").val(null);
                $("#HSNNumber").css("border-color", "red");
                document.getElementById("vmHSNNumber").innerHTML = $("#valueduplicate").text();

            })
        </script>
    }
    str = null;
}
