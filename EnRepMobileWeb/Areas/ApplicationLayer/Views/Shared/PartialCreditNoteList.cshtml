@model EnRepMobileWeb.MODELS.ApplicationLayer.FinancialAccountingAndControl.GeneralLedgerVoucher.CreditNote.CreditNoteList_Model
@using EnRepMobileWeb.Resources;
@{
    var rowId = 0;
}
<script src="~/Content/Scripts/Common/CommonPartialJS.js"></script>
<script src="~/Content/Scripts/ApplicationLayer/FinancialAccountingAndControl/GeneralLedgerVoucher/CreditNote/CreditNoteList.js"></script>
<script src="~/Content/Scripts/Common/CommonPartialJS.js"></script>
<table id="datatable-buttons" class="table table-striped rowhighlighttbl width2500 itmfrz" cellspacing="0" width="100%">
    <thead>
        <tr class="sticky-header">
            <th width="3%"> @Resource.SrNo</th>
            <th width="8%" class="itmStick">@Resource.GLVoucher <br /> @Resource.Number</th>
            <th width="6%">@Resource.GLVoucher <br />  @Resource.Date</th>
            <th width="8%" hidden>@Resource.GLVoucherDate</th>
            <th width="6%">@Resource.Entity <br />  @Resource.Type</th>
            <th width="6%">@Resource.Entity <br />  @Resource.Name</th>
            <th width="5%">@Resource.Currency</th>
            <th width="6%">@Resource.Amount <br />   @Resource.InSpecific</th>
            <th width="6%">@Resource.Amount <br />   @Resource.InBase</th>
            <th width="7%">@Resource.BillNumber</th>
            <th width="6%">@Resource.BillDate</th>
            <th width="6%">@Resource.Status</th>
            <th width="6%">@Resource.CreatedBy</th>
            <th width="6%">@Resource.Createdon</th>
            <th width="5%">@Resource.ApprovedBy</th>
            <th width="6%">@Resource.ApprovedOn</th>
            <th width="6%">@Resource.AmendedBy</th>
            <th width="6%">@Resource.AmendedOn</th>
        </tr>
    </thead>
    <tbody id="">
        @if (Model.VoucherList != null)
        {
            foreach (var row in Model.VoucherList)
            {
                rowId++;
        <tr>
            <td>@rowId</td>
            @*<td id="VouNumber">@row.VouNumber</td>*@
            <td id="VouNumber" class="ItmNameBreak itmStick tditemfrz"><div class="col-sm-10 no-padding" id="Cn_Vouno" >@row.VouNumber</div>
            <div class="col-sm-2 i_Icon"><button type="button" id="VoucherDetail" class="calculator" onclick="OnClickIconBtn(event);" data-toggle="modal" data-target="#VoucherDetailMW" data-backdrop="static" data-keyboard="false"><img src="/Content/Images/iIcon1.png" alt="" title="@Resource.VoucherDetail"> </button></div>
            </td>

            <td id="Cn_Date">@row.VouDate</td>
            <td hidden id="hdVouDate">@row.hdVouDate</td>
            <td>@row.entity_type</td>
            <td>@row.entity_name</td>
            <td class="center">@row.curr_logo</td>
            <td class="num_right">@row.Amountsp</td>
            <td class="num_right">@row.Amountbs</td>
            <td>@row.BillNo</td>
            <td>@row.BillDt</td>
            <td id="Doc_Status">@row.VouStatus</td>
            <td>@row.create_by</td>
            <td>@row.CreatedON</td>
            <td>@row.app_by</td>
            <td>@row.ApprovedOn</td>
            <td>@row.mod_by</td>
            <td>@row.ModifiedOn</td>
        </tr>
            }
        }
    </tbody>
</table>
@*@if (Session["VouSearch"] != null)*@
@if (Model.VouSearch != null)
{
    //if (Session["VouSearch"].ToString() == "Vou_Search")
    if (Model.VouSearch == "Vou_Search")
    {
        @Scripts.Render("~/Content/Scripts/DataTableRefreshJs")
        @Scripts.Render("~/Content/Scripts/DatatablesJs")
        //Session["VouSearch"] = null;
        Model.VouSearch = null;
    }
}





