@{
    var DisableD = true;
    var DisableH = true;
    var DisableAmend = true;
    var ForDisableOCDlt = "Enable";
    var transtype = "";
    var display = "none";
    var disablePrintbtn = "";
    var FCloseAccess = "N";

    if (Model.TransType == "Update")
    {
        var v = Model.OrdStatus;
        if (Model.Command == "Edit")
        {
            if (Model.OrdStatus == "D")
            {
                DisableD = false;
            }
            else
            {
                DisableD = true;
            }

            //DisableH = false;
        }
        else
        {
            DisableD = true;
            DisableH = true;
        }
        if (Model.Amend == "Amend")
        {
            DisableAmend = false;
            display = "block";
        }
    }
    else
    {
        if (Model.Command == "Refresh")
        {
            DisableD = true;
            DisableH = true;
        }
        else
        {
            DisableD = false;
            DisableH = false;
        }
    }
    if ((Model.DocumentStatus != "D") || DisableH)
    {
    }
    else
    {
        if (Model.SPO_No == null)
        {
            DisableH = false;
        }
        else
        {
            DisableH = true;
            transtype = "Update";
        }
    }
    if (DisableD)
    {
        ViewBag.Disable = true;
        ForDisableOCDlt = "Disable";
    }
    else
    {
        ViewBag.Disable = false;
        ForDisableOCDlt = "Enable";
        disablePrintbtn = "disabled";
    }
    <script>
         sessionStorage.setItem("PQTransType", "@transtype");
    </script>
    if (Model.SPO_No == null || ((Model.OrdStatus == "D" || Model.OrdStatus == "A") && Model.Command == "Edit"))/*Add || ((Model.StatusCode == "D" || Model.StatusCode == "A") && Model.Command == "Edit") by Hina sharma on 11-10-2024*/
    {
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
    if (Model.Src_Type == null)
    {
        Model.Src_Type = "D";
    }
}
@{
    if (Model.Message != null)
    {
        var message = Model.Message;
        Model.hdnsaveApprovebtn = null;
        if (message == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
            </script>
        }
        if (message == "Deleted")
        {
            <script type="text/javascript">
                swal("", $("#deletemsg").text(), "success");
            </script>
        }
        if (message == "Approved")
        {
            <script type="text/javascript">
                swal("", $("#approvemsg").text(), "success");
            </script>
        }
        if (Model.Message == "Approved_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    swal("", $("#approvemsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        if (message == "Cancelled")
        {
            <script type="text/javascript">
                swal("", $("#cancelmsg").text(), "success");
            </script>
        }
        if (Model.Message == "Cancelled_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    swal("", $("#cancelmsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        if (message == "Used")
        {
            <script type="text/javascript">
                swal("", $("#PurchaseOrderHasBnPrcsdCanNotBeMdfd").text(), "warning");
            </script>
        }
        //Added by Nidhi on 10-09-2025
        if (message == "ErrorInMail")
        {
            <script type="text/javascript">
                swal("", $("#Emailsendingfailedduetotechnicalissue").text(), "warning");
            </script>
        }
        //Session.Remove("Message");
    }
}
