@using EnRepMobileWeb.Resources;
<script src="~/Content/Scripts/Common/CommonPartialJS.js"></script>
<script src="~/Content/Scripts/BusinessLayer/GLSetup/GLList.js"></script>
@*//*** All Session Removed by shubham maurya on 21-12-2022 16:05***//*@
<table id="datatable-buttons" class="table table-striped table-bordered width1500 rowhighlighttbl" style="width:100%">
    <thead>
        <tr class="sticky-header">
            <th width="3%">@Resource.SrNo</th>
            <th>@Resource.AccountID</th>
            <th>@Resource.AccountName</th>
            <th>@Resource.AccountGroup</th>
            <th hidden="hidden"></th>
            <th>@Resource.AccountType</th>
            <th hidden="hidden"></th>
            <th>@Resource.Currency</th>
            <th>@Resource.ActiveStatus</th>
            <th hidden="hidden"></th>
            <th hidden="hidden"></th>
            <th hidden="hidden"></th>
            <th hidden="hidden"></th>
            <th hidden="hidden"></th>
            <th hidden="hidden"></th>
            <th hidden="hidden"></th>
            <th>@Resource.CreatedBy</th>
            <th>@Resource.Createdon</th>
            <th>@Resource.AmendedBy</th>
            <th>@Resource.AmendedOn</th>
        </tr>
    </thead>
    <tbody>
        @if (ViewBag.VBGLList != null)
        {
            foreach (System.Data.DataRow item in ViewBag.VBGLList.Rows)
            {
        <tr>
            <td>@item["SNo"].ToString()</td>
            <td id="accid">@item["acc_id"].ToString() </td>
            <td>@item["acc_name"].ToString() </td>
            <td>@item["acc_group_name"].ToString()</td>
            <td hidden="hidden" id="acc_grp_id">@item["acc_grp_id"]</td>
            <td>@item["AccType"].ToString()</td>
            <td hidden="hidden" id="acc_type">@item["acc_type"].ToString()</td>
            <td>@item["curr_name"].ToString()</td>
            @if (@item["act_status_tr"].ToString() == "N")
            {
                <td class="center"><i class="fa fa-times-circle text-danger" aria-hidden="true"></i></td>
            }
            else
            {
                <td class="center"><i class="fa fa-check text-success " aria-hidden="true"></i></td>
            }
            <td hidden="hidden" id="roa">@item["roa"].ToString()</td>
            <td hidden="hidden" id="plr">@item["plr"].ToString()</td>
            <td hidden="hidden" id="ibt">@item["ibt"].ToString()</td>
            <td hidden="hidden" id="iwt">@item["iwt"].ToString()</td>
            <td hidden="hidden" id="egl">@item["egl"].ToString()</td>
            <td hidden="hidden" id="sta">@item["sta"].ToString()</td>
            <td hidden="hidden" id="act_status_tr">@item["act_status_tr"].ToString()</td>
            <td>@item["create_by"].ToString()</td>
            <td>@item["create_dt"].ToString()</td>
            <td>@item["mod_by"].ToString()</td>
            <td>@item["mod_dt"].ToString()</td>
        </tr>
            }
        }
    </tbody>
</table>
@if (Model.GLSearch != null)
{
    //if (Session["GLSearch"].ToString() == "GL_Search")
    if (Model.GLSearch == "GL_Search")
    {
        @Scripts.Render("~/Content/Scripts/DataTableRefreshJs")
        @Scripts.Render("~/Content/Scripts/DatatablesJs")
    }
}