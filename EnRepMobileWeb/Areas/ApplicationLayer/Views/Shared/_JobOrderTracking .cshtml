@using EnRepMobileWeb.Resources;
<div class="modal-content">
    <div class="modal-header">
        <h2>@Resource.JobOrder @Resource.Tracking</h2> <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>
    <div class="modal-body pop_top">
        @*<form id="JobOrderTracking" data-parsley-validate class="form-horizontal form-label-left">*@
            <div class="row">
                <div class="col-md-55">
                    <label class="col-form-label label-align" for="">@Resource.JobOrder @Resource.Number</label>
                    @*<input type="text" id="" class="form-control" placeholder="@Resource.JobOrder @Resource.Number" readonly>*@
                    @if (ViewBag.JobOrdNo != null && ViewBag.JobOrdNo != "")
                    {
                        <input type="text" id="LstJoTrckJONumber" class="form-control" value="@ViewBag.JobOrdNo" placeholder="@Resource.JobOrder @Resource.Number" readonly>
                    }
                    else
                    {
                        <input type="text" id="LstJoTrckJONumber" class="form-control" placeholder="@Resource.JobOrder @Resource.Number" readonly>
                    }
                </div>
                <div class="col-md-55">
                    <label class="col-form-label label-align" for="">@Resource.JobOrder @Resource.Date</label>
                    @*<input type="date" id="OrderDate" class="form-control" placeholder="@Resource.JobOrder @Resource.Date" readonly>*@
                    @if (ViewBag.JO_date != null && ViewBag.JO_date != "")
                    {
                        <input type="date" id="LstJoTrckJODate" value="@ViewBag.JO_date" class="form-control" readonly>
                    }
                    else
                    {
                        <input type="date" id="LstJoTrckJODate" class="form-control" readonly>
                    }
                </div>
               
                <div class="col-md-55">
                    <label class="col-form-label label-align" for="">@Resource.ContractorName</label>
                    @*<input type="text" id="" class="form-control" placeholder="@Resource.ContractorName" readonly>*@
                    @if (ViewBag.SuppName != null && ViewBag.SuppName != "")
                    {
                        <input type="text" id="LstJoTrckSupplierName" class="form-control" value="@ViewBag.SuppName" placeholder="@Resource.ContractorName" readonly>
                    }
                    else
                    {
                        <input type="text" id="LstJoTrckSupplierName" class="form-control" placeholder="@Resource.ContractorName" readonly>
                    }
                </div>
                <div class="col-md-55">
                    <label class="col-form-label label-align" for="">@Resource.Finished @Resource.Product</label>
                    @if (ViewBag.FnshdPrdtName != null && ViewBag.FnshdPrdtName != "")
                    {
                        <input type="text" id="LstJoTrckFnshdPrdctName" class="form-control" value="@ViewBag.FnshdPrdtName" placeholder="@Resource.Finished @Resource.Product" readonly>
                    }
                    else
                    {
                        <input type="text" id="LstJoTrckFnshdPrdctName" class="form-control" placeholder="@Resource.Finished @Resource.Product" readonly>
                    }
                    @*<input type="text" id="" class="form-control" placeholder="@Resource.Finished @Resource.Product" readonly>*@
                </div>
                <div class="col-md-55">
                    <label class="col-form-label label-align" for="">@Resource.UOM</label>
                    @*<input type="text" id="" class="form-control" placeholder="@Resource.UOM" readonly>*@
                    @if (ViewBag.FnshdPrdctUom != null && ViewBag.FnshdPrdctUom != "")
                    {
                        <input type="text" id="LstJoTrckFnshdPrdctUom" class="form-control" value="@ViewBag.FnshdPrdctUom" placeholder="@Resource.UOM" readonly>
                    }
                    else
                    {
                        <input type="text" id="LstJoTrckFnshdPrdctUom" class="form-control" placeholder="@Resource.UOM" readonly>
                    }
                </div>
                <div class="col-md-55">
                    <label class="col-form-label label-align" for="">@Resource.OperationName</label>
                    @*<input type="text" id="" class="form-control" placeholder="@Resource.OperationName" readonly>*@
                    @if (ViewBag.OperationName != null && ViewBag.OperationName != "")
                    {
                        <input type="text" id="LstJoTrckOPName" class="form-control" value="@ViewBag.OperationName" placeholder="@Resource.OperationName" readonly>
                    }
                    else
                    {
                        <input type="text" id="LstJoTrckOPName" class="form-control" placeholder="@Resource.OperationName" readonly>
                    }
                </div>
                <div class="col-md-55">
                    <label class="col-form-label label-align" for="">@Resource.Operation @Resource.Output @Resource.Product</label>
                    @*<input type="text" id="" class="form-control" placeholder="@Resource.Operation @Resource.Output @Resource.Product" readonly>*@
                    @if (ViewBag.OpOutPrdtName != null && ViewBag.OpOutPrdtName != "")
                    {
                        <input type="text" id="LstJoTrckOPOutPrdctName" class="form-control" value="@ViewBag.OpOutPrdtName" placeholder="@Resource.Operation @Resource.Output @Resource.Product" readonly>
                    }
                    else
                    {
                        <input type="text" id="LstJoTrckOPOutPrdctName" class="form-control" placeholder="@Resource.Operation @Resource.Output @Resource.Product" readonly>
                    }
                </div>
                <div class="col-md-55">
                    <label class="col-form-label label-align" for="">@Resource.UOM</label>
                    @*<input type="text" id="" class="form-control" placeholder="@Resource.UOM" readonly>*@
                    @if (ViewBag.OpOutPrdtUom != null && ViewBag.OpOutPrdtUom != "")
                    {
                        <input type="text" id="LstJoTrckOPOutPrdctUom" class="form-control" value="@ViewBag.OpOutPrdtUom" placeholder="@Resource.UOM" readonly>
                    }
                    else
                    {
                        <input type="text" id="LstJoTrckOPOutPrdctUom" class="form-control" placeholder="@Resource.UOM" readonly>
                    }
                </div>
                <div class="col-md-55">
                    <label class="col-form-label label-align" for="">@Resource.Quantity</label>
                    @*<input type="text" id="" class="form-control num_right" placeholder="0000.00" readonly>*@
                    @if (ViewBag.JOQuantity != null && ViewBag.JOQuantity != "")
                    {
                        <input type="text" id="LstJoTrckJOQty" class="form-control" value="@ViewBag.JOQuantity" placeholder="0000.00" readonly>
                    }
                    else
                    {
                        <input type="text" id="LstJoTrckJOQty" class="form-control" placeholder="0000.00" readonly>
                    }
                </div>
            </div>
        @*</form>*@
        <div class="card-box table-responsive dy_table margin10">
            <div class="sticky-table sticky-ltr-cells sb-container container-example1 pop_csv">
                <table id="Table_JOTracking" class="table table-striped table-bordered width2500 margin5 rowhighlighttbl" style="width:100%;">
                    <thead>
                        <tr class="sticky-header">
                            <th width="2%">@Resource.SrNo</th>
                            <th width="6%" class="th_green">@Resource.Dispatch <br /> @Resource.Number</th>
                            <th width="5%" class="th_green">@Resource.Dispatch <br /> @Resource.Date</th>
                            <th width="5%" class="th_green">@Resource.Dispatch <br />  @Resource.Quantity</th>
                            <th width="5%" class="th_blue">@Resource.DeliveryNote <br />  @Resource.Number</th>
                            <th width="5%" class="th_blue">@Resource.DeliveryNote <br />  @Resource.Date</th>
                            <th width="5%" class="th_blue">@Resource.Received <br /> @Resource.Quantity</th>
                            <th width="5%" class="th_orange">@Resource.QC <br /> @Resource.Number</th>
                            <th width="5%" class="th_orange">@Resource.QC <br /> @Resource.Date</th>
                            <th width="5%" class="th_orange">@Resource.Accepted <br />  @Resource.Quantity</th>
                            <th width="5%" class="th_orange">@Resource.Rejected <br />  @Resource.Quantity</th>
                            <th width="5%" class="th_orange">@Resource.Reworkable <br />  @Resource.Quantity</th>
                            <th width="5%" class="th_green1">@Resource.GRN <br /> @Resource.Number</th>
                            <th width="4%" class="th_green1">@Resource.GRN <br /> @Resource.Date</th>
                            <th width="5%" class="th_green1">@Resource.GRN <br />  @Resource.Quantity</th>
                            <th width="5%" class="th_yellow">@Resource.Invoice <br /> @Resource.Number</th>
                            <th width="4%" class="th_yellow">@Resource.Invoice <br /> @Resource.Date</th>
                            <th width="5%" class="th_yellow">@Resource.Invoice <br />  @Resource.Quantity</th>
                        </tr>
                    </thead>
                    <tbody>
                    
                        @if (ViewBag.JOTrackingList != null)
                        {

                            var rowIdx = 0;
                            foreach (System.Data.DataRow Row in ViewBag.JOTrackingList.Rows)
                            {

                                rowIdx++;
                                <tr>
                                    <td>@rowIdx</td>
                                    <td class="th_green">@Row["dispatch_no"].ToString()</td>
                                    <td class="th_green">@Row["dispatch_dt"].ToString()</td>
                                    <td class="th_green" align="right">@Row["disp_qty"].ToString()</td>
                                    <td class="th_blue">@Row["dn_no"].ToString()</td>
                                    <td class="th_blue">@Row["dn_dt"].ToString()</td>
                                    <td class="th_blue num_right">@Row["recd_qty"].ToString()</td>
                                    <td class="th_orange">@Row["qc_no"].ToString()</td>
                                    <td class="th_orange">@Row["qc_dt"].ToString()</td>
                                    <td class="th_orange num_right">@Row["accept_qty"].ToString()</td>
                                    <td class="th_orange num_right">@Row["reject_qty"].ToString()</td>
                                    <td class="th_orange num_right">@Row["rework_qty"].ToString()</td>
                                    <td class="th_green1">@Row["mr_no"].ToString()</td>
                                    <td class="th_green1">@Row["mr_dt"].ToString()</td>
                                    <td class="th_green1 num_right">@Row["rec_qty"].ToString()</td>
                                    <td class="th_yellow">@Row["inv_no"].ToString()</td>
                                    <td class="th_yellow">@Row["inv_dt"].ToString()</td>
                                    <td class="th_yellow num_right">@Row["inv_qty"].ToString()</td>
                                    
                                </tr>
                            }
                        }
                    
