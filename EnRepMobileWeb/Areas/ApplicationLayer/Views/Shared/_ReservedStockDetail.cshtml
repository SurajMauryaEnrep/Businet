                                <th width="11%">@Resource.Serial1</th>
                                <th width="11%" id="thavlqty">@Resource.Available <br /> @Resource.Quantity</th>
                                <th width="11%" id="threvqty">@Resource.Reserved <br /> @Resource.Quantity</th>
