@using EnRepMobileWeb.Resources;
@{
    var rowId = 0;
    string tableWidth = "width3000";
    if (ViewBag.DocumentMenuId == "105101136")
    {
        tableWidth = "width1400";
    }
    else
    {
        tableWidth = "width3200";
    }
}
<script src="~/Content/Scripts/Common/CommonPartialJS.js"></script>
<div class="modal-content">
    <div class="modal-header">
        <h2>@Resource.Order @Resource.Tracking </h2> <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>
    <div class="modal-body pop_top">
        @*<form id="PurchaseOrderTracking" data-parsley-validate class="form-horizontal form-label-left">*@
        <div class="row">
            <div class="col-md-3 col-sm-12 available_right ">
                <label class="col-form-label label-align" for="">@Resource.PONumber</label>
                @if (ViewBag.po_no != null && ViewBag.po_no != "")
                {
                    <input type="text" id="PurchaseOrderNumber" class="form-control" value="@ViewBag.po_no" placeholder="@Resource.PONumber" readonly>
                }
                else
                {
                    <input type="text" id="PurchaseOrderNumber" class="form-control" placeholder="@Resource.PONumber" readonly>
                }
            </div>
            <div class="col-md-3 col-sm-12 available_right ">
                <label class="col-form-label label-align" for="">@Resource.OrderDate</label>
                @if (ViewBag.PO_date != null && ViewBag.PO_date != "")
                {
                    <input type="date" id="OrderDate" value="@ViewBag.PO_date" class="form-control" readonly>
                }
                else
                {
                    <input type="date" id="OrderDate" class="form-control" readonly>
                }
            </div>
            <div class="col-md-3 col-sm-12 available_right ">
                <label class="col-form-label label-align" for="">@Resource.SupplierName</label>
                @if (ViewBag.suppName != null && ViewBag.suppName != "")
                {
                    <input type="text" id="SupplierName" class="form-control" value="@ViewBag.suppName" placeholder="@Resource.SupplierName" readonly>
                }
                else
                {
                    <input type="text" id="SupplierName" class="form-control" placeholder="@Resource.SupplierName" readonly>
                }
            </div>
        </div>
        @*</form>*@
        <div class="card-box table-responsive dy_table margin10">
            <div class="sticky-table sticky-ltr-cells sb-container container-example1 pop_csv">
                <table id="Table_POTracking" class="table table-striped table-bordered @tableWidth margin5 rowhighlighttbl" style="width:100%;">
                    <thead>
                        @if (ViewBag.DocumentMenuId == "105101136")
                        {
                        <tr class="sticky-header">
                            <th width="2%">&nbsp;</th>
                            <th width="28%">&nbsp;</th>
                            <th width="10%">&nbsp;</th>
                            <th width="10%">&nbsp;</th>
                            <th width="10%">&nbsp;</th>
                            <th width="10%">&nbsp;</th>
                            <th width="10%" class="light_cream1_colour">&nbsp;</th>
                            <th width="10%" class="light_cream1_colour">@Resource.InvoiceDetail</th>
                            <th width="10%" class="light_cream1_colour">&nbsp;</th>
                            <th width="4%" class="light_blue_colour">&nbsp;</th>
                            <th width="4%" class="light_blue_colour">@Resource.ReturnDetail</th>
                            <th width="4%" class="light_blue_colour">&nbsp;</th>
                        </tr>
                        <tr class="sticky-header">
                            <th width="2%">@Resource.SrNo</th>
                            <th width="28%">@Resource.ItemName</th>
                            <th width="10%">@Resource.UOM</th>
                            <th width="10%">@Resource.Item @Resource.Type</th>
                            <th width="10%">@Resource.Order <br />  @Resource.Quantity</th>
                            <th width="10%">@Resource.Pending <br />  @Resource.Quantity</th>
                            <th width="10%" class="light_cream1_colour">@Resource.Invoice <br />  @Resource.Number</th>
                            <th width="10%" class="light_cream1_colour">@Resource.Invoice <br />  @Resource.Date</th>
                            <th width="10%" class="light_cream1_colour">@Resource.Invoice <br />  @Resource.Quantity</th>
                            <th width="10%" class="light_blue_colour">@Resource.Return <br />  @Resource.Number</th>
                            <th width="10%" class="light_blue_colour">@Resource.Return <br />  @Resource.Date</th>
                            <th width="10%" class="light_blue_colour">@Resource.Return <br />  @Resource.Quantity</th>
                        </tr>
                        }
                        else
                        {
                    <tr class="sticky-header">
                        <th width="2%">&nbsp;</th>
                        <th width="13%">&nbsp;</th>
                        <th width="3%">&nbsp;</th>
                        <th width="5%">&nbsp;</th>
                        <th width="5%">&nbsp;</th>
                        <th width="5%">&nbsp;</th>
                        <th width="5%" class="light_cream_colour">&nbsp;</th>
                        <th width="5%" class="light_cream_colour">@Resource.DeliveryNoteDetail</th>
                        <th width="5%" class="light_cream_colour">&nbsp;</th>
                        <th width="5%" class="light_blue_colour">&nbsp;</th>
                        <th width="4%" class="light_blue_colour">&nbsp;</th>
                        <th width="4%" class="light_blue_colour">&nbsp;</th>
                        <th width="4%" class="light_blue_colour">@Resource.QCDetail</th>
                        <th width="4%" class="light_blue_colour">&nbsp;</th>
                        <th width="4%" class="light_blue_colour">&nbsp;</th>
                        <th width="4%" class="light_blue_colour">&nbsp;</th>
                        <th width="4%" class="light_blue1_colour">&nbsp;</th>
                        <th width="4%" class="light_blue1_colour">@Resource.GoodReceiptDetail</th>
                        <th width="4%" class="light_blue1_colour">&nbsp;</th>
                        <th width="4%" class="light_cream1_colour">&nbsp;</th>
                        <th width="4%" class="light_cream1_colour">@Resource.InvoiceDetail</th>
                        <th width="4%" class="light_cream1_colour">&nbsp;</th>
                        <th width="4%" class="light_blue_colour">&nbsp;</th>
                        <th width="4%" class="light_blue_colour">@Resource.ReturnDetail</th>
                        <th width="4%" class="light_blue_colour">&nbsp;</th>
                    </tr>
                    <tr class="sticky-header">
                        <th width="2%">@Resource.SrNo</th>
                        <th width="13%">@Resource.ItemName</th>
                        <th width="3%">@Resource.UOM</th>
                        <th width="5%">@Resource.Item @Resource.Type</th>
                        <th width="5%">@Resource.Order <br />  @Resource.Quantity</th>
                        <th width="5%">@Resource.Pending <br />  @Resource.Quantity</th>
                        <th width="5%" class="light_cream_colour">@Resource.DeliveryNote <br /> @Resource.Number</th>
                        <th width="5%" class="light_cream_colour">@Resource.Delivery <br />  @Resource.Date</th>
                        <th width="5%" class="light_cream_colour">@Resource.Delivery <br />  @Resource.Quantity</th>
                        <th width="5%" class="light_blue_colour">@Resource.QC <br />  @Resource.Number</th>
                        <th width="4%" class="light_blue_colour">@Resource.QCDate</th>
                        <th width="4%" class="light_blue_colour">@Resource.Accepted <br />  @Resource.Quantity</th>
                        <th width="4%" class="light_blue_colour">@Resource.Rejected <br />  @Resource.Quantity</th>
                        <th width="4%" class="light_blue_colour">@Resource.Reworkable <br />  @Resource.Quantity</th>
                        <th width="4%" class="light_blue_colour">@Resource.Short <br />  @Resource.Quantity</th>
                        <th width="4%" class="light_blue_colour">@Resource.Sample <br />  @Resource.Quantity</th>
                        <th width="4%" class="light_blue1_colour">@Resource.Receipt <br />  @Resource.Number</th>
                        <th width="4%" class="light_blue1_colour">@Resource.Receipt <br />  @Resource.Date</th>
                        <th width="4%" class="light_blue1_colour">@Resource.Receipt <br />  @Resource.Quantity</th>
                        <th width="4%" class="light_cream1_colour">@Resource.Invoice <br />  @Resource.Number</th>
                        <th width="4%" class="light_cream1_colour">@Resource.Invoice <br />  @Resource.Date</th>
                        <th width="4%" class="light_cream1_colour">@Resource.Invoice <br />  @Resource.Quantity</th>
                        <th width="4%" class="light_blue_colour">@Resource.Return <br />  @Resource.Number</th>
                        <th width="4%" class="light_blue_colour">@Resource.Return <br />  @Resource.Date</th>
                        <th width="4%" class="light_blue_colour">@Resource.Return <br />  @Resource.Quantity</th>
                    </tr>
                        }
                    </thead>
                    <tbody id="">
                        @if (ViewBag.POTrackingList != null)
                        {

                            var rowIdx = 0;
                            foreach (System.Data.DataRow Row in ViewBag.POTrackingList.Rows)
                            {

                                rowIdx++;
                                if (ViewBag.DocumentMenuId == "105101136")
                                {
                        <tr>
                            <td>@rowIdx</td>
                            <td>@Row["item_name"].ToString()</td>
                            <td>@Row["uom_name"].ToString()</td>
                            <td>@Row["item_type"].ToString()</td>
                            <td align="right">@Row["ord_qty_base"].ToString()</td>
                            <td align="right">@Row["pending_qty"].ToString()</td>
                            <td class="light_cream1_colour">@Row["inv_no"].ToString()</td>
                            <td class="light_cream1_colour">@Row["inv_dt"].ToString()</td>
                            <td class="light_cream1_colour num_right">@Row["inv_qty"].ToString()</td>
                            <td class="light_blue_colour">@Row["prt_no"].ToString()</td>
                            <td class="light_blue_colour">@Row["prt_dt"].ToString()</td>
                            <td class="light_blue_colour num_right">@Row["PrtQty"].ToString()</td>
                        </tr>
                                }
                                else
                                {
                    <tr>
                        <td>@rowIdx</td>
                        <td>@Row["item_name"].ToString()</td>
                        <td>@Row["uom_name"].ToString()</td>
                        <td>@Row["item_type"].ToString()</td>
                        <td align="right">@Row["ord_qty_base"].ToString()</td>
                        <td align="right">@Row["pending_qty"].ToString()</td>
                        <td class="light_cream_colour">@Row["dn_no"].ToString()</td>
                        <td class="light_cream_colour">@Row["dn_dt"].ToString()</td>
                        <td class="light_cream_colour num_right">@Row["recd_qty"].ToString()</td>
                        <td class="light_blue_colour">@Row["qc_no"].ToString()</td>
                        <td class="light_blue_colour">@Row["qc_dt"].ToString()</td>
                        <td class="light_blue_colour num_right">@Row["accept_qty"].ToString()</td>
                        <td class="light_blue_colour num_right">@Row["reject_qty"].ToString()</td>
                        <td class="light_blue_colour num_right">@Row["rework_qty"].ToString()</td>
                        <td class="light_blue_colour num_right">@Row["short_qty"].ToString()</td>
                        <td class="light_blue_colour num_right">@Row["sample_qty"].ToString()</td>
                        <td class="light_blue1_colour">@Row["mr_no"].ToString()</td>
                        <td class="light_blue1_colour">@Row["mr_dt"].ToString()</td>
                        <td class="light_blue1_colour num_right">@Row["rec_qty"].ToString()</td>
                        <td class="light_cream1_colour">@Row["inv_no"].ToString()</td>
                        <td class="light_cream1_colour">@Row["inv_dt"].ToString()</td>
                        <td class="light_cream1_colour num_right">@Row["inv_qty"].ToString()</td>
                        <td class="light_blue_colour">@Row["prt_no"].ToString()</td>
                        <td class="light_blue_colour">@Row["prt_dt"].ToString()</td>
                        <td class="light_blue_colour num_right">@Row["PrtQty"].ToString()</td>
                    </tr>
                                }
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
