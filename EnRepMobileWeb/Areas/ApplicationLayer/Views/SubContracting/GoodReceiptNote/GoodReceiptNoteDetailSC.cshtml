@{
    if (Model.Message != null)
    {
        var message = Model.Message;
        Model.hdnsaveApprovebtn = null;
        if (Model.Message == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
            </script>
        }
        if (Model.Message == "Deleted")
        {
            <script type="text/javascript">
                swal("", $("#deletemsg").text(), "success");
            </script>
        }
        if (Model.Message == "Approved")
        {
            <script type="text/javascript">
                swal("", $("#approvemsg").text(), "success");
            </script>
        }
        if (Model.Message == "Cancelled")
        {
            <script type="text/javascript">
                swal("", $("#cancelmsg").text(), "success");
            </script>
        }
        if (Model.Message == "Used")
        {
            <script type="text/javascript">
                swal("", $("#MaterialHasBeenDispatchedDocumentCanNotBeCancelled").text(), "warning");
            </script>
        }
        if (Model.Message == "StockNotAvailable")
        {
            <script type="text/javascript">
                swal("", $("#Stocknotavailable").text(), "warning");
            </script>
        }
        if (message == "CostingComplited")
        {
            <script type="text/javascript">
                swal("", $("#span_CostingCompletedandStockUpdated").text(), "success");
            </script>
        }
        if (message == "CostingDetailNotFound")
        {
            <script type="text/javascript">
                swal("", $("#span_CostingDetailsNotFound").text(), "warning");
            </script>
        }
        if (Model.Message == "DocModify")
        {
            <script type="text/javascript">
                swal("", $("#SourceDocumentHasBeenModifiedCanNotProceed").text(), "warning");
            </script>
        }
        if (Model.Message == "InvProcessed")
        {
            <script type="text/javascript">
                swal("", $("#JobInvoiceHasBeenProcessedCanNotBeModified").text(), "warning");
            </script>
        }
        //Session.Remove("Message");
    }
    if (Model.BtnName == "BtnEdit" && Model.Command == "Edit" && Model.GRNStatus == "PC")
    {
        <script type="text/javascript">
            $(document).ready(function () {
                var gst_number = $("#GrnSuppGst_Number").val();
                var GstApplicable = $("#Hdn_GstApplicable").text();
                if (GstApplicable == "Y") {
                    Cmn_OnSaveAddressApplyGST(gst_number, "GRNIServictmDetailsTbl", "TxtItemId", "", "TxtGrnValue");
                }
            });
        </script>
    }
}
