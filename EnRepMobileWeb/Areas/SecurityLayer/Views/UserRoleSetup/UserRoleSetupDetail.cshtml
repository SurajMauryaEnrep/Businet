@{
    if (Session["URS_Command"] != null)
    {
        if (Session["URS_Command"].ToString() == "Update" || Session["URS_Command"].ToString() == "Refresh")
        {
            <script>
                $(document).ready(function () {
                    var UserRole_DetailList = new Array();
                    $('input[type=checkbox]:checked').each(function () {
                        debugger
                        var ID = (this.checked ? $(this).attr("id") : "");
                        var featureid = $("#hf_FID" + ID).val();
                        var Docid = $("#hf_DocID" + ID).val();
                        $("#" + ID).trigger('click');
                        var UserRoleList = {};

                        UserRoleList.feature_id = featureid;
                        UserRoleList.ChkboxID = ID;
                        UserRoleList.doc_id = Docid;
                        UserRoleList.grant_access = "Y";
                        UserRole_DetailList.push(UserRoleList);
                    });
                    if (UserRole_DetailList.length > 0) {
                        for (i = 0; i < UserRole_DetailList.length; i++) {
                            $("#" + UserRole_DetailList[i].ChkboxID).trigger('click');
                        }
                        $('#div_chkboxtree input[type=checkbox]').attr('disabled', 'true');
                        UserRole_DetailList = null;
                    }
                    else {
                        $('#div_chkboxtree input[type=checkbox]').attr('disabled', 'true');
                    }
                });

            </script>
        }
        else
        {
            <script>
                $(document).ready(function () {
                    var UserRole_DetailList = new Array();
                    $('input[type=checkbox]:checked').each(function () {
                        debugger
                        var ID = (this.checked ? $(this).attr("id") : "");
                        var featureid = $("#hf_FID" + ID).val();
                        var Docid = $("#hf_DocID" + ID).val();
                        $("#" + ID).trigger('click');
                        var UserRoleList = {};

                        UserRoleList.feature_id = featureid;
                        UserRoleList.ChkboxID = ID;
                        UserRoleList.doc_id = Docid;
                        UserRoleList.grant_access = "Y";
                        UserRole_DetailList.push(UserRoleList);
                    });
                    if (UserRole_DetailList.length > 0) {
                        for (i = 0; i < UserRole_DetailList.length; i++) {
                            $("#" + UserRole_DetailList[i].ChkboxID).trigger('click');
                        }
                        $('#div_chkboxtree input[type=checkbox]').removeAttr("disabled");
                        UserRole_DetailList = null;
                    }
                    else {
                        $('#div_chkboxtree input[type=checkbox]').removeAttr("disabled");
                    }
                });
            </script>
        }
    }
    else
    {
        <script type="text/javascript">
            $('#div_chkboxtree input[type=checkbox]').removeAttr("disabled");
        </script>
    }
