                                <span id="SpanValidUptoErrorMsg" class="error_message"></span>
                                @*<input value="" class="form-control" id="ValidUpto" maxlength="20" name="ValidUpto" placeholder="@Resource.Portfolio" type="date">*@
                            </div>
                            @if (Disable)
                            {

                            }
                            else
                            {
                                if (Model.DocumentStatus == null)
                                {
                                    <div class="col-md-55 col-sm-12 pl-0">
                                        <label class="col-form-label label-align">@Resource.ReplicateWith</label>
                                        <select class="form-control" id="ddlReplicateWith" name="ReplicateWith" onchange="OnChanngReplicateWith()" @*onchange="ddlReplicateWith_onchange()"*@>
                                        </select>
                                    </div>
                                }
                            }

                            <div class="col-md-55 col-sm-12 mt-3">
