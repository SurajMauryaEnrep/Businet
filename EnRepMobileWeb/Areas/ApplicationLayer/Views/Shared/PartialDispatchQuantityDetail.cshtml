                    <input autocomplete="off" class="form-control" id="txtDispOpOutUom" name="UOM" placeholder="@Resource.UOM" readonly="readonly" type="text" value="">
                </div>
                <div class="col-md-6 col-sm-12 pl-0">
                    <label class="col-form-label label-align">@Resource.Order @Resource.Quantity </label>
                    <input autocomplete="off" class="form-control num_right" id="txtDispOrdQty" name="OrderQuantity" placeholder="0000.00" readonly="readonly" type="text" value="">
                </div>
            </div>
        </div>
