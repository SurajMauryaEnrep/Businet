    var display = "none";
    var disablePrintbtn = "";
    var FCloseAccess = "N";
    decimal AssVal = 0;
    if (Model.TransType == "Update")
    {
        var v = Model.OrdStatus;
        if (Model.Command == "Edit")
        {
            if (Model.OrdStatus == "D")
            {
                DisableD = false;
            }
            else
            {
                DisableD = true;
            }

            //DisableH = false;
        }
        else
        {
            DisableD = true;
            DisableH = true;
        }
    }
    else
    {
        if (Model.Command == "Refresh")
        {
            DisableD = true;
            DisableH = true;
        }
        else
        {
            DisableD = false;
            DisableH = false;
        }
    }
    if ((Model.DocumentStatus != "D") || DisableH)
    {
    }
    else
    {
        if (Model.PO_No == null)
        {
            DisableH = false;
        }
        else
        {
            DisableH = true;
            transtype = "Update";
        }
    }
    if (DisableD)
    {
        ViewBag.Disable = true;
        ForDisableOCDlt = "Disable";
    }
    else
    {
        ViewBag.Disable = false;
        ForDisableOCDlt = "Enable";
        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
    if (Model.Pending_SourceDocument == "CreateDocument_Pending")
    {
        DisableH = true;
    }
    <script>
         sessionStorage.setItem("PQTransType", "@transtype");
    </script>
    if (Model.PO_No == null || ((Model.OrdStatus == "D" || Model.OrdStatus == "A") && Model.Command == "Edit"))
    {

        disablePrintbtn = "disabled style=filter:grayscale(100%)";
    }
    else if (Model.PO_No != null)
    {
        if (Model.PO_No.Split('(').Count() > 1)
        {
            disablePrintbtn = "disabled style=filter:grayscale(100%)";
        }
    }
    if (Model.Src_Type == null)
    {
        Model.Src_Type = "D";
    }
}
@{
    if (Model.Message != null)
    {
        var message = Model.Message;
        Model.hdnsaveApprovebtn = null;
        if (message == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
            </script>
        }
        if (message == "Deleted")
        {
            <script type="text/javascript">
                swal("", $("#deletemsg").text(), "success");
            </script>
        }
        if (message == "Approved")
        {
            <script type="text/javascript">
                swal("", $("#approvemsg").text(), "success");
            </script>
        }
        if (Model.Message == "Approved_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    swal("", $("#approvemsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        if (message == "Cancelled")
        {
            <script type="text/javascript">
                swal("", $("#cancelmsg").text(), "success");
            </script>
        }
        if (Model.Message == "Cancelled_ErrorInMail")
        {
            <script type="text/javascript">
                $(document).ready(function () {
                    swal("", $("#cancelmsg").text() + '. \n ' + $("#ThereIsSomethingWrongWhileSendingAlerts").text(), "warning");
                });
            </script>
        }
        if (message == "Used")
        {
            <script type="text/javascript">
                swal("", $("#PurchaseOrderHasBnPrcsdCanNotBeMdfd").text(), "warning");
            </script>
        }
        if (message == "deliverynoteunderprocess")
        {
            <script type="text/javascript">
                swal("", $("#deliverynoteunderprocess").text(), "warning");
            </script>
        }
        if (message == "Consumableinvoiceunderprocess")
        {
            <script type="text/javascript">
                swal("", $("#ConsumableInvoiceCreatedCanNotBeModified").text(), "warning");
            </script>
        }
        if (message == "DocModify")
        {
            <script type="text/javascript">
                swal("", $("#SourceDocumentHasBeenModifiedCanNotProceed").text(), "warning");
            </script>
        }
        if (Model.Message == "ErrorInMail")
        {
            <script type="text/javascript">
                swal("", $("#Emailsendingfailedduetotechnicalissue").text(), "warning");
            </script>
        }
        //Session.Remove("Message");
    }
    if (Model.BtnName == "BtnEdit" && Model.Command == "Edit" && Model.OrdStatus == "D" && Model.DocumentMenuId == "105101130")
    {
        <script type="text/javascript">
            $(document).ready(function () {
                var gst_number = $("#ship_add_gstNo").val();
                var GstApplicable = $("#Hdn_GstApplicable").text();
                if (GstApplicable == "Y") {
                    Cmn_OnSaveAddressApplyGST(gst_number, "PoItmDetailsTbl", "hfItemID", "SNohiddenfiled", "item_ass_val");
                }
            });
        </script>
    }
}
