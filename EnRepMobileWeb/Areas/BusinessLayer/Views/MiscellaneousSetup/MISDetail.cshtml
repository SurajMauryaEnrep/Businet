<style>
    /* Button css */
    #button-1000 .knobs:before {
        content: '';
        position: absolute;
        top: 2px;
        left: 2px;
        width: 14px;
        height: 14px;
        color: #fff;
        font-size: 10px;
        font-weight: bold;
        text-align: center;
        line-height: 1;
        padding: 0px 4px;
        background-color: #fff;
        border-radius: 50%;
        transition: 0.3s cubic-bezier(0.18, 0.89, 0.35, 1.15) all;
        box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.35), 0 10px 10px -5px rgba(0, 0, 0, 0.20);
    }

    #button-1000 .checkbox000:checked + .knobs:before {
        content: '';
        left: 25px;
        background-color: #fff;
    }

    #button-1000 .checkbox000:checked ~ .layer {
        background-color: #17c2bd;
    }

    #button-1000 .knobs, #button-1000 .knobs:before, #button-1000 .layer {
        transition: 0.3s ease all;
    }

    .knobs, .layer {
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
    }

    .button000 {
        position: relative;
        top: 50%;
        width: 40px;
        height: 18px;
        margin: -15px auto 0 auto;
        overflow: hidden;
        margin: 0;
    }

        .button000.r, .button000.r .layer {
            border-radius: 100px;
        }

    .checkbox000 {
        position: relative;
        width: 100%;
        height: 100%;
        padding: 0;
        margin: 0;
        opacity: 0;
        cursor: pointer;
        z-index: 3;
    }

    .knobs {
        z-index: 2;
    }

    .layer {
        width: 100%;
        background-color: #d3d3d3;
        transition: 0.3s ease all;
        z-index: 1;
    }
</style>

