                                    <tr>
                                        <td style="font-weight:bold;"> GL Account :</td>
                                        <td>&nbsp;</td>
                                        <td> @ViewBag.PrintGL.Rows[0]["acc_name"]</td>
                                        <td width="450">&nbsp;</td>
                                        <td style="font-weight:bold;">Opening Balance </td>
                                        <td width="10"> &nbsp;</td>
                                        <td>@ViewBag.PrintGL.Rows[0]["op_bal"].ToString()</td>
                                        <td width="50">Dr</td>
                                    </tr>
                                </table>
                            </td>
                            <td width="15">&nbsp;</td>
                        </tr>
                    </table>
                </td>
            </tr>
