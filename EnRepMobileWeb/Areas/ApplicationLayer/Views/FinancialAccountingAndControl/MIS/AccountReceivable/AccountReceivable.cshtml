                            @*@if (Session["MenuDocumentId"] != null)*@
                            @Resource.All
                            @Html.RadioButtonFor(m => m.ReceivableType, "A", new { @class = "", @id = "ReceivableTypeA", onclick = "onclickReceivableType()", @checked = "checked", @name = "ReceivableTypeA" })
                            @Resource.Overdue
                            @Html.RadioButtonFor(m => m.ReceivableType, "O", new { @class = "", @id = "ReceivableTypeO", onclick = "onclickReceivableType()", @name = "ReceivableTypeO" })
                            @Resource.Upcoming @Resource.Due
                            @Html.RadioButtonFor(m => m.ReceivableType, "U", new { @class = "", @id = "ReceivableTypeU", onclick = "onclickReceivableType()", @name = "ReceivableTypeU" })

                        </div>
                        <div class="col-md-3 col-sm-12 pl-3 float-right">
