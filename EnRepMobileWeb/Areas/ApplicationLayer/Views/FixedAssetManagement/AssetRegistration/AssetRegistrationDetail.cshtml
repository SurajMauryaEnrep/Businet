                                                                <th width="5%">@Resource.SrNo</th>
                                                                <th width="16%">@Resource.FinancialYear</th>
                                                                <th width="16%">@Resource.Period</th>
                                                                <th width="10%">@Resource.Calendar @Resource.Year </th>
                                                                <th width="14%">@Resource.Depreciation <br /> @Resource.Amount</th>
                                                                <th width="13%">@Resource.Additional <br /> @Resource.Depreciation @Resource.Amount</th>
                                                                <th width="13%">@Resource.Revised <br /> @Resource.Value</th>
                                                                <th width="13%">@Resource.Depreciation <br /> @Resource.Status</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @if (ViewBag.DepCalendar != null)
                                                            {
                                                                int j = 0;
                                                                foreach (System.Data.DataRow dr in ViewBag.DepCalendar.Rows)
                                                                {
                                                                    j++;
                                                                    <tr>
                                                                        <td>@j</td>
                                                                        <td>@dr["f_fy"]</td>
                                                                        <td>@dr["f_period"]</td>
                                                                        <td>@dr["cal_yr"]</td>
                                                                        @*<td class="num_right">@dr["proc_amt"]</td>*@
                                                                        <td class="num_right">@dr["dep_amt"]</td>
                                                                        <td class="num_right">@dr["add_dep_amt"]</td>
                                                                        <td class="num_right">@dr["current_amt"]</td>
                                                                        @if (dr["dep_processed"].ToString() == "Processed")
                                                                        {
                                                                            <td class="alert alert-success">@dr["dep_processed"]</td>
                                                                        }
                                                                        else if (dr["dep_processed"].ToString() == "Pending")
                                                                        {
                                                                            <td class="alert alert-danger">@dr["dep_processed"]</td>

                                                                        }
                                                                        else if (dr["dep_processed"].ToString().Trim() == "In-Process")
                                                                        {
                                                                            <td class="alert alert-warning">@dr["dep_processed"]</td>
                                                                        }
                                                                        @*<td>@dr["dep_processed"]</td>*@

                                                                    </tr>
                                                                }
                                                            }
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </section>


                            @*/*------------------Attatchmnet Start----------------*/*@
                            @Html.Partial("~/Areas/Common/Views/Comn_PartialAttatchmentDetail.cshtml")
                            @Html.HiddenFor(model => model.attatchmentdetail, new { @id = "hdn_Attatchment_details" })
                            @*/*------------------Attatchmnet End----------------*/*@
                        </article>
                    </div>
                </div>
            </div>
        </div>
    }
}
