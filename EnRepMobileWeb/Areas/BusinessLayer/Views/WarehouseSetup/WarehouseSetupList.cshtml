@using EnRepMobileWeb.Resources;
@model EnRepMobileWeb.MODELS.BusinessLayer.WarehouseSetup.WarehouseSetupModel
@using System.Data
@if (Session["UserId"] == null)
{
    Response.Redirect("~/Login");
}
@{
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}
<script src="~/Content/Css/jquery/dist/jquery.min.js"></script>
<script src="~/Content/Scripts/BusinessLayer/WarehouseSetup/WarehouseSetup.js"></script>
<title>@Model.Title</title>
<div class="row">
    <div class="col-md-12 col-sm-12 ">
        <div class="x_panel mar_pad1 ">
            <div class="x_content mar_pad1">
                @Html.HiddenFor(m => m.ListFilterData, new { @id = "ListFilterData" })
                <h2 class="col-md-7 col-sm-12 heading_left_padding ">@ViewBag.MenuPageName</h2>
                <div id="" class="col-md-5 col-sm-12 work_flow heading_right_padding">
                    @if (ViewBag.VBRoleList != null)
                    {
                        foreach (System.Data.DataRow Row in ViewBag.VBRoleList.Rows)
                        {
                            if (Row["feature_id"].ToString() == "103175001")
                            {
                                <ul>
                                    <li>
                                        <a class="tooltip-test"
                                           title=" @Resource.AddNew" href="@Url.Action("AddWarehouseSetupDetail","WarehouseSetup")"><img src="~/Content/Images/addgreen.png" width="32" id="addNewSupplier" height="32" alt="Add"></a>
                                    </li>
                                </ul>
                            }
                        }
                    }

                </div>
                <div class="x_title pad_mar ">
                    <div class="col-md-12 col-sm-12 icon_tool ">
                        @* -------------------------------- *@
                        <form class="available_stock pad_mar" action="" method="post" novalidate>
                            <div class="col-md-3 col-sm-12">
                                <label class="col-form-label label-align">
                                    @Resource.WarehouseName
                                </label>
                                @Html.DropDownListFor(Model => Model.WarehouseName, new SelectList(Model.wh_Namelists, "WareH_id", "wareH_name"), @*"ALL",*@ new { @class = "form-control", @id = "WarehouseNamefilter" })
                                @*<input  type="hidden" id="SuppListName" value='@Url.Action("GetAutoCompleteSuppList", "SupplierList")' />*@
                            </div>
                            <div class="col-md-2 col-sm-12">
                                <label class="col-form-label label-align">
                                    @Resource.WarehouseType
                                </label>
                                @Html.DropDownListFor(model => model.WarehouseTypefilter, new SelectList(new List<SelectListItem> {
                                                           new SelectListItem {Text="ALL",Value="" },
                                                           new SelectListItem {Text="External",Value="E" },
                                                           new SelectListItem {Text="Internal",Value="I" },
                                                           }, "Value", "Text"
            ), new { @class = "form-control", @id = "WarehouseTypefilter", @name = "WarehouseTypefilter" })
                                @*<select class="form-control" id="WarehouseTypefilter">
                            <option value="">ALL</option>
                            <option value="E">External</option>
                            <option value="I">Internal</option>
                        </select>*@
                            </div>
                            <div class="col-md-1 col-sm-12">
                                <button type="button" class="btn btn-primary btn-sm margintop20" onclick="FilterListwarehouse()" id="BtnSearch"><i class="fa fa-search" aria-hidden="true"></i></button>
                            </div>
                        </form>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="x_title pad_mar ">
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12 col-sm-12 ">
        <div class="x_panel mar_pad1 ">
            <div class="x_content mar_pad1">
                <div class="col-md-12 col-sm-12 heading_left_padding  padding_right ">
                    <div class="x_panel margin_top_zero">
                        <div class="x_content">
                            <div class="row">
                                <div class="card-box table-responsive dy_table">
                                    <div id="Wh_tbody" class="sticky-table sticky-ltr-cells sb-container container-example1">
                                    @Html.Partial("~/Areas/BusinessLayer/Views/Shared/PartialWarehouseList.cshtml")
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>