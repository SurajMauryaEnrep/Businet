@{
    var Disable = true;
    if (Session["URS_TransType"] != null)
    {
        if (Session["URS_TransType"].ToString() == "Update")
        {
            if (Session["URS_Command"].ToString() == "Edit")
            {
                Disable = false;
            }
            else
            {
                Disable = true;
            }
        }
        else
        {
            Disable = false;
        }
    }
    else
    {
        Disable = false;
    }
    if (Session["URS_TransType"].ToString() == "Refresh")
    {
        Disable = true;
    }
    if (Session["URS_TransType"].ToString() == "Save")
    {
        if (Session["URS_Command"].ToString() == "AddNewUserRoleSetup")
        {
            Disable = false;
        }
        else if (Session["URS_Command"].ToString() == "Edit")
        {
            Disable = false;
        }
        else
        {
            Disable = true;
        }
    }
    if (ViewBag.Message == "Duplicate")
    {
        Disable = false;
    }
}
@{
    if (Session["URS_Message"] != null)
    {
        var message = Session["URS_Message"].ToString();
        if (message != null)
        {
            Model.hdnsaveApprovebtn = null;
            <script>
                $(document).ready(function () {
                    $("#hdnsavebtn").val("");
                })
            </script>
        }
        if (ViewBag.URSMessage == "Duplicate")
        {
            Model.hdnsaveApprovebtn = null;
            <script>
                $(document).ready(function () {
                    $("#hdnsavebtn").val("");
                })
            </script>
        }

        if (Session["URS_Message"].ToString() == "Save")
        {
            <script type="text/javascript">
                swal("", $("#savemsg").text(), "success");
            </script>
        }
        if (Session["URS_Message"].ToString() == "Deleted")
        {
            <script type="text/javascript">
                swal("", $("#deletemsg").text(), "success");
            </script>
        }
        if (Session["URS_Message"].ToString() == "Exists")
        {
            <script type="text/javascript">
                swal("", $("#DependencyExist").text(), "warning");
            </script>
        }

        Session["URS_Message"] = null;
    }
}

