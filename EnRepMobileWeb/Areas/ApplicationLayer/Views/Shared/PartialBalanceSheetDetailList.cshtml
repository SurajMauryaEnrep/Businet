        tot_assetncurr_amt = "0.00";
        tot_passetncurr_amt = "0.00";
    }

    if (ViewBag.lib_capflag != null)
    {
        if (ViewBag.lib_capflag == "ca")
        {
            start = 0;
            foreach (System.Data.DataRow Row in ViewBag.capi_libadata.Rows)
            {
                tot_liba_amt = Row["ctot_amt"].ToString();
                tot_pliba_amt = Row["ptot_amt"].ToString();
                tot_libcap_amt = Row["cgrptot_amt"].ToString();
                tot_plibcap_amt = Row["pgrptot_amt"].ToString();
                start = 1;
                if (start == 1)
                {
                    break;
                }
            }
        }
    }
    else
    {
        tot_libcap_amt = "0.00";
        tot_plibcap_amt = "0.00";
    }
    if (ViewBag.lib_currflag != null)
    {
        if (ViewBag.lib_currflag == "cu")
        {
            start = 0;
            foreach (System.Data.DataRow Row in ViewBag.curr_libadata.Rows)
            {
                tot_liba_amt = Row["ctot_amt"].ToString();
                tot_pliba_amt = Row["ptot_amt"].ToString();
                tot_libcurr_amt = Row["cgrptot_amt"].ToString();
                tot_plibcurr_amt = Row["pgrptot_amt"].ToString();
                start = 1;
                if (start == 1)
                {
                    break;
                }
            }
        }
    }
    else
    {
        tot_libcurr_amt = "0.00";
        tot_plibcurr_amt = "0.00";
    }
    if (ViewBag.lib_ncurrflag != null)
