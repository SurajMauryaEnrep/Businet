@{
    var str = Model.Message;
}

@if (!string.IsNullOrEmpty(str))
{
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            var message = "@str";

            switch (message) {
                case "Save":
                    swal("", document.getElementById("savemsg").textContent, "success");
                    break;
                case "Deleted":
                    swal("", document.getElementById("deletemsg").textContent, "success");
                    break;
                case "Exists":
                    swal("", document.getElementById("DependencyExist").textContent, "warning");
                    break;
                case "Duplicate":
                    document.getElementById("vmProspectName").innerHTML = document.getElementById("valueduplicate").textContent;
                    document.getElementById("ProspectName").style.borderColor = "red";
                    break;
                case "DuplicateGst_No":
                    document.getElementById("SpanSuppGSTMidPrt").innerHTML = document.getElementById("valueduplicate").textContent;
                    document.getElementById("GSTMidPrt").style.borderColor = "red";
                    document.getElementById("GSTLastPrt").style.borderColor = "red";
                    document.getElementById("SpanSuppGSTMidPrt").style.display = "block";
                    break;
                case "Cancelled":
                    swal("", document.getElementById("cancelmsg").textContent, "success");
                    break;
            }
        });
    </script>
}

