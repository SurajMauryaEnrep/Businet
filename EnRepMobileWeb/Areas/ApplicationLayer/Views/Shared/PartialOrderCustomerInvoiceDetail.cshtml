<script src="~/Content/Scripts/Common/CommonPartialJS.js"></script>
<div class="modal-content">
    <div class="modal-header">
        <h2>@Resource.Order @Resource.Summary</h2>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>
    <div class="modal-body" style="padding-top:0px;">
        <div class="col-md-4 col-sm-12" style="padding-left:0px;">
            <label class="col-form-label label-align">@Resource.CustomerName</label>
            <input id="CustomerName" class="form-control" autocomplete="off" type="text" name="Customer" placeholder="@Resource.CustomerName" disabled="">
            <input hidden="hidden" id="hdnCustId" />
        </div>

        <div class="col-md-2 col-sm-12" style="padding-left:0px;">
            <label class="col-form-label label-align">@Resource.Currency</label>
            <input id="Currency" class="form-control" autocomplete="off" type="text" name="Currency" placeholder="@Resource.Currency" disabled="">
            <input hidden id="Currency_id" />
        </div>
        <div class="col-md-2 col-sm-12" style="padding-left:0px;">
            <label class="col-form-label label-align">@Resource.FromDate</label>
            <input id="FromDate" class="form-control" value="" autocomplete="off" type="date" name="FromDate" placeholder="" disabled="">
        </div>

        <div class="col-md-2 col-sm-12" style="padding-left:0px;">
            <label class="col-form-label label-align">@Resource.ToDate</label>
            <input id="ToDate" class="form-control" value="" autocomplete="off" type="date" name="ToDate" placeholder="" disabled="">
        </div>

        <div class="clearfix"></div>
        @*<div class="tableFixHead height30VH margin10">
            <table id="" class="table table-striped width1800 rowhighlighttbl" cellspacing="0" width="100%">
                <thead>
                    <tr class="headings">*@
        <div class="card-box table-responsive dy_table margin15">
            <div id="" class="sticky-table sticky-ltr-cells sb-container container-example1 OrderHistory height30VH">
                <table id="Datatable12" data-order="" class="table table-striped width1900 table-bordered rowhighlighttbl itmfrz" style="width:100%">
                    <thead>
                        <tr class="sticky-header">
                            <th width="3%">@Resource.SrNo</th>
                            <th width="12%" class="itmStick">@Resource.OrderNumber</th>
                            <th width="5%">@Resource.OrderDate</th>
                            <th width="5%">@Resource.Customer @Resource.ReferenceNumber</th>
                            <th hidden="hidden"></th>
                            <th width="8%">@Resource.SalesRepresentative</th>
                            <th width="8%">@Resource.GrossValue <br /> @Resource.InSpecific</th>
                            <th width="8%">@Resource.GrossValue <br /> @Resource.InBase</th>
                            <th width="8%">@Resource.TaxAmount <br /> @Resource.InSpecific</th>
                            <th width="8%">@Resource.TaxAmount <br /> @Resource.InBase</th>
                            <th width="7%">@Resource.OtherCharges <br /> @Resource.InSpecific </th>
                            <th width="7%">@Resource.OtherCharges <br /> @Resource.InBase </th>
                            <th width="7%">@Resource.Order @Resource.Amount  <br /> @Resource.InSpecific </th>
                            <th width="7%">@Resource.Order @Resource.Amount  <br /> @Resource.InBase </th>
                            <th width="7%">@Resource.ApprovedOn</th>
                        </tr>
                    </thead>
                    <tbody id="">
                        @if (ViewBag.CWOrderDetailList != null)
                        {
                            var srno = 1;
                            foreach (System.Data.DataRow dr in ViewBag.CWOrderDetailList.Rows)
                            {
                        <tr>
                            <td>@srno</td>
                            <td id="app_inv_no" class="ItmNameBreak itmStick tditemfrz">@dr["app_so_no"].ToString()</td>
                            <td>@dr["so_dt1"].ToString()</td>
                            <td hidden="hidden" id="txtInv_dt">@dr["so_dt"].ToString()</td>
                            <td>@dr["ref_doc_no"].ToString()</td>
                            <td>@dr["sale_per"].ToString()</td>
                            <td class="num_right">@dr["sale_amount_spec"].ToString()</td>
                            <td class="num_right">@dr["sale_amount_bs"].ToString()</td>
                            <td class="num_right">@dr["tax_amt_spec"].ToString()</td>
                            <td class="num_right">@dr["tax_amt_bs"].ToString()</td>
                            <td class="num_right">@dr["oc_amt_spec"].ToString()</td>
                            <td class="num_right">@dr["oc_amt_bs"].ToString()</td>
                            <td class="num_right">
                                <div class="col-sm-11">@dr["Order_amt_spec"].ToString()</div>
                                <div class="col-sm-1 i_Icon">
                                    <button type="button" id="" class="calculator iButton" onclick="CW_OnclickInvoiceAmtGetItem(event)" data-toggle="modal" data-target="#CustomerInvoiceProductDetail" data-backdrop="static" data-keyboard="false">  <img src="~/Content/Images/iIcon1.png" alt="" title="@Resource.ProductDetail" /> @*<i class="fa fa-info" aria-hidden="true" title="@Resource.InvoiceDetail"></i>*@ </button>
                                </div>
                            </td>
                            <td class="num_right">
                                <div class="col-sm-11" id="">@dr["Order_amt_bs"].ToString()</div>
                                <div class="col-sm-1 i_Icon">
                                    <button type="button" id="" class="calculator iButton" onclick="CW_OnclickInvoiceAmtGetItem(event)" data-toggle="modal" data-target="#CustomerInvoiceProductDetail" data-backdrop="static" data-keyboard="false">  <img src="~/Content/Images/iIcon1.png" alt="" title="@Resource.ProductDetail" /> @*<i class="fa fa-info" aria-hidden="true" title="@Resource.InvoiceDetail"></i>*@ </button>
                                </div>
                            </td>

                            <td>@dr["app_dt"].ToString()</td>
                        </tr>
                                srno = srno + 1;
                                //totalsale_amt_spec = totalsale_amt_spec + dr["sale_amount_spec"]);
                                //totaltax_amt_spec = totaltax_amt_spec + dr["tax_amt_spec"]);
                                //totalOC_amt_spec = totalOC_amt_spec + dr["oc_amt_spec"]);
                                //totalsale_amt = totalsale_amt + dr["sale_amount_bs"]);
                                //totaltax_amt = totaltax_amt + dr["tax_amt_bs"]);
                                //totalOC_amt = totalOC_amt + dr["oc_amt_bs"]);
                                //totalInvoice_amt = totalInvoice_amt + dr["Order_amt_bs"]);
                                //totalInvoice_amt_spec = totalInvoice_amt_spec + dr["Order_amt_spec"]);
                            }

                        }
                    </tbody>
                    <tfoot>
                        @if (ViewBag.TotalCW_Ord_Details != null)
                        {
                            foreach (System.Data.DataRow dr in ViewBag.TotalCW_Ord_Details.Rows)
                            {
                        <tr class="total">
                            <td>&nbsp;</td>
                            <td>&nbsp;</td>
                            <td>&nbsp;</td>
                            <td>&nbsp;</td>
                            <td align="right"><strong>@Resource.Total</strong></td>
                            <td class="num_right"><strong>@dr["sale_amount_spec"].ToString()</strong></td>
                            <td class="num_right"><strong>@dr["sale_amount_bs"].ToString()</strong></td>
                            <td class="num_right"><strong>@dr["tax_amt_spec"].ToString()</strong></td>
                            <td class="num_right"><strong>@dr["tax_amt_bs"].ToString()</strong></td>
                            <td class="num_right"><strong>@dr["oc_amt_spec"].ToString()</strong></td>
                            <td class="num_right"><strong>@dr["oc_amt_bs"].ToString()</strong></td>
                            <td class="num_right"><strong>@dr["Order_amt_spec"].ToString()</strong></td>
                            <td class="num_right"><strong>@dr["Order_amt_bs"].ToString()</strong></td>
                            <td>&nbsp;</td>
                        </tr>
                            }
                        }

                    </tfoot>
                </table>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal" id="" onclick="OnClickSerialDiscardAndExit()">@Resource.Close</button>
    </div>
</div>





