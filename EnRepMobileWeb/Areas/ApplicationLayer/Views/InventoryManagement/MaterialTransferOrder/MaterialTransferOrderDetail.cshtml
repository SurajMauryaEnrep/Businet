        //if (Session["Message"].ToString() == "Approved")
        if (Model.Message == "Approved")
        {
            <script type="text/javascript">
                swal("", $("#approvemsg").text(), "success");
            </script>
        }
        //if (Session["Message"].ToString() == "Cancelled")
