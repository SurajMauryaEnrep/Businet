                        <div class="clear"></div>
                        <div class="card-box table-responsive dy_table">
                            <div class="price_item_detail">
                                <div class=" col-sm-12">
                                    <div class="tableFixHead">
                                        @*<h2>@Resource.ItemDetail</h2>*@
                                        <table id="SupplierPriceListTbl" class="table table-bordered mt-3 itmfrz">
                                            <thead>
                                                <tr class="headings">
                                                    <th width="2%">&nbsp;</th>
                                                    <th width="2%">@Resource.SrNo</th>
                                                    @*<th width="19%">@Resource.ItemName</th>*@
                                                    <th width="30%" class="itmStick">
                                                        <div class="col-md-2 col-sm-12"></div>
                                                        <div class="col-md-6 col-sm-12">
                                                            <span class="item_add">
                                                                @Resource.ItemName <span id="spanItemReq" class="required">*</span>
                                                            </span>
                                                            @if (Disable)
                                                            {
                                                            }
                                                            else
                                                            {
                                                                <a href="#">
                                                                    <div class="plus_icon1" id="PlusBtnAddItem"> <i class="fa fa-plus" id="" onclick="AddNewRow()" title="Add New"></i> </div>
                                                                </a>
                                                            }
                                                        </div>
                                                        <div class="col-md-2 col-sm-12"></div>
                                                    </th>
                                                    <th width="6%" title="@Resource.UnitOfMeasure"> @Resource.UOM </th>
                                                    <th width="10%">@Resource.PackSize</th>
                                                    <th width="10%">@Resource.MRP</th>
                                                    <th width="10%">@Resource.Price <span class="required">*</span></th>
                                                    <th width="10%">@Resource.Discount <br />  @Resource.InPercentage</th>
                                                    <th width="10%">@Resource.Effective @Resource.Price</th>
                                                    <th width="10%">@Resource.Price @Resource.History</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @if (ViewBag.ItemDetail != null)
                                                {
                                                    var disableFlag = "";
                                                    if (Disable)
                                                    {
                                                        disableFlag = "disabled";
                                                    }
                                                    var rowIdx = 0;
                                                    foreach (System.Data.DataRow Row in ViewBag.ItemDetail.Rows)
                                                    {
                                                        rowIdx++;
                                                        <tr id="R+@rowIdx">
                                                            <td class=" red center">
                                                                @if (Disable)
                                                                {
                                                                    <i class="fa fa-trash" disabled style="filter: grayscale(100%)" aria-hidden="true" id="delBtnIcon" title="@Resource.Delete"></i>
                                                                }
                                                                else
                                                                {
                                                                    <i class="deleteIcon fa fa-trash" aria-hidden="true" title="Delete"></i>
                                                                }
                                                            </td>
                                                            <td class="sr_padding" id="srno">@rowIdx</td>
                                                            <td>
                                                                <div class="lpo_form">
                                                                    <div class="col-sm-11 no-padding">
                                                                        <select class="form-control" @disableFlag id="@("SPLItemListName"+rowIdx)" name="SPLItemListName+@rowIdx" onchange="OnChangePOItemName(@rowIdx,event)">
                                                                            <option value="@Row["item_id"].ToString()">@Row["item_name"].ToString()</option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="col-sm-1 i_Icon">
                                                                        <button type="button" class="calculator subItmImg" data-toggle="modal" data-target="#ItemInfo" data-backdrop="static" onclick="OnClickIconBtn(event);" data-keyboard="false"><img src="/Content/Images/iIcon1.png" alt="" title="@Resource.ItemInformation">  </button>
                                                                    </div>
                                                                    <span id="@("SpanSPLItemListName_Error"+rowIdx)" class="error-message is-visible"></span>
                                                                    <input class="" type="hidden" id="SNohiddenfiled" value="@rowIdx" />
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <input id="UOM" class="form-control" autocomplete="off" type="text" name="UOM" value="@Row["uom_alias"].ToString()" placeholder="@Resource.UOM" readonly="readonly">
                                                                <input type="hidden" id="UOMID" value="@Row["uom_id"].ToString()">
                                                            </td>
                                                            <td> <input id="PackSize" @disableFlag autocomplete="off" value="@Row["pack_size"].ToString()" class="form-control" name="PackSize" placeholder="@Resource.PackSize"></td>
                                                            <td><input id="MRP" @disableFlag autocomplete="off" value="@Row["mrp"].ToString()" onchange="onchangeMRP(event)" class="form-control num_right" name="MRP" placeholder="0000.00"></td>
                                                            <td>
                                                                <div class="lpo_form">
                                                                    <input id="Price" @disableFlag autocomplete="off" value="@Row["price"].ToString()" onchange="onchangePrice(event)" class="form-control num_right" name="Price" placeholder="0000.00">
                                                                    <span id="Span_Price" class="error-message is-visible"></span>
                                                                </div>
                                                            </td>
                                                            <td><input id="DiscountInPercentage" @disableFlag value="@Row["item_disc_perc"].ToString()" autocomplete="off" onchange="onchangeDiscountInPercentage(event)" onkeypress="return FloatValuePerOnly(this,event);" class="form-control num_right" name="DiscountInPercentage" placeholder="0000.00"></td>
                                                            <td><input id="EffectivePrice" value="@Row["effective_price"].ToString()" class="form-control num_right" name="EffectivePrice" placeholder="0000.00" disabled="disabled"></td>
                                                            <td class="center">
                                                                <button type="button" id="SubItemAvlQty" class="calculator subItmImg" data-toggle="modal" onclick="onclickRevisionHistory(event)" data-target="#RevisionHistory" data-backdrop="static" data-keyboard="false"> <i class="fa fa-refresh ref" id="" onclick="" title="@Resource.Revision @Resource.History "></i> </button>
                                                            </td>
                                                        </tr>
                                                    }
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
}

